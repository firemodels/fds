&HEAD CHID='NIST_Propane_20kW_GEOM_Prescribed_p5cm', TITLE='NIST Propane Fire, 37 cm diameter burner, 0.5 cm resolution' /

&TIME T_END=60. /

&MISC SIMULATION_MODE='LES', TMPA=16., HUMIDITY=25. /

&DUMP DT_HRR=2., DT_DEVC=0.0000001 /

&MULT ID='m1', DX=0.32,DY=0.32,DZ=0.32, I_UPPER=3,J_UPPER=3,K_UPPER=5/ 96 mesh
&MESH IJK=64,64,64, XB=-0.64,-0.32,-0.64,-0.32,-0.20,0.12, MULT_ID='m1'/

&RADI NUMBER_RADIATION_ANGLES=16, TIME_STEP_INCREMENT=1, ANGLE_INCREMENT=1 /

&REAC RADIATIVE_FRACTION=0.23, FUEL='PROPANE', SOOT_YIELD=0.024, CO_YIELD=0.005, N_SIMPLE_CHEMISTRY_REACTIONS=2, FUEL_C_TO_CO_FRACTION=0.75, FUEL_H_TO_H2_FRACTION=0.5 /

&MATL ID='STEEL', CONDUCTIVITY=54, DENSITY=7800, SPECIFIC_HEAT=0.465, EMISSIVITY=0.8 /
&SURF ID='PAN', COLOR='GRAY 50', MATL_ID='STEEL', THICKNESS=0.003/
&SURF ID='POOL', COLOR='LIGHT BLUE', SPEC_ID='PROPANE', MASS_FLUX=0.004 /

&GEOM ID='37cm  burner', SURF_IDS='POOL','PAN','PAN',
      CYLINDER_LENGTH=0.10,
      CYLINDER_RADIUS=0.185,
      CYLINDER_ORIGIN=0.,0.,-0.10,
      CYLINDER_AXIS=0.,0.,1.,
      CYLINDER_NSEG_AXIS=6,
      CYLINDER_NSEG_THETA=48 /

&VENT MB='ZMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /

&CATF OTHER_FILES='NIST_Propane_37cm_DEVC.fds'/

&SLCF PBY=0.001, QUANTITY='VELOCITY', VECTOR=T /
&SLCF PBY=0.001, QUANTITY='TEMPERATURE', CELL_CENTERED=T /
&SLCF PBY=0.001, QUANTITY='TEMPERATURE' /
&SLCF PBY=0.001, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN', CELL_CENTERED=T /
&SLCF PBY=0.001, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE', CELL_CENTERED=T /
&SLCF PBY=0.001, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE', CELL_CENTERED=T /
&SLCF PBY=0.001, QUANTITY='VOLUME FRACTION', SPEC_ID='PROPANE', CELL_CENTERED=T /

&TAIL /
