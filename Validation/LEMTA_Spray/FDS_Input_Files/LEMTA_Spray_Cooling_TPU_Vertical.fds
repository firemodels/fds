&HEAD CHID='LEMTA_Spray_Cooling_TPU_Vertical', TITLE='Simulation LEMTA TPU vertical plate cooling experiment' /

&TIME T_END=1000 /

&MESH IJK=20,24,24, XB=-0.30,0.70,-0.6,0.6,-0.6,0.6 /

&MISC TMPA=19 /

&PRES MAX_PRESSURE_ITERATIONS=30 /

&OBST XB=-0.002,0.00,-0.50,0.50,-0.50,0.50, SURF_ID='Hot metallic plate', MATL_ID='Steel', CELL_SIZE=0.05 /

&OBST XB=-0.25,-0.20,-0.25,0.25,-0.25,0.25, SURF_ID6='INERT','Radiative panel','INERT','INERT','INERT','INERT' /

&MATL ID='Steel'
      SPECIFIC_HEAT_RAMP = 'c_ramp'
      CONDUCTIVITY_RAMP = 'k_ramp'
      DENSITY = 7850 /

&RAMP ID='k_ramp', T=   0., F=55 /
&RAMP ID='k_ramp', T= 100., F=52 /
&RAMP ID='k_ramp', T= 200., F=49 /
&RAMP ID='k_ramp', T= 300., F=45 /
&RAMP ID='k_ramp', T= 400., F=42 /
&RAMP ID='k_ramp', T= 600., F=34 /
&RAMP ID='k_ramp', T= 800., F=30 /
&RAMP ID='k_ramp', T=1000., F=29 /

&RAMP ID='c_ramp', T=  50., F=0.45 /
&RAMP ID='c_ramp', T= 250., F=0.50 /
&RAMP ID='c_ramp', T= 450., F=0.58 /
&RAMP ID='c_ramp', T= 650., F=0.70 /
&RAMP ID='c_ramp', T= 700., F=0.90 /
&RAMP ID='c_ramp', T= 750., F=1.10 /
&RAMP ID='c_ramp', T= 800., F=0.90 /
&RAMP ID='c_ramp', T=1000., F=0.50 /
&RAMP ID='c_ramp', T=1200., F=0.52 /

&SURF ID='Hot metallic plate', COLOR='BLACK', HT3D=T, CELL_SIZE_FACTOR=0.2, STRETCH_FACTOR=1. /

&SURF ID = 'Radiative panel'
      NET_HEAT_FLUX = 80
      RAMP_Q = 'NHFRAMP' /

&RAMP ID='NHFRAMP', T=  0.0, F=0.0 /
&RAMP ID='NHFRAMP', T=  5.0, F=0.0 /
&RAMP ID='NHFRAMP', T=  6.0, F=1.0 /
&RAMP ID='NHFRAMP', T=655.0, F=1.0 /
&RAMP ID='NHFRAMP', T=655.1, F=0.0 /

&SPEC ID='WATER VAPOR', MASS_EXTINCTION_COEFFICIENT=500 /

&SM3D QUANTITY='DENSITY', SPEC_ID='WATER VAPOR' /

&PART ID='Watermist', SAMPLING_FACTOR=1, SPEC_ID='WATER VAPOR', DIAMETER=182, GAMMA_D=3.3, QUANTITIES='PARTICLE TEMPERATURE','PARTICLE DIAMETER', HORIZONTAL_VELOCITY=2. /

&PROP ID='sprinkler',
      PART_ID='Watermist',
      OFFSET=0.02,
      SPRAY_ANGLE=0,21,
      FLOW_RATE=1.6,
      PARTICLE_VELOCITY=70,
      PARTICLES_PER_SECOND=5000 /

&DUMP DT_DEVC=1., DT_PROF=2. /

&DEVC ID='SPR', XYZ=0.5,0.0,0.0, ORIENTATION=-1,0,0, PROP_ID='sprinkler', QUANTITY='TIME', SETPOINT=656.0 /

&VENT DB='XMIN', SURF_ID='OPEN' /
&VENT DB='XMAX', SURF_ID='OPEN' /
&VENT DB='YMIN', SURF_ID='OPEN' /
&VENT DB='YMAX', SURF_ID='OPEN' /
&VENT DB='ZMIN', SURF_ID='OPEN' /
&VENT DB='ZMAX', SURF_ID='OPEN' /

&SLCF PBX=-0.001, QUANTITY='TEMPERATURE' /
&SLCF PBX=0.001, QUANTITY='TEMPERATURE' /
&SLCF PBY=0, QUANTITY='TEMPERATURE' /
&SLCF PBY=0, QUANTITY='W-VELOCITY' /

&PROF ID='bottom', XYZ=-0.005,0,0, QUANTITY='TEMPERATURE', IOR=-1 /
&PROF ID='top',    XYZ= 0.005,0,0, QUANTITY='TEMPERATURE', IOR= 1 /

&DEVC ID='T_bottom', XYZ=-0.005,0,0, QUANTITY='WALL TEMPERATURE', IOR=-1 /
&DEVC ID='T_top',    XYZ= 0.005,0,0, QUANTITY='WALL TEMPERATURE', IOR= 1 /

&BNDF QUANTITY='MPUA', PART_ID='Watermist' /
&BNDF QUANTITY='RADIATIVE HEAT FLUX'/
&BNDF QUANTITY='CONVECTIVE HEAT FLUX'/
&BNDF QUANTITY='TOTAL HEAT FLUX'/
&BNDF QUANTITY='NET HEAT FLUX'/
&BNDF QUANTITY='WALL TEMPERATURE'/

&TAIL /

