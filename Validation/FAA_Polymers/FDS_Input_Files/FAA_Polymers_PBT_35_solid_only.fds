&HEAD CHID='FAA_Polymers_PBT_35_solid_only', TITLE='PBT at 35 kW/m2, 0.4 mm thick, solid phase only' /

&MESH IJK=3,3,4, XB=-0.015,0.015,-0.015,0.015,0.0,0.04 /

&TIME T_END=400., DT=0.01, WALL_INCREMENT=1 / 
&MISC SOLID_PHASE_ONLY=.TRUE. , Y_O2_INFTY=0.001/

&REAC FUEL='PBT', C=4., H=10., O=2., SOOT_YIELD=0.02 / 

&MATL ID='PBT'
      ABSORPTION_COEFFICIENT=2560.
      N_REACTIONS=1
      A(1) = 2.49E14
      E(1) = 2.12E5
      EMISSIVITY=0.88
      DENSITY=1300.
      SPEC_ID='PBT'
      NU_SPEC=1.
      HEAT_OF_REACTION=507.
      HEAT_OF_COMBUSTION=19500.
      CONDUCTIVITY = 0.29
      SPECIFIC_HEAT = 2.23 /

&MATL ID='Kaowool'
      CONDUCTIVITY = 0.08
      SPECIFIC_HEAT = 0.80
      DENSITY = 48. /

&SURF ID='SAMPLE'
      COLOR='BLACK'
      MATL_ID='PBT','Kaowool'
      THICKNESS=0.004,0.015
      CELL_SIZE_FACTOR=0.1
      HEAT_TRANSFER_COEFFICIENT=0.
      EXTERNAL_FLUX_RAMP='FluxRamp' 
      EXTERNAL_FLUX=35. / 
&RAMP ID='FluxRamp', T=  0., F=1.00 /
&RAMP ID='FluxRamp', T=121., F=1.00 /
&RAMP ID='FluxRamp', T=122., F=1.386 /
&RAMP ID='FluxRamp', T=1000.,F=1.386 /

&VENT XB=-0.005,0.005,-0.005,0.005,0.0,0.00, SURF_ID='SAMPLE' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DUMP DT_DEVC=2., SIG_FIGS=4 /

&DEVC XYZ=0.,0.,0., QUANTITY='BURNING RATE', IOR=3, ID='MLR' /

&TAIL / 

