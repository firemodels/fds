&HEAD CHID='7p1_cm_methane_2mm_16mesh', TITLE='Purdue 7.1 cm methane burner' / Y. Xin et al. Combustion and Flame 141 (2005) 329-335

MESH IJK=50,50,100, XB=-.1,.1,-.1,.1,0,0.4 / 4 mm single mesh

&MULT ID='mesh array', DX=.1,DY=.1,DZ=.1, I_UPPER=1,J_UPPER=1,K_UPPER=3 /
&MESH IJK=50,50,50, XB=-.1,0,-.1,0,0,.1, MULT_ID='mesh array' / 2 mm 16 mesh

&TIME T_END=20/

&MISC TMPA=15.
      SIMULATION_MODE='LES'/

&SPEC ID='METHANE',       LUMPED_COMPONENT_ONLY=.TRUE./
&SPEC ID='ETHYLENE',      LUMPED_COMPONENT_ONLY=.TRUE./
&SPEC ID='NITROGEN',      LUMPED_COMPONENT_ONLY=.TRUE./
&SPEC ID='CARBON DIOXIDE',LUMPED_COMPONENT_ONLY=.TRUE./

&SPEC ID='natural gas'
      SPEC_ID(1)='METHANE',       VOLUME_FRACTION(1)=92.2
      SPEC_ID(2)='ETHYLENE',      VOLUME_FRACTION(2)= 3.3
      SPEC_ID(3)='NITROGEN',      VOLUME_FRACTION(3)= 3.9
      SPEC_ID(4)='CARBON DIOXIDE',VOLUME_FRACTION(4)= 0.6/

&REAC FUEL='natural gas'
      RADIATIVE_FRACTION=0.1
      SOOT_YIELD=0.01 /

&SURF ID='FLAME'
      COLOR='ORANGE'
      VEL=-.0314, TAU_V=0, MASS_FRACTION(1)=1., SPEC_ID(1)='natural gas'
      FREE_SLIP=.TRUE./ ! MASS_FLUX(1) = 0.02129

&VENT PBX=-.1, SURF_ID='OPEN'/
&VENT PBX= .1, SURF_ID='OPEN'/
&VENT PBY=-.1, SURF_ID='OPEN'/
&VENT PBY= .1, SURF_ID='OPEN'/
&VENT PBZ= .4, SURF_ID='OPEN'/

! add synthetic turbulence to break symmetry
&VENT XB=-.0355,.0355,-.0355,.0355,0,0
      XYZ=0,0,0, RADIUS=0.0355, SPREAD_RATE=1000
      SURF_ID='FLAME', N_EDDY=100, L_EDDY=0.008875, VEL_RMS=0.006 /

&VENT PBZ=0, SURF_ID='OPEN'/

&SLCF PBZ=0, QUANTITY='VELOCITY', VECTOR=.TRUE./
&SLCF PBY=0.001, QUANTITY='VELOCITY', VECTOR=.TRUE./
&SLCF PBY=0.001, QUANTITY='MASS FRACTION', SPEC_ID='natural gas', CELL_CENTERED=.TRUE./
&SLCF PBY=0.001, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE./
&SLCF PBY=0.001, QUANTITY='HRRPUV', CELL_CENTERED=.TRUE./
&SLCF PBY=0.001, QUANTITY='DIVERGENCE', CELL_CENTERED=.TRUE./
&SLCF PBY=0.001, QUANTITY='MIXTURE FRACTION', CELL_CENTERED=.TRUE./
&SLCF PBY=0.001, QUANTITY='HRRPUV', CELL_CENTERED=.TRUE./

&DEVC ID='T_mean',  XB=-.05,.05,0,0,0.10011,0.10011, STATISTICS_START=10, POINTS=50, QUANTITY='TEMPERATURE' /
&DEVC ID='1T_mean', XB=-.05,.05,0,0,0.04970,0.04970, STATISTICS_START=10, POINTS=50, QUANTITY='TEMPERATURE' /
&DEVC ID='2T_mean', XB=-.05,.05,0,0,0.00994,0.00994, STATISTICS_START=10, POINTS=50, QUANTITY='TEMPERATURE' /
&DEVC ID='3T_mean', XB=-.05,.05,0,0,0.00497,0.00497, STATISTICS_START=10, POINTS=50, QUANTITY='TEMPERATURE' /

&DEVC ID='f_mean',  XB=-.05,.05,0,0,0.10011,0.10011, STATISTICS_START=10, POINTS=50, QUANTITY='MIXTURE FRACTION' /
&DEVC ID='1f_mean', XB=-.05,.05,0,0,0.04970,0.04970, STATISTICS_START=10, POINTS=50, QUANTITY='MIXTURE FRACTION' /
&DEVC ID='2f_mean', XB=-.05,.05,0,0,0.00994,0.00994, STATISTICS_START=10, POINTS=50, QUANTITY='MIXTURE FRACTION' /
&DEVC ID='3f_mean', XB=-.05,.05,0,0,0.00497,0.00497, STATISTICS_START=10, POINTS=50, QUANTITY='MIXTURE FRACTION' /

&DEVC ID='U_mean',  XB=-.05,.05,0,0,0.04970,0.04970, STATISTICS_START=10, POINTS=50, QUANTITY='U-VELOCITY' /
&DEVC ID='1U_mean', XB=-.05,.05,0,0,0.03976,0.03976, STATISTICS_START=10, POINTS=50, QUANTITY='U-VELOCITY' /
&DEVC ID='2U_mean', XB=-.05,.05,0,0,0.02982,0.02982, STATISTICS_START=10, POINTS=50, QUANTITY='U-VELOCITY' /
&DEVC ID='3U_mean', XB=-.05,.05,0,0,0.01988,0.01988, STATISTICS_START=10, POINTS=50, QUANTITY='U-VELOCITY' /

&DEVC ID='W_mean',  XB=-.05,.05,0,0,0.06035,0.06035, STATISTICS_START=10, POINTS=50, QUANTITY='W-VELOCITY' /
&DEVC ID='1W_mean', XB=-.05,.05,0,0,0.04970,0.04970, STATISTICS_START=10, POINTS=50, QUANTITY='W-VELOCITY' /
&DEVC ID='2W_mean', XB=-.05,.05,0,0,0.03976,0.03976, STATISTICS_START=10, POINTS=50, QUANTITY='W-VELOCITY' /
&DEVC ID='3W_mean', XB=-.05,.05,0,0,0.02982,0.02982, STATISTICS_START=10, POINTS=50, QUANTITY='W-VELOCITY' /

&DEVC ID='UB_mean', XB=-.05,.05,0,0,0.03550,0.03550, STATISTICS_START=10, POINTS=50, QUANTITY='U-VELOCITY' /
&DEVC ID='UB_rms',  XB=-.05,.05,0,0,0.03550,0.03550, STATISTICS_START=10, POINTS=50, QUANTITY='U-VELOCITY', TEMPORAL_STATISTIC='RMS' /

&DEVC ID='VB_mean', XB=-.05,.05,0,0,0.03550,0.03550, STATISTICS_START=10, POINTS=50, QUANTITY='V-VELOCITY' /
&DEVC ID='VB_rms',  XB=-.05,.05,0,0,0.03550,0.03550, STATISTICS_START=10, POINTS=50, QUANTITY='V-VELOCITY', TEMPORAL_STATISTIC='RMS' /

&DEVC ID='WB_mean', XB=-.05,.05,0,0,0.03550,0.03550, STATISTICS_START=10, POINTS=50, QUANTITY='W-VELOCITY' /
&DEVC ID='WB_rms',  XB=-.05,.05,0,0,0.03550,0.03550, STATISTICS_START=10, POINTS=50, QUANTITY='W-VELOCITY', TEMPORAL_STATISTIC='RMS' /


&TAIL /
