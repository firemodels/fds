&HEAD CHID='methanol_30_cm', TITLE='30 cm methanol pool' /

&MESH IJK=20,20,30, XB=-0.6,0.6,-0.6,0.6,0.0,1.8 /

&TIME T_END=100. / 

&REAC FUEL               = 'METHANOL'
      CO_YIELD           = 0.001
      SOOT_YIELD         = 0.0 /

/  RADIATIVE_FRACTION=0.20 /
/  FYI                = 'Methanol C_1 H_4 O_1'
/ , HEAT_OF_VAPORIZATION=1104. /(with heat of vaporization in kJ/kg)


&MATL ID            = 'STEEL'
      EMISSIVITY    = 1.0
      DENSITY       = 7850.
      CONDUCTIVITY  = 45.8
      SPECIFIC_HEAT = 0.46 /

&SURF ID        = 'STEEL SHEET'
      COLOR     = 'BLACK'
      MATL_ID   = 'STEEL'
      BACKING   = 'EXPOSED'
      THICKNESS = 0.0015/

&VENT MB='XMIN', SURF_ID = 'OPEN' /
&VENT MB='XMAX', SURF_ID = 'OPEN' /
&VENT MB='YMIN', SURF_ID = 'OPEN' /
&VENT MB='YMAX', SURF_ID = 'OPEN' /
&VENT MB='ZMAX', SURF_ID = 'OPEN' /

/ &Vent XB=-0.15,0.15,-0.15,0.15,0.145,0.145, DIAMETER = 0.30, SURF_IDS='burner' /

&SURF ID='burner', HRRPUA=291., TMP_FRONT=64.8, COLOR='RED'

&OBST XB=-0.15,0.15,-0.15,0.15,0.00,0.145, SURF_IDS='burner','STEEL SHEET','STEEL SHEET' /

&OBST XB=-0.15,-0.15,-0.15,0.15,0.00,0.15, SURF_ID='STEEL SHEET' /
&OBST XB=0.15,0.15,-0.15,0.15,0.00,0.15, SURF_ID='STEEL SHEET' /
&OBST XB=-0.15,0.15,-0.15,-.15,0.00,0.15, SURF_ID='STEEL SHEET' /
&OBST XB=-0.15,0.15,0.15,0.15,0.00,0.15, SURF_ID='STEEL SHEET' /

&SLCF PBY=0.0, QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
&SLCF PBY=0.0, QUANTITY='HRRPUV' /

&DEVC XB=-0.15,0.15,-0.15,0.15,0.148,0.152,QUANTITY='GAUGE HEAT FLUX',STATISTICS='SURFACE INTEGRAL',ID='qdot'/

&OBST XB=0,0.60,0,0.03,0.15,0.15, SURF_ID='STEEL SHEET' /
&DEVC XB=0,0.60,0,0.03,0.15,0.15,QUANTITY='GAUGE HEAT FLUX',Points=21, IOR = 3, ID='qdown'/

&BNDF QUANTITY='GAUGE HEAT FLUX' /
&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='RADIATIVE HEAT FLUX' /
&BNDF QUANTITY='CONVECTIVE HEAT FLUX' /
&BNDF QUANTITY='NET HEAT FLUX' /

&TAIL /


/ &DUMP DT_HRR=5., SIG_FIGS=4, SIG_FIGS_EXP=2 /

/ &BNDF QUANTITY='BURNING RATE' /
/ &BNDF QUANTITY='WALL TEMPERATURE' /
/ &BNDF QUANTITY='BURNING RATE' /
