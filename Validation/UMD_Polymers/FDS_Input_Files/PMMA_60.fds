&HEAD CHID='PMMA_60', TITLE='PMMA Gasification at 60 kW/m2' /

&MESH IJK=3,3,4, XB=-0.15,0.15,-0.15,0.15,0.0,0.4 / 

&MISC Y_O2_INFTY=0.0001, SOLID_PHASE_ONLY=.TRUE., TMPA=97. /

&TIME T_END=400, WALL_INCREMENT=1, DT=0.1 /

&REAC FUEL ='CB GAS', C=1., H=4., SOOT_YIELD=0. /

&MATL ID='PMMA'
      ABSORPTION_COEFFICIENT=2240.
      N_REACTIONS=1
      A(1) = 8.6E12
      E(1) = 1.88E5
      EMISSIVITY=0.95
      DENSITY= 1160.
      SPEC_ID='CB GAS'
      NU_SPEC= 0.985
      MATL_ID='PMMA char'
      NU_MATL=0.015
      CONDUCTIVITY_RAMP ='k_PMMA'
      SPECIFIC_HEAT_RAMP = 'cp_PMMA'
      HEAT_OF_REACTION = 846. /

&MATL ID='PMMA char'
      ABSORPTION_COEFFICIENT=2240.
      EMISSIVITY=0.95
      DENSITY=1160.
      CONDUCTIVITY_RAMP = 'k_PMMA'
      SPECIFIC_HEAT_RAMP = 'cp_PMMA' /

&RAMP ID='k_PMMA', T= 27., F=0.336 /
&RAMP ID='k_PMMA', T=107., F=0.306 /
&RAMP ID='k_PMMA', T=117., F=0.176 /
&RAMP ID='k_PMMA', T=597., F=0.061 /

&RAMP ID='cp_PMMA', T= 27., F=1.69 /
&RAMP ID='cp_PMMA', T=597., F=3.76 /

&SURF ID='top'
      COLOR='BLACK'
      MATL_ID ='PMMA'
      THICKNESS= 0.006
      STRETCH_FACTOR=1.
      EXTERNAL_FLUX=60.
      TMP_GAS_BACK=37., TMP_INNER=32.
      HEAT_TRANSFER_COEFFICIENT=5. /

&VENT XB=-.05,0.05,-.05,0.05,0.0,0.0, SURF_ID='top' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DUMP DT_DEVC=10. /
&DEVC ID='MLR', XYZ=0.0,0.0,0.0, QUANTITY='BURNING RATE', IOR=3 /

&TAIL / 
