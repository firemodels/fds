&HEAD CHID='Case_F19', TITLE='Australian Grassland Fire Experiment, Case F19' /

&MESH IJK=80,80,40, XB=-20.0,20.0,0.0,40.0,0.0,20.0, MULT_ID='mesh' /
&MULT ID='mesh', DX=40., DY=40., DZ=20., I_LOWER=0, I_UPPER=5, J_LOWER=-3, J_UPPER=2, K_UPPER=0 /

&TIME T_END=180. / 

&MISC TMPA=34., U0=4.8, MEAN_FORCING(1)=.TRUE., DT_MEAN_FORCING=1. /

&REAC FUEL='CELLULOSE', C=6, H=10, O=5, SOOT_YIELD=0.015 /

&SPEC ID='WATER VAPOR' /

&SURF ID='IGNITION', HRRPUA=1460., COLOR='RED', TMP_FRONT=100., RAMP_Q='ignitor' /
&RAMP ID='ignitor', T= 0., F=0. /
&RAMP ID='ignitor', T= 1., F=1. /
&RAMP ID='ignitor', T=11., F=1. /
&RAMP ID='ignitor', T=12., F=0. /

&VENT XB=0.0,3.3,-87.,87.,0.,0., SURF_ID='IGNITION', XYZ=0,0,0, SPREAD_RATE=1.56 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&MATL ID='DIRT'
      CONDUCTIVITY = 0.25
      SPECIFIC_HEAT = 2.
      DENSITY = 1300. /

&SURF ID='GROUND' 
      DEFAULT=.TRUE. 
      MATL_ID='DIRT'
      THICKNESS=0.2 /

&SURF ID                        = 'wet vegetation'
      MATL_ID(1,1:2)            = 'KANGAROO GRASS','MOISTURE'
      MATL_MASS_FRACTION(1,1:2) = 0.942,0.058
      THICKNESS                 = 0.00016
      LENGTH                    = 0.51
      GEOMETRY                  = 'CYLINDRICAL' /
      
&MATL ID = 'KANGAROO GRASS'
      DENSITY = 512.
      CONDUCTIVITY = 0.1
      SPECIFIC_HEAT= 1.5
      N_REACTIONS = 1
      REFERENCE_TEMPERATURE = 200.
      NU_MATL = 0.2
      NU_SPEC = 0.8
      SPEC_ID = 'CELLULOSE'
      HEAT_OF_COMBUSTION = 15600.
      HEAT_OF_REACTION = 418.
      MATL_ID  = 'CHAR' /
 
&MATL ID = 'MOISTURE'
      DENSITY = 1000.
      CONDUCTIVITY = 0.1
      SPECIFIC_HEAT= 4.184
      N_REACTIONS = 1
      REFERENCE_TEMPERATURE = 100.
      NU_SPEC = 1.0
      SPEC_ID = 'WATER VAPOR'
      HEAT_OF_REACTION= 2500. /

&MATL ID = 'CHAR'
      DENSITY  = 100.
      CONDUCTIVITY = 0.1
      SPECIFIC_HEAT = 1.0 /

&PART ID='blade of grass', SAMPLING_FACTOR=20, SURF_ID='wet vegetation', PROP_ID='grass image'
      QUANTITIES='PARTICLE TEMPERATURE','PARTICLE MASS','PARTICLE DIAMETER', STATIC=.TRUE., COLOR='BROWN' /

&INIT PART_ID='blade of grass', XB=0.,200.,-100.,100.,0.00,0.51, N_PARTICLES=800000, MASS_PER_VOLUME=0.61  /

&PROP ID='grass image', SMOKEVIEW_ID='TUBE', SMOKEVIEW_PARAMETERS='L=0.51','D=0.00016','DIRZ=1' /

&DUMP DT_HRR=1., DT_DEVC=1., SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SLCF PBY=1., QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
&SLCF PBZ=2., QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBY=1., QUANTITY='INTEGRATED INTENSITY', CELL_CENTERED=.TRUE. /

&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='GAUGE HEAT FLUX' /

&DEVC ID='x1', QUANTITY='TEMPERATURE', STATISTICS='MAXLOC X', XB=-20, 20,-1,0,0,1 /
&DEVC ID='x2', QUANTITY='TEMPERATURE', STATISTICS='MAXLOC X', XB= 20, 60,-1,0,0,1 /
&DEVC ID='x3', QUANTITY='TEMPERATURE', STATISTICS='MAXLOC X', XB= 60,100,-1,0,0,1 /
&DEVC ID='x4', QUANTITY='TEMPERATURE', STATISTICS='MAXLOC X', XB=100,140,-1,0,0,1 /
&DEVC ID='x5', QUANTITY='TEMPERATURE', STATISTICS='MAXLOC X', XB=140,180,-1,0,0,1 /
&DEVC ID='x6', QUANTITY='TEMPERATURE', STATISTICS='MAXLOC X', XB=180,220,-1,0,0,1 /

&DEVC ID='T1', QUANTITY='TEMPERATURE', STATISTICS='MAX', XB=-20, 20,-1,0,0,1 /
&DEVC ID='T2', QUANTITY='TEMPERATURE', STATISTICS='MAX', XB= 20, 60,-1,0,0,1 /
&DEVC ID='T3', QUANTITY='TEMPERATURE', STATISTICS='MAX', XB= 60,100,-1,0,0,1 /
&DEVC ID='T4', QUANTITY='TEMPERATURE', STATISTICS='MAX', XB=100,140,-1,0,0,1 /
&DEVC ID='T5', QUANTITY='TEMPERATURE', STATISTICS='MAX', XB=140,180,-1,0,0,1 /
&DEVC ID='T6', QUANTITY='TEMPERATURE', STATISTICS='MAX', XB=180,220,-1,0,0,1 /

&TAIL /
