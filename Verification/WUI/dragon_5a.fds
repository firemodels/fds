&HEAD CHID='dragon_5a', TITLE='Manzello et al., FSJ (2008), Fig 5a' /
  
MESH IJK=50,25,20, XB=0,10,0,5,0,4 /
MESH IJK=75,45,70, XB=10,25,-2,7,0,14 /

&MESH IJK=50,25,20, XB=10,15,0,5,0,4 / ! testing purposes
  
&TIME T_END=130./

&DUMP DT_DEVC=10./
  
PROP ID='p1', VELOCITY_COMPONENT=1, P0=2. /
DEVC XB=10.8,11,2.4,2.6,2.05,2.25, QUANTITY='VELOCITY PATCH', PROP_ID='p1', DEVC_ID='t1' /
DEVC ID='t1', XYZ=10.9,2.5,2.15, QUANTITY='TIME', SETPOINT=0./
&DEVC ID='t2', XYZ=10.9,2.5,2.15, QUANTITY='TIME', SETPOINT=120., INITIAL_STATE=.TRUE./ ! needed to count the embers in the air at t=120
  
&OBST XB=10.0,25.0,-0.2,0.0,0.0,5.0, COLOR='TAN', TRANSPARENCY=0.5 /
&OBST XB=10.0,25.0, 5.0,5.2,0.0,5.0, COLOR='TAN', TRANSPARENCY=0.5 /
&OBST XB=10.8,11,2.4,2.6,0.0,2.05 COLOR='BLACK' /
  
SURF ID='wind_in', VEL=-0., COLOR='BLUE'/
  
VENT XB=0.0,0.0,0.0,5.0,0.0,4.0, SURF_ID='wind_in', N_EDDY=0, L_EDDY=0, VEL_RMS=0 /
VENT XB=25.0,25.0,-2.0,7.0,0.0,14.0, SURF_ID='OPEN' /

&VENT XB=10.0,10.0,0.0,5.0,0.0,4.0, SURF_ID='OPEN' / ! testing
&VENT XB=15.0,15.0,0.0,5.0,0.0,4.0, SURF_ID='OPEN' / ! testing
&VENT XB=10.0,15.0,0.0,5.0,4.0,4.0, SURF_ID='OPEN' / ! testing

&SLCF PBY=2.5, QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBZ=2.0, QUANTITY='VELOCITY', VECTOR=.TRUE. /

! effective density from mean mass (0.0331 g) and spherical volume from diameter of collected ember (L=13.5 mm, D=5.6 mm)
&PART ID='embers', DENSITY=360, DIAMETER=5600, MONODISPERSE=.TRUE., DRAG_LAW='CYLINDER', SAMPLING_FACTOR=1, COLOR='ORANGE', EVAPORATE=.FALSE., INITIAL_TEMPERATURE=1000., QUANTITIES(1:3)='DROPLET DIAMETER','DROPLET VELOCITY','DROPLET TEMPERATURE' /

&PART ID='embers1', DENSITY=360, DIAMETER=3900, MONODISPERSE=.TRUE., SAMPLING_FACTOR=1, COLOR='ORANGE', EVAPORATE=.FALSE., INITIAL_TEMPERATURE=1000., QUANTITIES(1:3)='DROPLET DIAMETER','DROPLET VELOCITY','DROPLET TEMPERATURE' /
&PART ID='embers2', DENSITY=360, DIAMETER=5200, MONODISPERSE=.TRUE., SAMPLING_FACTOR=1, COLOR='ORANGE', EVAPORATE=.FALSE., INITIAL_TEMPERATURE=1000., QUANTITIES(1:3)='DROPLET DIAMETER','DROPLET VELOCITY','DROPLET TEMPERATURE' /
&PART ID='embers3', DENSITY=360, DIAMETER=6100, MONODISPERSE=.TRUE., SAMPLING_FACTOR=1, COLOR='ORANGE', EVAPORATE=.FALSE., INITIAL_TEMPERATURE=1000., QUANTITIES(1:3)='DROPLET DIAMETER','DROPLET VELOCITY','DROPLET TEMPERATURE' /
&PART ID='embers4', DENSITY=360, DIAMETER=6800, MONODISPERSE=.TRUE., SAMPLING_FACTOR=1, COLOR='ORANGE', EVAPORATE=.FALSE., INITIAL_TEMPERATURE=1000., QUANTITIES(1:3)='DROPLET DIAMETER','DROPLET VELOCITY','DROPLET TEMPERATURE' /
&PART ID='embers5', DENSITY=360, DIAMETER=7400, MONODISPERSE=.TRUE., SAMPLING_FACTOR=1, COLOR='ORANGE', EVAPORATE=.FALSE., INITIAL_TEMPERATURE=1000., QUANTITIES(1:3)='DROPLET DIAMETER','DROPLET VELOCITY','DROPLET TEMPERATURE' /
&PART ID='embers6', DENSITY=360, DIAMETER=7900, MONODISPERSE=.TRUE., SAMPLING_FACTOR=1, COLOR='ORANGE', EVAPORATE=.FALSE., INITIAL_TEMPERATURE=1000., QUANTITIES(1:3)='DROPLET DIAMETER','DROPLET VELOCITY','DROPLET TEMPERATURE' /
&PART ID='embers7', DENSITY=360, DIAMETER=8700, MONODISPERSE=.TRUE., SAMPLING_FACTOR=1, COLOR='ORANGE', EVAPORATE=.FALSE., INITIAL_TEMPERATURE=1000., QUANTITIES(1:3)='DROPLET DIAMETER','DROPLET VELOCITY','DROPLET TEMPERATURE' /
