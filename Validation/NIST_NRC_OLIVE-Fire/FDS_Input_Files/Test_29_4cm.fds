&HVAC ID='Left Lower Leakage', TYPE_ID='LEAK', VENT_ID='Inside Left Lower Leakage', VENT2_ID='Outside Left Lower Leakage', AREA=0.0275,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /
&VENT XB=-0.84,-0.84,-0.68, 0.68, 1.68, 2.12, ID='Inside Left Upper Leakage', SURF_ID='STEEL PLATE' /
&VENT XB=-0.88,-0.88,-0.68, 0.68, 1.68, 2.12, ID='Outside Left Upper Leakage', SURF_ID='STEEL PLATE' /
