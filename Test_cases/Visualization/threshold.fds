&HEAD CHID='threshold',TITLE='Test of threshold temperature display SVN $Revision$' /

&MESH IJK=16,16,8 XB=0.0,1.6,0.0,1.6,0.0,0.8 /
&MESH IJK=16,16,8 XB=0.0,1.6,0.0,1.6,0.8,1.6 /

&TIME TWFIN=100. /
&DUMP NFRAMES=1000/

&MISC SURF_DEFAULT='GYPSUM BOARD' /
&SURF ID='BURNER', HRRPUA=3000., PART_ID='smoke' /

&MATL ID            = 'GYPSUM PLASTER'
      FYI           = 'Quintiere, Fire Behavior'
      CONDUCTIVITY  = 0.48
      SPECIFIC_HEAT = 0.84
      DENSITY       = 1440. /

&SURF ID             = 'GYPSUM BOARD'
      RGB            = 200,200,200
      MATL_ID        = 'GYPSUM PLASTER'
      THICKNESS      = 0.012 /


&PART ID='smoke', MASSLESS=.TRUE., SAMPLING_FACTOR=1 /

&VENT XB=0.2,0.6,0.6,1.0,0.0,0.0,SURF_ID='BURNER' /
&OBST XB=1.0,1.4,0.2,1.4,0.2,1.4,SURF_ID='GYPSUM BOARD' /

&VENT XB=0.0,0.0,0.0,1.6,0.0,0.8,SURF_ID='OPEN' /
&VENT XB=1.6,1.6,0.0,1.6,0.0,0.8,SURF_ID='OPEN' /
&VENT XB=0.0,1.6,0.0,0.0,0.0,0.8,SURF_ID='OPEN' /
&VENT XB=0.0,1.6,1.6,1.6,0.0,0.8,SURF_ID='OPEN' /


&ISOF QUANTITY='TEMPERATURE',VALUE=30.0 /
&ISOF QUANTITY='TEMPERATURE',VALUE=50.0 /
&ISOF QUANTITY='TEMPERATURE',VALUE=100.0 /

&SLCF PBX=0.8,QUANTITY='TEMPERATURE',VECTOR=.TRUE. /
&SLCF PBY=0.8,QUANTITY='TEMPERATURE',VECTOR=.TRUE. /
&SLCF PBZ=1.4,QUANTITY='TEMPERATURE',VECTOR=.TRUE. /
&SLCF PBZ=0.6,QUANTITY='TEMPERATURE',VECTOR=.TRUE. /

&BNDF QUANTITY='HEAT_FLUX' /
&BNDF QUANTITY='BURNING_RATE' /
&BNDF QUANTITY='WALL_TEMPERATURE' /

&TAIL /
