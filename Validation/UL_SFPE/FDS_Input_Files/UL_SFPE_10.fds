&HEAD CHID='UL_SFPE_10', TITLE='10 ft ceiling with 100 mm grid spacing' /

&MESH IJK=100,100,30, XB=10.25,20.25,10.25,20.25,0.0,3.0 /
&MESH IJK=100,100,30, XB=10.25,20.25,20.25,30.25,0.0,3.0 / 

&TIME TWFIN=600. / 

&MATL ID                 = 'CEILING TILE'
      FYI                = 'Data as provided by UL'
      CONDUCTIVITY       = 0.0611
      DENSITY            = 313
      SPECIFIC_HEAT      = 0.753 /

&SURF ID                 = 'CEILING TILE'
      MATL_ID            = 'CEILING TILE'
      COLOR              = 'BEIGE'
      THICKNESS          = 0.0158 
      BACKING            = 'EXPOSED' /

&REAC ID='HEPTANE'
      FUEL='N-HEPTANE'
      FYI='Heptane, C_7 H_16'
      C=7.
      H=8.
      CO_YIELD=0.006  
      SOOT_YIELD=0.015 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='CEILING TILE' /

&PART ID='heptane droplets',FUEL=.TRUE.,DENSITY=688.,
      QUANTITIES(1:2)='DROPLET_DIAMETER','DROPLET_TEMPERATURE',
      DIAMETER=100.,HEAT_OF_COMBUSTION=45000. /

&PROP ID='nozzle', SMOKEVIEW_ID='nozzle', PART_ID='heptane droplets', FLOW_RATE=0.511, FLOW_TAU=-300.,
      DROPLET_VELOCITY=5., SPRAY_ANGLE=0.,45.    /

&DEVC XYZ=14.75,15.25, 0.50, PROP_ID='nozzle', ORIENTATION=0,0,1, QUANTITY='TIME', SETPOINT=0., ID='noz_A' /
&DEVC XYZ=15.75,15.25, 0.50, PROP_ID='nozzle', ORIENTATION=0,0,1, QUANTITY='TIME', SETPOINT=0., ID='noz_B' /  
&DEVC XYZ=15.25,14.75, 0.50, PROP_ID='nozzle', ORIENTATION=0,0,1, QUANTITY='TIME', SETPOINT=0., ID='noz_D' / 
&DEVC XYZ=15.25,15.75, 0.50, PROP_ID='nozzle', ORIENTATION=0,0,1, QUANTITY='TIME', SETPOINT=0., ID='noz_G' / 

&PROP ID='HD-32',  QUANTITY='LINK TEMPERATURE', RTI=32.,  ACTIVATION_TEMPERATURE=1000. /
&PROP ID='HD-164', QUANTITY='LINK TEMPERATURE', RTI=164., ACTIVATION_TEMPERATURE=1000. /
&PROP ID='HD-287', QUANTITY='LINK TEMPERATURE', RTI=287., ACTIVATION_TEMPERATURE=1000. /

&DEVC XYZ=15.05,15.25, 2.89, PROP_ID='HD-287',        ID='NIST01' /
&DEVC XYZ=15.15,15.25, 2.89, PROP_ID='HD-164',        ID='NIST02' /
&DEVC XYZ=15.35,15.25, 2.89, PROP_ID='HD-32',         ID='NIST03' /
&DEVC XYZ=15.45,15.25, 2.89, QUANTITY='THERMOCOUPLE', ID='NIST04' /
&DEVC XYZ=15.05,17.41, 2.89, PROP_ID='HD-287',        ID='NIST05' /
&DEVC XYZ=15.15,17.41, 2.89, PROP_ID='HD-164',        ID='NIST06' /
&DEVC XYZ=15.35,17.41, 2.89, PROP_ID='HD-32',         ID='NIST07' /
&DEVC XYZ=15.45,17.41, 2.89, QUANTITY='THERMOCOUPLE', ID='NIST08' /
&DEVC XYZ=15.05,21.71, 2.89, PROP_ID='HD-287',        ID='NIST19' /
&DEVC XYZ=15.15,21.71, 2.89, PROP_ID='HD-164',        ID='NIST20' /
&DEVC XYZ=15.35,21.71, 2.89, PROP_ID='HD-32',         ID='NIST21' /
&DEVC XYZ=15.45,21.71, 2.89, QUANTITY='THERMOCOUPLE', ID='NIST22' /
&DEVC XYZ=15.05,26.03, 2.89, PROP_ID='HD-287',        ID='NIST33' /
&DEVC XYZ=15.15,26.03, 2.89, PROP_ID='HD-164',        ID='NIST34' /
&DEVC XYZ=15.35,26.03, 2.89, PROP_ID='HD-32',         ID='NIST35' /
&DEVC XYZ=15.45,26.03, 2.89, QUANTITY='THERMOCOUPLE', ID='NIST36' /

&SLCF PBX=15.25, QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
&SLCF PBX=15.25, QUANTITY='VELOCITY' /

&TAIL /

