&HEAD CHID='Sandia_He_1m_dx10cm', TITLE='Sandia 1m helium plume, 10 cm resolution' /

! Reference: T. J. O'Hern, E. J. Weckman, A. L. Gerhart, S. R. Tieszen, and R. W. Schefer,
! Experimental study of a turbulent buoyant helium plume, J. Fluid Mech., 544:143-171, 2005.

&MULT ID='mesh array', DX=1.5,DY=1.5,DZ=1.0, I_UPPER=1,J_UPPER=1,K_UPPER=3 /
&MESH IJK=15,15,10, XB=-1.5,0.0,-1.5,0.0,0,1, MULT_ID='mesh array' /

&MULT ID='outer mesh array', DX=1.5,DY=1.5,DZ=1.0,
      I_LOWER=0,I_LOWER_SKIP=1,I_UPPER_SKIP=2,I_UPPER=3,
      J_LOWER=0,J_LOWER_SKIP=1,J_UPPER_SKIP=2,J_UPPER=3,
      K_UPPER=3 /
&MESH IJK=15,15,10, XB=-3.0,-1.5,-3.0,-1.5,0,1, MULT_ID='outer mesh array' /

&TIME T_END=20. /

&DUMP SIG_FIGS=4, SIG_FIGS_EXP=2, DT_DEVC_LINE=10., STORE_SPECIES_FLUX=T /

&MISC TMPA=12.
      P_INF=80900.
      HUMIDITY=20.
      SIMULATION_MODE='LES'/

&SPEC ID='HELIUM',  LUMPED_COMPONENT_ONLY=T/
&SPEC ID='ACETONE', LUMPED_COMPONENT_ONLY=T/
&SPEC ID='OXYGEN',  LUMPED_COMPONENT_ONLY=T/
&SPEC ID='PLUME FLUID MIXTURE',
      SPEC_ID(1)='HELIUM',  VOLUME_FRACTION(1)=0.964,
      SPEC_ID(2)='ACETONE', VOLUME_FRACTION(2)=0.017,
      SPEC_ID(3)='OXYGEN',  VOLUME_FRACTION(3)=0.019/

&RADI RADIATION=.FALSE./

&MATL ID='STEEL', CONDUCTIVITY=54., SPECIFIC_HEAT=0.465, DENSITY=7850., EMISSIVITY=0.9 /
&SURF ID='PLATE', COLOR='GRAY', DEFAULT=.TRUE., MATL_ID='STEEL', THICKNESS=0.025 /

&SURF ID='PLUME', SPEC_ID='PLUME FLUID MIXTURE', MASS_FLUX(1)=0.0605, TMP_FRONT=12. / = 0.186 kg/m^3 * 0.325 m/s

&VENT XB=-0.5,0.5,-0.5,0.5,0,0, XYZ=0,0,0.0001, RADIUS=0.5, COLOR='BLUE', SURF_ID='PLUME', SPREAD_RATE=1000 /

&VENT XB=-1,1,-1,1,0,0, XYZ=0,0,0, RADIUS=1, SURF_ID='PLATE'/

&SURF ID='DEFAULT WALL', DEFAULT=T, COLOR='BEIGE', TRANSPARENCY=0.5/
&VENT MB='ZMIN',SURF_ID='OPEN'/
&VENT XB=-1.15,1.15,-1.15,1.15,3.999,3.999,SURF_ID='OPEN'/

&SLCF PBY=-.0625, QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBY=-.0625, QUANTITY='DENSITY', CELL_CENTERED=.TRUE. /
&SLCF PBY=-.0625, QUANTITY='MASS FRACTION', SPEC_ID='PLUME FLUID MIXTURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=-.0625, QUANTITY='WAVELET ERROR', QUANTITY2='MASS FRACTION', SPEC_ID='PLUME FLUID MIXTURE', CELL_CENTERED=.TRUE. /

Radial profiles of velocity and mass fraction

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, QUANTITY='CELL W', ID='Wp2', POINTS=16, X_ID='x' /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, QUANTITY='CELL W', ID='Wp4', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, QUANTITY='CELL W', ID='Wp6', POINTS=16, HIDE_COORDINATES=.TRUE. /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, QUANTITY='CELL U', ID='Up2', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, QUANTITY='CELL U', ID='Up4', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, QUANTITY='CELL U', ID='Up6', POINTS=16, HIDE_COORDINATES=.TRUE. /

