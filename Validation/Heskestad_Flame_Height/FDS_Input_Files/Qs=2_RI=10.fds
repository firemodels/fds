&CTRL ID='pct99', FUNCTION_TYPE='PERCENTILE', INPUT_ID='HRR', PERCENTILE=0.99 /
&DEVC ID='Lf_95', XYZ=0,0,0, QUANTITY='CONTROL VALUE', CTRL_ID='pct95', TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=31., UNITS='m' /
&DEVC ID='Lf_97', XYZ=0,0,0, QUANTITY='CONTROL VALUE', CTRL_ID='pct97', TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=31., UNITS='m' /
&DEVC ID='Lf_99', XYZ=0,0,0, QUANTITY='CONTROL VALUE', CTRL_ID='pct99', TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=31., UNITS='m' /

&TAIL /

