&HEAD CHID='favre_test', TITLE='NIST Propane Fire, 37 cm square pan, 2 cm resolution Favre avg' /

&MESH XB=-0.3,0.3,-0.3,0.3,-0.04,0.16, IJK=30,30,10, MULT_ID='m1' /
&MULT ID='m1', DX=0.6, DY=0.6, DZ=0.2, K_UPPER=2 / 3 mesh

&TIME T_END=10. /

&DUMP DT_DEVC=0.0001 /

&REAC FUEL='PROPANE' /

&SURF ID='POOL', COLOR='LIGHT BLUE', SPEC_ID='PROPANE', MASS_FLUX=0.0068, TAU_MF=0. /

&VENT MB='ZMIN', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /

&OBST XB=-0.185,0.185,-0.185,0.185,-0.040, 0.000, SURF_IDS='POOL','INERT','INERT' /

&SLCF PBY=0.001, QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBY=0.001, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.001, QUANTITY='MASS FRACTION', SPEC_ID='OXYGEN', CELL_CENTERED=.TRUE. /

&DEVC XB= 0,0,0,0,0.06,0.46, QUANTITY='MASS FRACTION', SPEC_ID='OXYGEN', ID='YTILDE_O2', POINTS=3, TEMPORAL_STATISTIC='FAVRE AVERAGE' /

&DEVC XYZ= 0,0,0.06, QUANTITY='DENSITY', ID='RHO_1' /
&DEVC XYZ= 0,0,0.26, QUANTITY='DENSITY', ID='RHO_2' /
&DEVC XYZ= 0,0,0.46, QUANTITY='DENSITY', ID='RHO_3' /
&DEVC XYZ= 0,0,0.06, QUANTITY='MASS FRACTION', SPEC_ID='OXYGEN', ID='YO2_1' /
&DEVC XYZ= 0,0,0.26, QUANTITY='MASS FRACTION', SPEC_ID='OXYGEN', ID='YO2_2' /
&DEVC XYZ= 0,0,0.46, QUANTITY='MASS FRACTION', SPEC_ID='OXYGEN', ID='YO2_3' /

&TAIL /
