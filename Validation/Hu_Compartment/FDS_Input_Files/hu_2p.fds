&HEAD CHID='hu_2p',TITLE='Hu et al. IAFSS (2005) compartment fire Case 2 prescribed mass loss rate'/

&MESH IJK=36,40,40, XB=-0.16,0.2,-0.2,0.2,0.0,0.4/ main box

&MESH IJK=40,40,20, XB=-.2,-.16,-.2,.2,0.38,.40/ top inside
&MESH IJK=40,40,20, XB=-.2,-.16,-.2,.2,0.00,.02/ bot inside
&MESH IJK= 4,40,36, XB=-.2,-.16,-.2,.2,0.02,.38/ mid inside  (min 4 cells in a given direction for Poisson solver)

&TIME T_END=250./

&DUMP DT_DEVC=1./

&MISC CHECK_VN=.TRUE./

&REAC FUEL       = 'N-HEPTANE'
      SOOT_YIELD = 0.037/

&VENT XB=-0.2,-0.2,-0.2,0.2,0.39,0.4,ID='INLET1',SURF_ID='OPEN',COLOR='BLUE'/
&VENT XB=-0.2,-0.2,-0.2,0.2,0.0,0.01,ID='INLET2',SURF_ID='OPEN',COLOR='GREEN'/

&VENT XB=-0.095,0.095,-0.095,0.095,0.00,0.00, XYZ=0,0,0, RADIUS=0.095, SURF_ID='FIRE', COLOR='RED'/

&SURF ID='FIRE', SPEC_ID(1)='N-HEPTANE', MASS_FLUX(1)=0.028, RAMP_MF(1)='case2'/

&RAMP ID='case2' T=0, F=0.18/
&RAMP ID='case2' T=50, F=0.75/
&RAMP ID='case2' T=100, F=1.0/
&RAMP ID='case2' T=150, F=1.0/
&RAMP ID='case2' T=210, F=0.71/
&RAMP ID='case2' T=226, F=0.21/
&RAMP ID='case2' T=250, F=0.1/

&SURF ID        = 'INSULATION'
      MATL_ID   = 'CERAMIC FIBER'
      COLOR     = 'GRAY'
      THICKNESS = 0.020
      HEAT_TRANSFER_MODEL='LOGLAW'
      DEFAULT   = .TRUE. /

&MATL ID                 = 'CERAMIC FIBER'
      CONDUCTIVITY_RAMP  = 'k_fiber'
      SPECIFIC_HEAT      = 1.04
      DENSITY            =  128.
      EMISSIVITY         = 0.97 /
&RAMP ID='k_fiber',T= 20.,F=0.09 /
&RAMP ID='k_fiber',T=300.,F=0.09 /
&RAMP ID='k_fiber',T=600.,F=0.17 /
&RAMP ID='k_fiber',T=900.,F=0.25 /

&SLCF PBY=0., QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE./
&SLCF PBY=0., QUANTITY='VELOCITY', VECTOR=.TRUE./

&BNDF QUANTITY='YPLUS'/

