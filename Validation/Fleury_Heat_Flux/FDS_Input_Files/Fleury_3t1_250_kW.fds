&HEAD CHID='Fleury_3t1_250_kW', TITLE='Fleury Heat Flux Experiment' /

&MESH IJK=64,64,60, XB=-1.00,2.20,-1.00,2.20,0.00,3.00 /

&TIME T_END=30. /

&SURF ID='burner', HRRPUA=925.9, TMP_FRONT=100., COLOR='RED' /
&OBST XB=-0.15, 0.15,-.45,0.45,0.00,0.30, SURF_IDS='burner','INERT','INERT' /

&REAC FUEL='PROPANE', SOOT_YIELD=0.015 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DUMP SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SURF ID='target', RADIUS=0.001, EMISSIVITY=1., GEOMETRY='SPHERICAL' /
&PART ID='rad gauge f', STATIC=.TRUE., ORIENTATION(1,1:3)=-1,0,0, SURF_ID='target' /
&PART ID='rad gauge s', STATIC=.TRUE., ORIENTATION(1,1:3)=0,-1,0, SURF_ID='target' /

&INIT ID='f000', XYZ=0.45,0.0,0.3, NUMBER_INITIAL_PARTICLES=34, DX=0.05, PART_ID='rad gauge f' /
&INIT ID='f050', XYZ=0.45,0.0,0.8, NUMBER_INITIAL_PARTICLES=34, DX=0.05, PART_ID='rad gauge f' /
&INIT ID='f100', XYZ=0.45,0.0,1.3, NUMBER_INITIAL_PARTICLES=34, DX=0.05, PART_ID='rad gauge f' /
&INIT ID='f150', XYZ=0.45,0.0,1.8, NUMBER_INITIAL_PARTICLES=34, DX=0.05, PART_ID='rad gauge f' /

&INIT ID='s000', XYZ=0.0,0.45,0.3, NUMBER_INITIAL_PARTICLES=34, DY=0.05, PART_ID='rad gauge s' /
&INIT ID='s050', XYZ=0.0,0.45,0.8, NUMBER_INITIAL_PARTICLES=34, DY=0.05, PART_ID='rad gauge s' /
&INIT ID='s100', XYZ=0.0,0.45,1.3, NUMBER_INITIAL_PARTICLES=34, DY=0.05, PART_ID='rad gauge s' /
&INIT ID='s150', XYZ=0.0,0.45,1.8, NUMBER_INITIAL_PARTICLES=34, DY=0.05, PART_ID='rad gauge s' /

&DEVC ID='f000', INIT_ID='f000', POINTS=34, QUANTITY='RADIATIVE HEAT FLUX', X_ID='x distance' /
&DEVC ID='f050', INIT_ID='f050', POINTS=34, QUANTITY='RADIATIVE HEAT FLUX', HIDE_COORDINATES=.TRUE. /
&DEVC ID='f100', INIT_ID='f100', POINTS=34, QUANTITY='RADIATIVE HEAT FLUX', HIDE_COORDINATES=.TRUE. /
&DEVC ID='f150', INIT_ID='f150', POINTS=34, QUANTITY='RADIATIVE HEAT FLUX', HIDE_COORDINATES=.TRUE. /

&DEVC ID='s000', INIT_ID='s000', POINTS=34, QUANTITY='RADIATIVE HEAT FLUX', Y_ID='y distance' /
&DEVC ID='s050', INIT_ID='s050', POINTS=34, QUANTITY='RADIATIVE HEAT FLUX', HIDE_COORDINATES=.TRUE. /
&DEVC ID='s100', INIT_ID='s100', POINTS=34, QUANTITY='RADIATIVE HEAT FLUX', HIDE_COORDINATES=.TRUE. /
&DEVC ID='s150', INIT_ID='s150', POINTS=34, QUANTITY='RADIATIVE HEAT FLUX', HIDE_COORDINATES=.TRUE. /

&SLCF PBY=0.0, QUANTITY='TEMPERATURE' /
&SLCF PBY=0.0, QUANTITY='INTEGRATED INTENSITY'  /

&TAIL /

