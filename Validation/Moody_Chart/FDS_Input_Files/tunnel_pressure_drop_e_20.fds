! Test of tunnel friction factor
!
! Case matrix
! -----------
!
! Case A: Vel = 2 m/s, Roughness = 1.E-4 m
! Case B: Vel = 2 m/s, Roughness = 1.E-1 m
! Case C: Vel = 10 m/s, Roughness = 1.E-4 m
! Case D: Vel = 10 m/s, Roughness = 1.E-1 m
! Case E: Vel = 4 m/s, Roughness = 1.E-2 m

&HEAD CHID='tunnel_pressure_drop_e_20', TITLE='LES of flow through a rough tunnel'/

&MULT ID='m1', DX=20, I_UPPER=79/ 80 mesh case
&MESH IJK=20,50,20, XB=0,20,-6.4,6.4,0,5, MULT_ID='m1'/

&TIME T_END=800.0/
&DUMP DT_DEVC=400, DT_BNDF=400, VELOCITY_ERROR_FILE=T/

&MISC SIMULATION_MODE='LES'/
&PRES SOLVER='GLMAT'/

&SURF ID='inflow', VEL=-4, COLOR='BLUE'/
&SURF ID='rough wall',
      COLOR='GRAY',
      DEFAULT=T,
      ROUGHNESS=1.0E-2/

&VENT PBX=0, SURF_ID='inflow'/
&VENT PBX=1600, SURF_ID='OPEN'/

&SLCF PBY=0.0, QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0.0, QUANTITY='PRESSURE', CELL_CENTERED=T/

&BNDF QUANTITY='VISCOUS WALL UNITS', CELL_CENTERED=T/

&DEVC XB=700,700,-6.4,6.4,0,5, QUANTITY='U-VELOCITY', STATISTICS='MEAN', ID='UBAR'/
&DEVC XB=700,700,-6.4,6.4,0,5, QUANTITY='DENSITY', STATISTICS='MEAN', ID='RHO'/
&DEVC XB=700,700,-6.4,6.4,0,5, QUANTITY='MOLECULAR VISCOSITY', STATISTICS='MEAN', ID='MU'/

&DEVC XB=10,10,-6.4,6.4,0,5, QUANTITY='PRESSURE', STATISTICS='MEAN', ID='P10'/
