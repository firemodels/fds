&HEAD CHID='pine_21O2_25_1C', TITLE='Kashiwagi et al. Combust. Flame (1987), white pine gasification, 21 vol% O2, 25 kW/m2'/

&MESH IJK=8,8,8, XB=-0.028,0.048,-0.038,0.038,-0.038,0.038/

&TIME T_END=900., WALL_INCREMENT=1/

&DUMP DT_DEVC=10., DT_PROF=10., DT_RESTART=10./

&MISC TMPA=30., TEST_NEW_CHAR_MODEL=T/

&RADI NUMBER_RADIATION_ANGLES=16, TIME_STEP_INCREMENT=1, ANGLE_INCREMENT=1/

&VENT DB='ZMIN', SURF_ID='inflow'/
&VENT DB='ZMAX', SURF_ID='OPEN'/

&SURF ID='inflow', VEL=-0.01, COLOR='BLUE'/
&SURF ID='sample', COLOR='BROWN'/
&SURF ID='exposed sample', CONVECTION_LENGTH_SCALE=0.019, BLOWING=T, STRETCH_FACTOR=1,
      THICKNESS=0.038, BACKING='EXPOSED', MATL_ID(1,1:2)='PINE','MOISTURE', MATL_MASS_FRACTION(1,1:2)=0.95,0.05,
      COLOR='GRAY 10', EXTERNAL_FLUX=25./
&SURF ID='default wall', EMISSIVITY=0., HEAT_TRANSFER_COEFFICIENT=0., COLOR='INVISIBLE', FREE_SLIP=T, DEFAULT=T/

&OBST XB=0,0.038,-0.019,0.019,-0.019,0.019, SURF_ID6='exposed sample','sample','sample','sample','sample','sample'/

&SPEC ID='NITROGEN', BACKGROUND=T/
&SPEC ID='OXYGEN', MASS_FRACTION_0=0.232/ 21 vol % O2 to mass fraction
&SPEC ID='CARBON DIOXIDE'/
&SPEC ID='FUEL VAPOR'/
&SPEC ID='WATER VAPOR'/

! simple material for setup
! &MATL ID='PINE', DENSITY=360., SPECIFIC_HEAT=1.15, CONDUCTIVITY=0.3 /

! full material model with kinetics
&CATF OTHER_FILES='MATL.fds'/

&BNDF QUANTITY='MASS FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='MASS FLUX', SPEC_ID='OXYGEN', CELL_CENTERED=T/
&BNDF QUANTITY='WALL TEMPERATURE', CELL_CENTERED=T/
&BNDF QUANTITY='BACK WALL TEMPERATURE', CELL_CENTERED=T/
&BNDF QUANTITY='TOTAL HEAT FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='RADIATIVE HEAT FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='CONVECTIVE HEAT FLUX', CELL_CENTERED=T/
&BNDF QUANTITY='WALL THICKNESS', CELL_CENTERED=T/
&BNDF QUANTITY='SURFACE DENSITY', CELL_CENTERED=T/
&BNDF QUANTITY='SURFACE OXYGEN MASS FRACTION', CELL_CENTERED=T/
&BNDF QUANTITY='SURFACE OXYGEN ITERATIONS', CELL_CENTERED=T/
&BNDF QUANTITY='HEAT TRANSFER COEFFICIENT', CELL_CENTERED=T/

&DEVC XB=-0.028,0.048,-0.038,0.038,-0.038,0.038, QUANTITY='MASS FLUX', SURF_ID='exposed sample', SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='mf', CONVERSION_FACTOR=69252, UNITS='mg/cm2 s'/
&DEVC XB=-0.028,0.048,-0.038,0.038,-0.038,0.038, QUANTITY='MASS FLUX', SPEC_ID='OXYGEN',         SURF_ID='exposed sample', SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='norm mf O2',  CONVERSION_FACTOR=-25937, UNITS='mg/cm2 s'/ 69252/(-2.67)
&DEVC XB=-0.028,0.048,-0.038,0.038,-0.038,0.038, QUANTITY='MASS FLUX', SPEC_ID='CARBON DIOXIDE', SURF_ID='exposed sample', SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='norm mf CO2', CONVERSION_FACTOR= 18973, UNITS='mg/cm2 s'/ 69252/( 3.65)

&DEVC XB=-0.028,0.048,-0.038,0.038,-0.038,0.038, QUANTITY='MASS FLUX', SPEC_ID='OXYGEN',         SURF_ID='exposed sample', SPATIAL_STATISTIC='SURFACE INTEGRAL', TEMPORAL_STATISTIC='TIME INTEGRAL', ID='NORM MASS O2 CONSUMED',  CONVERSION_FACTOR=-0.374531835205993/ -1/2.67
&DEVC XB=-0.028,0.048,-0.038,0.038,-0.038,0.038, QUANTITY='MASS FLUX', SPEC_ID='CARBON DIOXIDE', SURF_ID='exposed sample', SPATIAL_STATISTIC='SURFACE INTEGRAL', TEMPORAL_STATISTIC='TIME INTEGRAL', ID='NORM MASS CO2 PRODUCED', CONVERSION_FACTOR= 0.273972602739726/  1/3.65

&DEVC XB=-0.028,0.048,-0.038,0.038,-0.038,0.038, QUANTITY='SURFACE DENSITY', SURF_ID='exposed sample', SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='MASS', SMOOTHING_TIME=10/
&DEVC XB=-0.028,0.048,-0.038,0.038,-0.038,0.038, QUANTITY='SURFACE DENSITY', MATL_ID='ASH', SURF_ID='exposed sample', SPATIAL_STATISTIC='SURFACE INTEGRAL', TEMPORAL_STATISTIC='INSTANT VALUE', ID='NORM MASS ASH PRODUCED', CONVERSION_FACTOR= 50./ 1/0.02

&CTRL ID='PID WT LOSS', FUNCTION_TYPE='PID', INPUT_ID='MASS', PROPORTIONAL_GAIN=0, DIFFERENTIAL_GAIN=1, INTEGRAL_GAIN=0/
&DEVC XYZ=0,0,0, ID='wt loss mf', QUANTITY='CONTROL VALUE', CTRL_ID='PID WT LOSS', CONVERSION_FACTOR=-69252, UNITS='mg/cm2 s'/

&DEVC XYZ=-0.001,0,0, QUANTITY='WALL TEMPERATURE', IOR=-1, ID='T0mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=0.005, IOR=-1, ID='T5mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=0.010, IOR=-1, ID='T10mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=0.015, IOR=-1, ID='T15mm'/
&DEVC XYZ=-0.001,0,0, QUANTITY='BACK WALL TEMPERATURE', IOR=-1, ID='T38mm'/

&PROF ID='Tx' , XYZ=-0.001,0,0, QUANTITY='INSIDE WALL TEMPERATURE', IOR=-1 /
&PROF ID='MOISTUREx', XYZ=-0.001,0,0, QUANTITY='SOLID DENSITY', MATL_ID='MOISTURE', IOR=-1 /
&PROF ID='PINEx', XYZ=-0.001,0,0, QUANTITY='SOLID DENSITY', MATL_ID='PINE', IOR=-1 /
&PROF ID='CHARx', XYZ=-0.001,0,0, QUANTITY='SOLID DENSITY', MATL_ID='CHAR', IOR=-1 /
&PROF ID='ASHx' , XYZ=-0.001,0,0, QUANTITY='SOLID DENSITY', MATL_ID='ASH' , IOR=-1 /
&PROF ID='O2x'  , XYZ=-0.001,0,0, QUANTITY='SOLID OXYGEN MASS FRACTION', MATL_ID='CHAR' , IOR=-1 /

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0, QUANTITY='TEMPERATURE', CELL_CENTERED=T/
&SLCF PBY=0, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN', CELL_CENTERED=T/

&TAIL /
