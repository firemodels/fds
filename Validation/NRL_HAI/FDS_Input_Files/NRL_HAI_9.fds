&HEAD CHID='NRL_HAI_9', TITLE='Back, Beyler, DiNenno and Tatem Wall Heat Flux Measurements, Test 9' /

&MESH IJK=32,32,96, XB=0.00,2.368,-1.184,1.184,-0.074,7.03 /

&TIME T_END=60. /

&REAC SOOT_YIELD=0.015 FUEL='PROPANE' /

&SURF ID='burner', HRRPUA=1067., TMP_FRONT=50., COLOR='TAN' /
&OBST XB=0.00,0.70,-.35,0.35,-.10,0.00,SURF_IDS='burner','STEEL WALL','STEEL WALL' /

&MATL ID            = 'GYPSUM PLASTER'
      FYI           = 'Quintiere, Fire Behavior'
      CONDUCTIVITY  = 0.48
      SPECIFIC_HEAT = 0.84
      DENSITY       = 1440. /
&MATL ID            = 'STEEL'
      FYI           = 'Drysdale, Intro to Fire Dynamics'
      EMISSIVITY    = 0.95
      SPECIFIC_HEAT = 0.46
      CONDUCTIVITY  = 45.8
      DENSITY       = 7850. /
&MATL ID            = 'CONCRETE'
      FYI           = 'Drysdale, Intro to Fire Dynamics'
      SPECIFIC_HEAT = 0.90
      DENSITY       = 2000.
      EMISSIVITY    = 0.95
      CONDUCTIVITY  = 1.0  /

&SURF ID         = 'FLOOR'
      MATL_ID    = 'CONCRETE'
      THICKNESS  = 0.1
      COLOR      = 'GRAY' /
&SURF ID         = 'STEEL WALL'
      MATL_ID    = 'STEEL'
      THICKNESS  = 0.002
      COLOR      = 'BLACK' /
&SURF ID            = 'GYPSUM BOARD'
      MATL_ID       = 'GYPSUM PLASTER'
      COLOR         = 'ANTIQUE WHITE'
