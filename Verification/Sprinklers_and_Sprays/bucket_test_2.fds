&HEAD CHID='bucket_test_2', TITLE='Single Sprinkler Test Case, SVN $Revision$' /

&MESH IJK=50,50,25, XB=-5.0,5.0,-5.0,5.0,0.0,5.0 /

&TIME T_END=10. /

&RADI RADIATION=.FALSE./

&SPEC ID='WATER VAPOR'/
&PART ID='water drops', SPEC_ID='WATER VAPOR', QUANTITIES(1:3)='PARTICLE DIAMETER','PARTICLE TEMPERATURE','PARTICLE AGE',
      DIAMETER=1750., SAMPLING_FACTOR=1 /

&PROP ID='K-11', QUANTITY='SPRINKLER LINK TEMPERATURE', OFFSET=0.50, PART_ID='water drops', 
      FLOW_RATE=60.,SPRAY_PATTERN_TABLE='TABLE1', PARTICLE_VELOCITY=10., SMOKEVIEW_ID='sprinkler_upright' /

&TABL ID='TABLE1',TABLE_DATA=30,31,0,1,5,0.2/
&TABL ID='TABLE1',TABLE_DATA=30,31,179,180,5,0.8/

&DEVC XYZ=0.0,0.0,4.9, PROP_ID='K-11', ID='Spr_1', QUANTITY='TIME', SETPOINT=5.,INITIAL_STATE=.TRUE.  /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /

&SLCF PBY=0.0,QUANTITY='TEMPERATURE' /
&SLCF PBY=0.0,QUANTITY='MASS FRACTION',SPEC_ID='WATER VAPOR' /
&SLCF PBY=0.0,QUANTITY='MPUV', PART_ID='water drops' /
&SLCF PBZ=0.1,QUANTITY='PARTICLE FLUX Z', PART_ID='water drops' /
&SLCF PBZ=2.5,QUANTITY='PARTICLE FLUX Z', PART_ID='water drops' /

&BNDF QUANTITY='AMPUA', PART_ID='water drops' /  Accumulated Mass Per Unit Area, i.e., the Buckets

&TAIL /
