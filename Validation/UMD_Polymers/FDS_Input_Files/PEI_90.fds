&RAMP ID='k_PEI_int', T=457., F=0.5887 /

&RAMP ID='cp_PEI_int', T=27., F=1.68240 /
&RAMP ID='cp_PEI_int', T=457., F=1.81484 /

&RAMP ID='k_PEI_char', T=27., F=0.4952 /
&RAMP ID='k_PEI_char', T=457., F=0.55298 /

&RAMP ID='cp_PEI_char', T=27., F=1.31224 /
&RAMP ID='cp_PEI_char', T=457., F=1.32978 /

&SURF ID='top'
      COLOR='BLACK'
