&HEAD CHID='geom_s=p1_dpdx=-p01_N8', TITLE='LES of flow through a rough channel' /

&MESH IJK=64,8,8, XB=0,8,-.5,.5,0,1 /

&TIME T_END=500.0 /

&WIND FORCE_VECTOR(1)=.01,STRATIFICATION=.FALSE. /

&DUMP NFRAMES=100, SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SURF ID='rough wall', DEFAULT=.TRUE., COLOR='GRAY', ROUGHNESS=0.1 /

&VENT MB='XMIN', SURF_ID='PERIODIC' /
&VENT MB='XMAX', SURF_ID='PERIODIC' /

&GEOM XB=-2,10,-.75,-.50,-0.25,1.25 , SURF_ID='rough wall' / front
&GEOM XB=-2,10, .50, .75,-0.25,1.25 , SURF_ID='rough wall' / back
&GEOM XB=-2,10,-.50, .50, 1.00,1.25 , SURF_ID='rough wall' / top
&GEOM XB=-2,10,-.50, .50,-0.25,0.00 , SURF_ID='rough wall' / bottom

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBX=4, QUANTITY='VELOCITY', VECTOR=.TRUE. /

&DEVC XB=4.0,4.0, -0.5,0.5, 0.0,1.0, QUANTITY='U-VELOCITY', STATISTICS='MEAN' /
&DEVC XYZ=4.0,0.0,0.5, QUANTITY='U-VELOCITY' /
&DEVC XYZ=4.0,0.0,0.5, QUANTITY='VISCOSITY' /
&DEVC XYZ=4.0,0.0,0.5, QUANTITY='DENSITY' /

&TAIL /
