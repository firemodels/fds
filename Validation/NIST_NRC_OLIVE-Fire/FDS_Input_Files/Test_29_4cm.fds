&VENT XB= 0.84, 0.84,-0.68, 0.68, 0.08, 0.72, ID='Inside Right Lower Leakage', SURF_ID='STEEL PLATE' /
&VENT XB= 0.88, 0.88,-0.68, 0.68, 0.08, 0.72, ID='Outside Right Lower Leakage', SURF_ID='STEEL PLATE' /
&HVAC ID='Right Lower Leakage', TYPE_ID='LEAK', VENT_ID='Inside Right Lower Leakage', VENT2_ID='Outside Right Lower Leakage', AREA=0.0275,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /
&VENT XB= 0.84, 0.84,-0.68, 0.68, 1.68, 2.12, ID='Inside Right Upper Leakage', SURF_ID='STEEL PLATE' /
&VENT XB= 0.88, 0.88,-0.68, 0.68, 1.68, 2.12, ID='Outside Right Upper Leakage', SURF_ID='STEEL PLATE' /
&HVAC ID='Right Upper Leakage', TYPE_ID='LEAK', VENT_ID='Inside Right Upper Leakage', VENT2_ID='Outside Right Upper Leakage', AREA=0.0275,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /

 SURF ID='BLOW', COLOR='BLUE', VOLUME_FLOW=-0.125, TAU_V=200. /
 VENT XB=-0.08, 0.08,-0.08,0.08, 0.0, 0.0, SURF_ID='BLOW' /
 DEVC ID='Vdot', QUANTITY='NORMAL VELOCITY', XB=-0.08, 0.08,-0.08,0.08, 0.0, 0.0, SPATIAL_STATISTIC='SURFACE INTEGRAL',
      CONVERSION_FACTOR=3600. /

&DUMP DT_DEVC=10., DT_HRR=10. /

&SLCF PBY=0.03, QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
&SLCF PBY=0.03, QUANTITY='PRESSURE', CELL_CENTERED=.TRUE. /
