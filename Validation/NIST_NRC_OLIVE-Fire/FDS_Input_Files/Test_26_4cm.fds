&HEAD CHID='Test_26_4cm', TITLE='Methane burner in a cabinet' /

&MESH IJK=12,12,12, XB=-0.48,0.0,-0.48,0.0,0.0,0.48, MULT_ID='mesh' /
&MULT ID='mesh', DX=0.48, DY=0.48, DZ=0.48, I_LOWER=-1, I_UPPER=2, J_LOWER=-1, J_UPPER=2, K_UPPER=4 /

&TIME T_END=900., TIME_SHRINK_FACTOR=1 /

&MATL ID            = 'STEEL'
      SPECIFIC_HEAT = 0.465
      CONDUCTIVITY  = 54.
      DENSITY       = 7833. /

&SURF ID            = 'STEEL PLATE'
      MATL_ID       = 'STEEL'
      DEFAULT       = .TRUE.
      THICKNESS     = 0.003
      COLOR         = 'GRAY 40' /

&PRES VELOCITY_TOLERANCE=0.002, MAX_PRESSURE_ITERATIONS=50 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /


&REAC FUEL='METHANE', RADIATIVE_FRACTION=0., N_SIMPLE_CHEMISTRY_REACTIONS=2, AUTO_IGNITION_TEMPERATURE=540.   AIT_EXCLUSION_ZONE= 0.04, 0.36,-0.16,0.16,0.50,0.56 /

&SURF ID='FIRE', HRRPUA=488.3, COLOR='ORANGE', TMP_FRONT=300., RAMP_Q='fire ramp' /
&RAMP ID='fire ramp', T=   0., F= 0.000 /
&RAMP ID='fire ramp', T=  10., F= 1.060 /
&RAMP ID='fire ramp', T= 240., F= 1.060 /
&RAMP ID='fire ramp', T= 250., F= 2.000 /
&RAMP ID='fire ramp', T= 520., F= 2.000 /
&RAMP ID='fire ramp', T= 530., F= 3.046 /
&RAMP ID='fire ramp', T= 750., F= 3.046 /
&RAMP ID='fire ramp', T= 760., F= 0.000 /

&OBST XB= 0.04, 0.36,-0.16, 0.16,0.32,0.52, SURF_IDS='FIRE','STEEL PLATE','STEEL PLATE' /

&OBST XB=-0.80, 0.80,-0.52,-0.48, 0.00, 2.28 / Front
&OBST XB=-0.80, 0.80, 0.48, 0.52, 0.00, 2.28 / Back
&OBST XB=-0.80,-0.76,-0.48, 0.48, 0.00, 2.28 / Left
&OBST XB= 0.76, 0.80,-0.48, 0.48, 0.00, 2.28 / Right
&OBST XB=-0.80, 0.80,-0.52, 0.52, 2.28, 2.32 / Top

&VENT XB=-0.76,-0.76,-0.32, 0.32,1.80,2.08, ID='Inside Upper Vent',  SURF_ID='STEEL PLATE' /
&VENT XB=-0.80,-0.80,-0.32, 0.32,1.80,2.08, ID='Outside Upper Vent', SURF_ID='STEEL PLATE' /
&HVAC ID='Upper Vent', TYPE_ID='LEAK', VENT_ID='Inside Upper Vent', VENT2_ID='Outside Upper Vent', AREA=0.015,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /
&VENT XB=-0.76,-0.76,-0.32, 0.32,0.32,0.60, ID='Inside Lower Vent',  SURF_ID='STEEL PLATE' /
&VENT XB=-0.80,-0.80,-0.32, 0.32,0.32,0.60, ID='Outside Lower Vent', SURF_ID='STEEL PLATE' /
&HVAC ID='Lower Vent', TYPE_ID='LEAK', VENT_ID='Inside Lower Vent', VENT2_ID='Outside Lower Vent', AREA=0.015,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /

&VENT XB= 0.76, 0.76,-0.48, 0.48, 0.08, 0.72, ID='Inside Lower Leakage', SURF_ID='STEEL PLATE' /
&VENT XB= 0.80, 0.80,-0.48, 0.48, 0.08, 0.72, ID='Outside Lower Leakage', SURF_ID='STEEL PLATE' /
&HVAC ID='Lower Leakage', TYPE_ID='LEAK', VENT_ID='Inside Lower Leakage', VENT2_ID='Outside Lower Leakage', AREA=0.029,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /
&VENT XB= 0.76, 0.76,-0.48, 0.48, 1.64, 2.28, ID='Inside Upper Leakage', SURF_ID='STEEL PLATE' /
&VENT XB= 0.80, 0.80,-0.48, 0.48, 1.64, 2.28, ID='Outside Upper Leakage', SURF_ID='STEEL PLATE' /
&HVAC ID='Upper Leakage', TYPE_ID='LEAK', VENT_ID='Inside Upper Leakage', VENT2_ID='Outside Upper Leakage', AREA=0.014,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /

 SURF ID='BLOW', COLOR='BLUE', VOLUME_FLOW=-0.125, TAU_V=200. /
 VENT XB=-0.08, 0.08,-0.08,0.08, 0.0, 0.0, SURF_ID='BLOW' /
 DEVC ID='Vdot', QUANTITY='NORMAL VELOCITY', XB=-0.08, 0.08,-0.08,0.08, 0.0, 0.0, SPATIAL_STATISTIC='SURFACE INTEGRAL',
      CONVERSION_FACTOR=3600. /

&DUMP DT_DEVC=10., DT_HRR=10. /

&SLCF PBY=0.03, QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
&SLCF PBY=0.03, QUANTITY='PRESSURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='VOLUME FRACTION', SPEC_ID='METHANE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='MASS FRACTION', SPEC_ID='SOOT', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='HRRPUV', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='PRESSURE', CELL_CENTERED=.TRUE. /

&BNDF QUANTITY='WALL TEMPERATURE', CELL_CENTERED=.TRUE. /

&DEVC ID='HRR_ideal', XB= 0.04, 0.36,-0.16, 0.16,0.50,0.56, QUANTITY='MASS FLUX', SPEC_ID='METHANE',
      SPATIAL_STATISTIC='SURFACE INTEGRAL', CONVERSION_FACTOR=50000, UNITS='kW' /
&DEVC ID='HRR_actual',XB=-2.00,2.00,-2.00,2.000,0.00,3.00, QUANTITY='HRRPUV',
      SPATIAL_STATISTIC='VOLUME INTEGRAL', TEMPORAL_STATISTIC='SMOOTHED', SMOOTHING_FACTOR=0.99 /
&DEVC ID='O2',       XYZ=-0.14,-0.38,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='OXYGEN' /
&DEVC ID='CO2',      XYZ=-0.14,-0.38,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='CARBON DIOXIDE' /
&DEVC ID='CO',       XYZ=-0.14,-0.38,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='CARBON MONOXIDE' /
&DEVC ID='TC-1',     XYZ=-0.14,-0.38,2.00, QUANTITY='THERMOCOUPLE' /
&DEVC ID='TC-2',     XYZ=-0.14,-0.38,1.40, QUANTITY='THERMOCOUPLE' /
&DEVC ID='TC-3',     XYZ=-0.14,-0.38,0.50, QUANTITY='THERMOCOUPLE' /
&DEVC ID='CH4',      XYZ=-0.14,-0.38,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='METHANE' /
&DEVC ID='SOOT',     XYZ=-0.14,-0.38,2.00, QUANTITY='MASS FRACTION',   SPEC_ID='SOOT' /
&DEVC ID='PRESSURE', XYZ=-0.14,-0.38,2.00, QUANTITY='PRESSURE' /

&TAIL/

