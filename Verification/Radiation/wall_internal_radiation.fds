      THICKNESS=0.1
      HEAT_TRANSFER_COEFFICIENT=0.0
      TMP_INNER = 1000. /

&VENT XB = 0.0,0.1,-0.05,0.05,0.0,0.0, SURF_ID = 'TAU_0.01' /
&VENT XB = 0.1,0.2,-0.05,0.05,0.0,0.0, SURF_ID = 'TAU_0.1' /
&VENT XB = 0.2,0.3,-0.05,0.05,0.0,0.0, SURF_ID = 'TAU_0.5' /
