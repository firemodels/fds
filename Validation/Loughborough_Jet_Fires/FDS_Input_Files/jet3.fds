&HEAD CHID='jet3', TITLE='Loughborough University Jet Fire Experiments' /

&TIME T_BEGIN=-60., T_END=30./

&MISC POROUS_FLOOR=F, HUMIDITY=50. /

&MESH IJK=40,40,25, XB=-4.0,4.0,-4.0,4.0,0,5.0, MULT_ID='mesh'  /
&MULT ID='mesh', DX=8, DY=8, DZ=5, I_LOWER=-3, I_UPPER=1, J_LOWER=-6, J_UPPER=4, K_UPPER=1 /  110 meshes

&VENT PBZ=0.   , SURF_ID='GROUND' /
&VENT MB='ZMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /

&WIND DIRECTION=267., PRESSURE_GRADIENT_FORCE=0.08 /

&SURF ID='GROUND', ROUGHNESS=0.5 /

&REAC FUEL ='METHANE'
      SOOT_YIELD = 0.01
      CO_YIELD=0.001
      RADIATIVE_FRACTION=0.202 /

&RADI NUMBER_RADIATION_ANGLES=600, PATH_LENGTH=100 /

&DEVC ID='nozzle_1', XYZ=-21.61,0,3.25, PROP_ID='nozzle', ORIENTATION=1,0,0, QUANTITY='TIME', SETPOINT=0. /

&PART ID='methane droplets',SPEC_ID='METHANE', ADHERE_TO_SOLID=-1, QUANTITIES(1:3)='PARTICLE DIAMETER','PARTICLE TEMPERATURE','PARTICLE AGE',
      DIAMETER=1000., INITIAL_TEMPERATURE=-160., SAMPLING_FACTOR=1 /

&PROP ID='nozzle', PART_ID='methane droplets', MASS_FLOW_RATE=19.5, FLOW_RAMP='flow',
