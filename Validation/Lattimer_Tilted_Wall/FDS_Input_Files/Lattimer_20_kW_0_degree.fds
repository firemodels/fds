&HEAD CHID='Lattimer_20_kW_0_degree', TITLE='Surface temperature on slopped wall, 20 kW propane fire' /

&MESH IJK=60,60,60, XB=-0.75,0.75,-0.75,0.75,-0.20,1.30 /

MULT ID='m1', DX=.5, DY=.5, DZ=.5, I_LOWER=-1,I_UPPER=1, J_LOWER=-1,J_UPPER=1, K_UPPER=2/
MESH IJK=20,20,20, XB=-0.25,0.25,-0.25,0.25,-0.20,0.30, MULT_ID='m1' /

&TIME T_END=500./

&DUMP DT_SLCF=0.1, DT_BNDF=1., DT_DEVC_LINE=100., DT_DEVC=10. /

&MISC BNDF_DEFAULT=.FALSE. /

&DEVC XYZ=0,0,0, ID='burner on', QUANTITY='TIME', SETPOINT=400., INITIAL_STATE=.TRUE. /
&SURF ID='burner', HRRPUA=222., TMP_FRONT=400., COLOR='RED' /
&VENT XB=0.00,0.30,-0.15,0.15,0.001,0.001, SURF_ID='burner', DEVC_ID='burner on' /
&OBST XB=0.00,0.30,-0.15,0.15,-0.10,0.00 /

&MATL ID='ceramic', CONDUCTIVITY=.15, SPECIFIC_HEAT=1., DENSITY=272. /
&SURF ID='ceramic board', EMISSIVITY=0.95, COLOR='BLACK', MATL_ID='ceramic', THICKNESS=0.0254, BACKING='EXPOSED', HEAT_TRANSFER_MODEL='LOGLAW' /
&OBST XB=-0.0254,-0.0,-0.305,0.305,0,0.915, SURF_ID='ceramic board', BNDF_FACE(1)=.TRUE. /

&REAC FUEL='PROPANE', SOOT_YIELD=0.01/

&VENT PBX=-0.75, SURF_ID='OPEN' /
&VENT PBX= 0.75, SURF_ID='OPEN' /
&VENT PBY=-0.75, SURF_ID='OPEN' /
&VENT PBY= 0.75, SURF_ID='OPEN' /
&VENT PBZ= 1.30, SURF_ID='OPEN' /

&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='VISCOUS WALL UNITS' /

DEVC XB=0,0,0,0,0,0.8, QUANTITY='WALL TEMPERATURE', ID='TIR', IOR=1, POINTS=15 /

&DEVC XYZ=0,0,0.075, QUANTITY='WALL TEMPERATURE', ID='TC1', IOR=1 /
