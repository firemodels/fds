&HEAD CHID='7p1_cm_methane_4mm', TITLE='Purdue 7.1 cm methane burner' / Y. Xin et al. Combustion and Flame 141 (2005) 329-335
  
&MESH IJK=50,50,100, XB=-.1,.1,-.1,.1,0,0.4 /
  
&TIME T_END=20/

&DUMP DT_DEVC_LINE=10/
      
&MISC TMPA=15.
      FLUX_LIMITER=4
      RESEARCH_MODE=.TRUE.
      CHECK_VN=.TRUE.,
      VN_MIN=0.4,VN_MAX=0.5 /
  
&REAC FUEL='METHANE'
      C=1.
      H=4.
      HEAT_OF_COMBUSTION=50350.
      SOOT_YIELD=0.01
      SUPPRESSION=.FALSE. /
  
&RADI RADIATIVE_FRACTION=0.1 /
  
&SURF ID='FLAME'
      COLOR='ORANGE'
      MASS_FLUX(1) = 0.02129, SPEC_ID(1) = 'METHANE'
      FREE_SLIP=.TRUE. /
  
&VENT MB='XMIN', SURF_ID='OPEN'/
&VENT MB='XMAX', SURF_ID='OPEN'/
&VENT MB='YMIN', SURF_ID='OPEN'/
&VENT MB='YMAX', SURF_ID='OPEN'/
&VENT MB='ZMAX', SURF_ID='OPEN'/

! weak synthetic turbulence, 1% intensity, just to break symmetry
&VENT XB=-.0355,.0355,-.0355,.0355,0,0
      XYZ=0,0,0, RADIUS=0.0355
      SURF_ID='FLAME', N_EDDY=10, L_EDDY=0.01775, VEL_RMS=0.0003 /

&VENT MB='ZMIN', SURF_ID='OPEN'/

&SLCF PBZ=0, QUANTITY='VELOCITY', VECTOR=.TRUE./
&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=.TRUE./
&SLCF PBY=0, QUANTITY='MASS FRACTION', SPEC_ID='METHANE', CELL_CENTERED=.TRUE./
&SLCF PBY=0, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE./
&SLCF PBY=0, QUANTITY='HRRPUV', CELL_CENTERED=.TRUE./
&SLCF PBY=0, QUANTITY='DIVERGENCE', CELL_CENTERED=.TRUE./
&SLCF PBY=0, QUANTITY='MIXTURE FRACTION', CELL_CENTERED=.TRUE./
    
&DEVC ID='T_mean', XB=-.05,.05,0,0,0.10011,0.10011, POINTS=50, QUANTITY='TEMPERATURE' /
