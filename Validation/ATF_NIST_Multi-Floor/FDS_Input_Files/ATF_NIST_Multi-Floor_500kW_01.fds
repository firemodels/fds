      CONDUCTIVITY  = 1.8        
      SPECIFIC_HEAT = 1.04       
      DENSITY       =  2280. /

&MATL ID            = 'STEEL'
      FYI           = 'Drysdale, Intro to Fire Dynamics'
      EMISSIVITY    = 0.95
      SPECIFIC_HEAT = 0.46
      CONDUCTIVITY  = 45.8
      DENSITY       = 7850. /

&SURF ID        = 'GYPSUM BOARD'
      MATL_ID   = 'GYPSUM'
      COLOR     = 'BEIGE'
