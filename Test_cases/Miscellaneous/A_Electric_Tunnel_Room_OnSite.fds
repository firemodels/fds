## Example Case for EPRI Seabrook Training Course 2008

&HEAD CHID='A_Electric_Tunnel_Room_OnSite', TITLE='Evaluation of Cabinet fire on Cable Trays.' /

&TIME T_END=0 / Set-up Only
 TIME T_BEGIN=-60, T_END=1800 /

&MISC SURF_DEFAULT='CONCRETE_SURFACE', BNDF_DEFAULT=.FALSE. /

&MESH ID='MESH', IJK=72,48,37, XB=0.00,14.60,0.00,9.80,0.00,7.60 / 20 cm MESH resolution
 MESH ID='MESH', IJK=144,96,74, XB=0.00,14.60,0.00,9.80,0.00,7.60 / 10 cm MESH resolution

&REAC ID='PE/PVC'
      C = 2.0
      H = 3.5
      OTHER = 0.5
      MW_OTHER=34.0
      HEAT_OF_COMBUSTION=24000.0
      SOOT_YIELD=0.1 /

&MATL ID           = 'CONCRETE'
    FYI            = 'Properties from NUREG-1805 pg. 2-11'
    SPECIFIC_HEAT  = 0.750
    CONDUCTIVITY   = 1.6
    DENSITY        = 2400.0 /

&SURF ID            = 'CONCRETE_SURFACE'
     COLOR          = 'GRAY 65'
     MATL_ID        = 'CONCRETE'
     THICKNESS      = 0.6 /

<Ventilation>

<Recirculation>
&SURF ID             = 'SUPPLY_VENT'
      COLOR          = 'BLUE 4'
      VOLUME_FLUX    = -0.1180
      RAMP_V	   = 'AIR_FLOW'
      POROUS         =.TRUE. /

&RAMP ID='AIR_FLOW', T=-60.0, F=0.0 /
&RAMP ID='AIR_FLOW', T=-30.0, F=1.0 /

&OBST XB=1.096,1.350,8.49,8.49,4.3985,4.6015, SURF_ID='SUPPLY_VENT' / supply vent 1
&OBST XB=2.446,2.700,8.49,8.49,4.3985,4.6015, SURF_ID='SUPPLY_VENT' / supply vent 2
&OBST XB=3.796,4.050,8.49,8.49,4.3985,4.6015, SURF_ID='SUPPLY_VENT' / supply vent 3
&OBST XB=5.146,5.400,8.49,8.49,4.3985,4.6015, SURF_ID='SUPPLY_VENT' / supply vent 4
&OBST XB=6.496,6.750,8.49,8.49,4.3985,4.6015, SURF_ID='SUPPLY_VENT' / supply vent 5
&OBST XB=7.846,8.100,8.49,8.49,4.3985,4.6015, SURF_ID='SUPPLY_VENT' / supply vent 6
&OBST XB=9.196,9.450,8.49,8.49,4.3985,4.6015, SURF_ID='SUPPLY_VENT' / supply vent 7
&OBST XB=10.546,10.800,8.49,8.49,4.3985,4.6015, SURF_ID='SUPPLY_VENT' / supply vent 8
&OBST XB=11.896,12.150,8.49,8.49,4.3985,4.6015, SURF_ID='SUPPLY_VENT' / supply vent 9
&OBST XB=13.246,13.500,8.49,8.49,4.3985,4.6015, SURF_ID='SUPPLY_VENT' / supply vent 10

</Recirculation>

<Dampers>

&VENT XB=0.0,0.0,1.0,1.3048,6.1,6.7, SURF_ID='OPEN' / Damper Vent 1
&VENT XB=14.6,14.6,1.0,1.3048,6.1,6.7, SURF_ID='OPEN' / Damper Vent 2

&OBST XB=0.0,0.1,1.0,1.3048,6.1,6.7, DEVC_ID='Link_1' / Damper 1
&OBST XB=14.5,14.6,1.0,1.3048,6.1,6.7, DEVC_ID='Link_2' / Damper 2

&PROP ID='Damper_Link', QUANTITY='LINK TEMPERATURE', ACTIVATION_TEMPERATURE=74., RTI=180.0/

&DEVC XYZ=0.1,1.1524,6.7, ID='Link_1', INITIAL_STATE=.FALSE., PROP_ID='Damper_Link' /
&DEVC XYZ=14.5,1.1524,6.7, ID='Link_2', INITIAL_STATE=.FALSE., PROP_ID='Damper_Link' /

</Dampers>

<\Ventilation>

<Fire>

&SURF ID             = 'CABINET_FIRE'
      COLOR          = 'ORANGE'
      HRRPUA         = 211.
      RAMP_Q         = 'fire' /

&RAMP ID='fire', T=   0, F=0.0000 /
&RAMP ID='fire', T=  30, F=0.0017 /
&RAMP ID='fire', T=  60, F=0.0069 /
&RAMP ID='fire', T=  90, F=0.0156 /
&RAMP ID='fire', T= 120, F=0.0278 /
&RAMP ID='fire', T= 150, F=0.0434 /
&RAMP ID='fire', T= 180, F=0.0625 /
&RAMP ID='fire', T= 210, F=0.0851 /
&RAMP ID='fire', T= 240, F=0.1111 /
&RAMP ID='fire', T= 270, F=0.1406 /
&RAMP ID='fire', T= 300, F=0.1736 /
&RAMP ID='fire', T= 330, F=0.2101 /
&RAMP ID='fire', T= 360, F=0.2500 /
&RAMP ID='fire', T= 390, F=0.2934 /
&RAMP ID='fire', T= 420, F=0.3403 /
&RAMP ID='fire', T= 450, F=0.3906 /
&RAMP ID='fire', T= 480, F=0.4444 /
&RAMP ID='fire', T= 510, F=0.5017 /
&RAMP ID='fire', T= 540, F=0.5625 /
&RAMP ID='fire', T= 570, F=0.6267 /
&RAMP ID='fire', T= 600, F=0.6944 /
&RAMP ID='fire', T= 630, F=0.7656 /
&RAMP ID='fire', T= 660, F=0.8403 /
&RAMP ID='fire', T= 690, F=0.9184 /
&RAMP ID='fire', T= 720, F=1.0000 /
&RAMP ID='fire', T=1200, F=1.0000 /
&RAMP ID='fire', T=1500, F=0.0000 /

&VENT XB=12.2712,13.2712,4.6816,5.6816,2.41,2.41, SURF_ID='CABINET_FIRE' /

<\Fire>

<Geometry>

<Cable Trays>

&MATL   ID                      = 'CABLE'
        SPECIFIC_HEAT           = 1.289
        CONDUCTIVITY            = 0.192
        DENSITY                 = 1380.0 /

&SURF   ID                      = 'CABLE_TRAY_A'
        COLOR                   = 'BLACK'
        MATL_ID                 = 'CABLE'
        HRRPUA                  = 265.
        IGNITION_TEMPERATURE    = 270.
        RAMP_Q                  = 'CABLE_FIRE_RAMP'
        THICKNESS               = 0.1016 /

&SURF   ID                      = 'CABLE_TRAY_B'
        COLOR                   = 'BLACK'
        MATL_ID                 = 'CABLE'
        HRRPUA                  = 265.
        IGNITION_TEMPERATURE    = 270.
        RAMP_Q                  = 'CABLE_FIRE_RAMP'
        THICKNESS               = 0.1016 /

&SURF   ID                      = 'CABLE_TRAY_C'
        COLOR                   = 'BLACK'
        MATL_ID                 = 'CABLE'
        HRRPUA                  = 265.
        IGNITION_TEMPERATURE    = 270.
        RAMP_Q                  = 'CABLE_FIRE_RAMP'
        THICKNESS               = 0.1016 /

&RAMP ID='CABLE_FIRE_RAMP', T= 0.0, F=0.0 /
&RAMP ID='CABLE_FIRE_RAMP', T= 5.0, F=1.0 /

&OBST XB=12.314,13.2284,0.0,9.8,3.0,3.2032, SURF_ID='CABLE_TRAY_A', BNDF_OBST=.TRUE. / cable tray A
&OBST XB=12.314,13.2284,0.0,9.8,4.9,5.1032, SURF_ID='CABLE_TRAY_B', BNDF_OBST=.TRUE. / cable tray B
&OBST XB=6.8428,7.7572,0.0,9.8,7.3968,7.6, SURF_ID='CABLE_TRAY_C', BNDF_OBST=.TRUE. / cable tray C

<\Cable Trays>

<Cabinet>

&MATL ID             = 'STEEL'
      FYI            = 'Properties from NUREG-1805 pg. 2-11'
      SPECIFIC_HEAT  = 0.465
      CONDUCTIVITY   = 54.0
      DENSITY        = 7850.0 /
      
&SURF ID             = 'CABINET_SURFACE'
      COLOR          = 'OLIVE'
      MATL_ID        = 'STEEL'
      THICKNESS      = 0.01 /
      
&OBST XB=12.1616,13.3808,1.2192,5.7912,0.0,2.4, SURF_ID='CABINET_SURFACE', BNDF_OBST=.TRUE. / cabinet

<\Cabinet>

<Doors>

&OBST XB=0.0,0.06,7.0568,7.9712,0.20,2.4, CTRL_ID='FIRE_BRIGADE', BNDF_OBST=.TRUE./ Door 1
&OBST XB=14.54,14.6,8.8856,9.8,0.20,2.4, CTRL_ID='FIRE_BRIGADE', BNDF_OBST=.TRUE./ Door 2

&VENT XB=0.0,0.0,7.0568,7.9712,0.0,2.4, SURF_ID='OPEN' / Door 1 Vent
&VENT XB=14.6,14.6,8.8856,9.8,0.0,2.4, SURF_ID='OPEN' / Door 2 Vent

&CTRL ID='FIRE_BRIGADE', FUNCTION_TYPE='TIME_DELAY', INPUT_ID='SMOKE_DETECTORS', DELAY=900., INITIAL_STATE=.TRUE. /
&CTRL ID='SMOKE_DETECTORS', FUNCTION_TYPE='ANY', INPUT_ID='Ion1','Ion2','Ion3','Ion4','Photo1','Photo2','Photo3','Photo4' /

<\Doors>

<\Geometry>


<Detectors>

&PROP ID='Cleary Ionization I1', QUANTITY='spot obscuration', ALPHA_E=2.50, BETA_E=-0.70, ALPHA_C=0.80, BETA_C=-0.90/
&PROP ID='Cleary Photoelectric P1', QUANTITY='spot obscuration', ALPHA_E=1.80, BETA_E=-1.00, ALPHA_C=1.00, BETA_C=-0.80/

&DEVC ID='Ion1', PROP_ID='Cleary Ionization I1', XYZ=10.70,2.40,7.60, LATCH=.FALSE. /
&DEVC ID='Ion2', PROP_ID='Cleary Ionization I1', XYZ=14.00,2.40,7.60, LATCH=.FALSE. /
&DEVC ID='Ion3', PROP_ID='Cleary Ionization I1', XYZ=10.70,7.00,7.60, LATCH=.FALSE. /
&DEVC ID='Ion4', PROP_ID='Cleary Ionization I1', XYZ=14.00,7.00,7.60, LATCH=.FALSE. /

&DEVC ID='Photo1', PROP_ID='Cleary Photoelectric P1', XYZ=10.70,2.40,7.60, LATCH=.FALSE. /
&DEVC ID='Photo2', PROP_ID='Cleary Photoelectric P1', XYZ=14.00,2.40,7.60, LATCH=.FALSE. /
&DEVC ID='Photo3', PROP_ID='Cleary Photoelectric P1', XYZ=10.70,7.00,7.60, LATCH=.FALSE. /
&DEVC ID='Photo4', PROP_ID='Cleary Photoelectric P1', XYZ=14.00,7.00,7.60, LATCH=.FALSE. /

&DEVC ID='LAYER_HEIGHT', QUANTITY='LAYER HEIGHT', XB=7.30,7.30,4.90,4.90,0.00,7.60/
&DEVC ID='LL_TEMP', QUANTITY='LOWER TEMPERATURE', XB=7.30,7.30,4.90,4.90,0.00,7.60/
&DEVC ID='UL_TEMP', QUANTITY='UPPER TEMPERATURE', XB=7.30,7.30,4.90,4.90,0.00,7.60/

<\Detectors>

<Sprinklers>

&PROP ID='EPRI_Sprinkler', QUANTITY='SPRINKLER LINK TEMPERATURE', RTI=148., C_FACTOR=0.7, ACTIVATION_TEMPERATURE=74., OFFSET=0.10, PART_ID='water drops', FLOW_RATE=80.0, DROPLET_VELOCITY=5., SPRAY_ANGLE=30.,80. /

&PART ID='water drops', WATER=.TRUE. /

&CTRL ID='nozzle trigger', FUNCTION_TYPE='ALL', INPUT_ID='smokey','delay' / 
&CTRL ID='delay', FUNCTION_TYPE='TIME_DELAY', INPUT_ID='smokey', DELAY=30. / 
&CTRL ID='smokey', FUNCTION_TYPE='AT_LEAST', N=2, INPUT_ID='Ion1','Ion2','Ion3','Ion4','Photo1','Photo2','Photo3','Photo4' /

&DEVC XYZ=12.7712,4.9,4.0, PROP_ID='EPRI_Sprinkler', QUANTITY='CONTROL', ID='Sprinkler_01', CTRL_ID='nozzle trigger' /

</Sprinklers>

<Output>

&BNDF QUANTITY='GAUGE_HEAT_FLUX' /
&BNDF QUANTITY='WALL_TEMPERATURE' /

&SLCF PBX=7.30, QUANTITY='TEMPERATURE' / Middle of Room X
&SLCF PBX=12.77, QUANTITY='TEMPERATURE' / Through Fire Vent X
&SLCF PBX=9.0, QUANTITY='TEMPERATURE' / Through Return Vent X

&SLCF PBY=4.9, QUANTITY='TEMPERATURE' / Middle of Room Y
&SLCF PBY=5.18, QUANTITY='TEMPERATURE' / Through Fire Vent Y
&SLCF PBX=6.75, QUANTITY='TEMPERATURE' / Through Return Vent Y
&SLCF PBY=7.514, QUANTITY='TEMPERATURE' / Through Door 1 Vent Y
&SLCF PBY=9.3428, QUANTITY='TEMPERATURE' / Through Door 2 Vent Y

&SLCF PBZ=4.5, QUANTITY='TEMPERATURE' / Through Supply Vents Z
&SLCF PBZ=7.1, QUANTITY='TEMPERATURE' / 0.5 Meters down from Ceiling Z
&SLCF PBZ=0.2, QUANTITY='TEMPERATURE' / 0.2 Meters up from Floor Z
&SLCF PBZ=1.0, QUANTITY='TEMPERATURE' / 1.0 Meters up from Floor Z

DEVC QUANTITY='WALL TEMPERATURE', ID='Temperature_Cable_Tray_A', XYZ=10.0,3.5,8.894, IOR=-3, SURF_ID='CABLE_TRAY_A', STATISTICS='MAX' /
DEVC QUANTITY='WALL TEMPERATURE', ID='Temperature_Cable_Tray_B', XYZ=10.0,3.5,8.894, IOR=-3, SURF_ID='CABLE_TRAY_B', STATISTICS='MAX' /
DEVC QUANTITY='WALL TEMPERATURE', ID='Temperature_Cable_Tray_B', XYZ=10.0,3.5,8.894, IOR=-3, SURF_ID='CABLE_TRAY_C', STATISTICS='MAX' /
DEVC QUANTITY='GAUGE HEAT FLUX', ID='HeatFlux_Cable_Tray_A', XYZ=2.75,3.5,2.95, IOR=-3, SURF_ID='CABLE_TRAY_A', STATISTICS='MAX' /
DEVC QUANTITY='GAUGE HEAT FLUX', ID='HeatFlux_Cable_Tray_B', XYZ=2.75,3.5,2.95, IOR=-3, SURF_ID='CABLE_TRAY_B', STATISTICS='MAX' /
DEVC QUANTITY='GAUGE HEAT FLUX', ID='HeatFlux_Cable_Tray_C', XYZ=2.75,3.5,2.95, IOR=-3, SURF_ID='CABLE_TRAY_C', STATISTICS='MAX' /

<\Output>

&TAIL/