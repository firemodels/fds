&HEAD CHID='Test_23_4cm', TITLE='Methane burner in a cabinet' /

&MESH IJK=18,12,12, XB=-0.72,0.0,-0.48,0.0,0.0,0.48, MULT_ID='mesh' /
&MULT ID='mesh', DX=0.72, DY=0.48, DZ=0.48, I_LOWER=-1, I_UPPER=2, J_LOWER=-1, J_UPPER=2, K_UPPER=4 /

&TIME T_END=900., TIME_SHRINK_FACTOR=1 /

&MATL ID            = 'STEEL'
      SPECIFIC_HEAT = 0.465
      CONDUCTIVITY  = 54.
      DENSITY       = 7833. /

&SURF ID            = 'STEEL PLATE'
      MATL_ID       = 'STEEL'
      DEFAULT       = .TRUE.
      THICKNESS     = 0.003
      COLOR         = 'GRAY 40' /

&PRES VELOCITY_TOLERANCE=0.002, MAX_PRESSURE_ITERATIONS=50 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /


&REAC FUEL='METHANE', RADIATIVE_FRACTION=0., N_SIMPLE_CHEMISTRY_REACTIONS=2, AUTO_IGNITION_TEMPERATURE=540.   AIT_EXCLUSION_ZONE=-0.96,-0.64,0.12,0.44,0.60,0.64 /

&SURF ID='FIRE', HRRPUA=488.3, COLOR='ORANGE', TMP_FRONT=300., RAMP_Q='fire ramp' /
&RAMP ID='fire ramp', T=   0., F=0.000 /
&RAMP ID='fire ramp', T=  10., F=1.050 /
&RAMP ID='fire ramp', T= 310., F=1.050 /
&RAMP ID='fire ramp', T= 320., F=1.500 /
&RAMP ID='fire ramp', T= 660., F=1.500 /
&RAMP ID='fire ramp', T= 670., F=0.000 /

&OBST XB=-0.96,-0.64,0.12,0.44,0.40,0.60, SURF_IDS='FIRE','STEEL PLATE','STEEL PLATE' /

&OBST XB=-1.28, 1.28,-0.52,-0.48, 0.00, 2.28 / Front
&OBST XB=-1.28, 1.28, 0.48, 0.52, 0.00, 2.28 / Back
&OBST XB=-1.28,-1.24,-0.48, 0.48, 0.00, 2.28 / Left
&OBST XB= 1.24, 1.28,-0.48, 0.48, 0.00, 2.28 / Right
&OBST XB=-1.28, 1.28,-0.52, 0.52, 2.28, 2.32 / Top

&VENT XB=-0.88,-0.44,-0.48,-0.48,1.52,1.76, ID='Inside Upper Vent',  SURF_ID='STEEL PLATE' /
