&HEAD CHID='NIST_He_14400_LC_ULV',TITLE='Helium release experiment 14400 seconds, Lower Center Burner, Upper Lower Vents'/

&MESH IJK=60,60,30, XB=0.0, 1.50, 0.0, 1.50, 0.0, 0.75 /

&TIME T_BEGIN=-0.1,T_END=18000. / 4 h release and an equilibrium phase
&MISC TMPA=21. /
&SPEC ID='HELIUM', MASS_EXTINCTION_COEFFICIENT=500. /

&DUMP DT_DEVC=60., DT_HRR=60., SIG_FIGS=4, SIG_FIGS_EXP=2, SMOKE3D_QUANTITY='MASS FRACTION', SMOKE3D_SPEC_ID='HELIUM' /

&VENT XB = 0.0,0.0,0.725,0.775,0.700,0.725,SURF_ID='HVAC',ID='U TOP',COLOR='GREEN'/
&HVAC ID='U TOP',TYPE_ID='NODE',DUCT_ID='U TOPD',VENT_ID='U TOP'/
&HVAC ID='U TOPA',TYPE_ID='NODE',DUCT_ID='U TOPD',AMBIENT=.TRUE.,XYZ=0.0,0.75,0.7125/
&HVAC ID='U TOPD',TYPE_ID='DUCT',NODE_ID='U TOP','U TOPA',AREA=0.0002311, LENGTH=0.0127,LOSS=2.221,2.221/

&VENT XB = 0.0,0.0,0.725,0.775,0.675,0.700,SURF_ID='HVAC',ID='U BOTTOM',COLOR='RED'/
&HVAC ID='U BOTTOM',TYPE_ID='NODE',DUCT_ID='U BOTTOMD',VENT_ID='U BOTTOM'/
&HVAC ID='U BOTTOMA',TYPE_ID='NODE',DUCT_ID='U BOTTOMD',AMBIENT=.TRUE.,XYZ=0.0,0.75,0.6875/
&HVAC ID='U BOTTOMD',TYPE_ID='DUCT',NODE_ID='U BOTTOM','U BOTTOMA',AREA=0.0002311, LENGTH=0.0127,LOSS=2.221,2.221/

&VENT XB = 0.0,0.0,0.725,0.775,0.050,0.075,SURF_ID='HVAC',ID='B TOP',COLOR='GREEN'/
&HVAC ID='B TOP',TYPE_ID='NODE',DUCT_ID='B TOPD',VENT_ID='B TOP'/
&HVAC ID='B TOPA',TYPE_ID='NODE',DUCT_ID='B TOPD',AMBIENT=.TRUE.,XYZ=0.0,0.75,0.0625/
&HVAC ID='B TOPD',TYPE_ID='DUCT',NODE_ID='B TOP','B TOPA',AREA=0.0002311, LENGTH=0.0127,LOSS=2.221,2.221/

&VENT XB = 0.0,0.0,0.725,0.775,0.025,0.050,SURF_ID='HVAC',ID='B BOTTOM',COLOR='RED'/
&HVAC ID='B BOTTOM',TYPE_ID='NODE',DUCT_ID='B BOTTOMD',VENT_ID='B BOTTOM'/
&HVAC ID='B BOTTOMA',TYPE_ID='NODE',DUCT_ID='B BOTTOMD',AMBIENT=.TRUE.,XYZ=0.0,0.75,0.0375/
&HVAC ID='B BOTTOMD',TYPE_ID='DUCT',NODE_ID='B BOTTOM','B BOTTOMA',AREA=0.0002311, LENGTH=0.0127,LOSS=2.221,2.221/

&SURF ID='HELIUM', SPEC_ID(1)='HELIUM', MASS_FLUX(1)=0.0093, RAMP_MF(1)='LEAKRAMP'/

Vent Area =  .001024 sq m, Flow rate = 3.54 lpm, Density = 0.166 kg/m3 at 294 K
(3.54 l/min *1/60 min/sec *.001 m3/L *.166 kg/m3  * 1/.001024 m2= .0093 kg/m2/s)

&RAMP ID='LEAKRAMP',T=0.0,F=0.0 / 
&RAMP ID='LEAKRAMP',T=0.1,F=1.0 /
&RAMP ID='LEAKRAMP',T=14400.0,F=1.0 /
&RAMP ID='LEAKRAMP',T=14400.1,F=0.0 /

&OBST XB=0.734,0.766,0.734,0.766,0.0,0.21,COLOR='RED' / Fischer Burner, center lower location
&VENT XB=0.734,0.766,0.734,0.766,0.21,0.21, SURF_ID='HELIUM', COLOR='BLUE' / Helium Vent, center lower location

&SLCF PBY=0.75, QUANTITY='VELOCITY', VECTOR=.TRUE./
&SLCF PBY=0.75, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM', CELL_CENTERED=.TRUE./
&SLCF PBY=0.75, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE./

&DEVC XYZ=0.375,0.375,0.093, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #1' /
