&HEAD CHID='Sippola_Test_10', TITLE='Validation of aerosol deposition in a square duct, 15 cm by 15 cm'/

&MESH IJK=500,15,15, XB=-3.0,2.0,0.0,0.15,0.0,0.15 /

&TIME T_END=100.0 /

&DUMP DT_DEVC=1, SIG_FIGS=4, SIG_FIGS_EXP=2 /

&MISC GRAVITATIONAL_DEPOSITION  = .TRUE.
      THERMOPHORETIC_DEPOSITION = .TRUE.
      TURBULENT_DEPOSITION      = .TRUE.
      TMPA = 23.0 /

&SPEC ID            = 'AEROSOL'
      AEROSOL       = .TRUE.
      MEAN_DIAMETER = 9.8E-6
      DENSITY_SOLID = 1030 /

&SURF ID               = 'INLET'
      VEL              = -5.3
      COLOR            = 'BLUE'
      SPEC_ID(1)       = 'AEROSOL'
      MASS_FRACTION(1) = 8.3e-5 /

&MATL ID            = 'STEEL'
      FYI           = 'Drysdale, Intro to Fire Dynamics'
      EMISSIVITY    = 0.95
      SPECIFIC_HEAT = 0.46
      CONDUCTIVITY  = 45.8
      DENSITY       = 7850. /

&SURF ID         = 'STEEL WALL'
      MATL_ID    = 'STEEL'
      THICKNESS  = 0.001
      COLOR      = 'SNOW 3' /

&VENT MB='XMIN', SURF_ID='INLET' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='STEEL WALL' /
&VENT MB='YMAX', SURF_ID='STEEL WALL' /
&VENT MB='ZMIN', SURF_ID='STEEL WALL' /
&VENT MB='ZMAX', SURF_ID='STEEL WALL' /

&DEVC XB=0.0,1.5,0.075,0.075,0.075,0.075, QUANTITY='DENSITY', SPEC_ID='AEROSOL', ID='Soot Concentration', POINTS=50 /
&DEVC XYZ=0.00,0.075,0.075, QUANTITY='DENSITY', SPEC_ID='AEROSOL', ID='Soot Concentration Upstream' /
&DEVC XYZ=1.50,0.075,0.075, QUANTITY='DENSITY', SPEC_ID='AEROSOL', ID='Soot Concentration Downstream' /

&DEVC XB=0.20,0.40,0.03,0.13,0.15,0.15, QUANTITY='SURFACE DEPOSITION', ID='Ceiling Panel 1', STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /
&DEVC XB=0.50,0.70,0.03,0.13,0.15,0.15, QUANTITY='SURFACE DEPOSITION', ID='Ceiling Panel 2', STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /
&DEVC XB=0.80,1.00,0.03,0.13,0.15,0.15, QUANTITY='SURFACE DEPOSITION', ID='Ceiling Panel 3', STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /
&DEVC XB=1.10,1.30,0.03,0.13,0.15,0.15, QUANTITY='SURFACE DEPOSITION', ID='Ceiling Panel 4', STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /

&DEVC XB=0.20,0.40,0.00,0.00,0.03,0.13, QUANTITY='SURFACE DEPOSITION', ID='Wall Panel 1',    STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /
&DEVC XB=0.50,0.70,0.00,0.00,0.03,0.13, QUANTITY='SURFACE DEPOSITION', ID='Wall Panel 2',    STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /
&DEVC XB=0.80,1.00,0.00,0.00,0.03,0.13, QUANTITY='SURFACE DEPOSITION', ID='Wall Panel 3',    STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /
&DEVC XB=1.10,1.30,0.00,0.00,0.03,0.13, QUANTITY='SURFACE DEPOSITION', ID='Wall Panel 4',    STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /

&DEVC XB=0.20,0.40,0.03,0.13,0.00,0.00, QUANTITY='SURFACE DEPOSITION', ID='Floor Panel 1',   STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /
&DEVC XB=0.50,0.70,0.03,0.13,0.00,0.00, QUANTITY='SURFACE DEPOSITION', ID='Floor Panel 2',   STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /
&DEVC XB=0.80,1.00,0.03,0.13,0.00,0.00, QUANTITY='SURFACE DEPOSITION', ID='Floor Panel 3',   STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /
&DEVC XB=1.10,1.30,0.03,0.13,0.00,0.00, QUANTITY='SURFACE DEPOSITION', ID='Floor Panel 4',   STATISTICS='SURFACE INTEGRAL', SPEC_ID='AEROSOL' /

&DEVC XYZ=0.75,0.075,0.150, QUANTITY='FRICTION VELOCITY', IOR=-3, ID='Friction Velocity Ceiling' /
&DEVC XYZ=0.75,0.000,0.075, QUANTITY='FRICTION VELOCITY', IOR=2,  ID='Friction Velocity Wall' /
&DEVC XYZ=0.75,0.075,0.000, QUANTITY='FRICTION VELOCITY', IOR=3,  ID='Friction Velocity Floor' /

&TAIL /
