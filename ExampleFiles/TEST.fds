&HEAD CHID='tree_2_m_14_pc', TITLE='Test for burning vegetation mass conservation' /

&MESH IJK=40,40,80, XB=-1.0,1.0,-1.0,1.0,0.0,4.0 /

&TIME T_END=30. / 

&REAC FUEL='CELLULOSE', C=6, H=10, O=5, SOOT_YIELD=0.015 /

&SPEC ID='WATER VAPOR' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /
&VENT MB='ZMIN', SURF_ID='OPEN' /

&SURF ID                        = 'foliage'
      MATL_ID(1,1:2)            = 'vegetation','MOISTURE'
      MATL_MASS_FRACTION(1,1:2) = 1.0,0.
      THICKNESS                 = 0.0005
      LENGTH                    = 0.05
      GEOMETRY                  = 'CYLINDRICAL' /

&MATL ID = 'vegetation'
      DENSITY = 514.
      CONDUCTIVITY = 2.0
      SPECIFIC_HEAT= 1.2
      N_REACTIONS = 1
      REFERENCE_TEMPERATURE = 200.
      REFERENCE_RATE = 0.0005
      HEATING_RATE = 1.6
      NU_SPEC = 0.74
      SPEC_ID = 'CELLULOSE'
      NU_MATL = 0.26
      MATL_ID = 'CHAR'
      HEAT_OF_COMBUSTION = 17700.
      HEAT_OF_REACTION = 418. /

&MATL ID = 'CHAR'
      DENSITY = 300.
      CONDUCTIVITY = 2.0
      SPECIFIC_HEAT= 1.2
      N_REACTIONS = 1
      REFERENCE_TEMPERATURE = 350.
      REFERENCE_RATE = 0.0002
      HEATING_RATE = 1.6
      NU_SPEC = 1.00
      SPEC_ID = 'CELLULOSE'
      HEAT_OF_COMBUSTION = 17700.
      HEAT_OF_REACTION = 418. /

&MATL ID = 'MOISTURE'
      DENSITY = 1000.
      CONDUCTIVITY = 2.0
      SPECIFIC_HEAT= 4.184
      N_REACTIONS = 1
      REFERENCE_TEMPERATURE = 100.
      REFERENCE_RATE = 0.002
      HEATING_RATE = 1.6
      NU_SPEC = 1.0
      SPEC_ID = 'WATER VAPOR'
      HEAT_OF_REACTION= 2500. /

&PART ID='foliage', DRAG_LAW='CYLINDER', SAMPLING_FACTOR=10, SURF_ID='foliage', PROP_ID='needle image'
      QUANTITIES='PARTICLE TEMPERATURE','PARTICLE MASS','PARTICLE DIAMETER', STATIC=.TRUE., COLOR='FOREST GREEN' /
&INIT PART_ID='foliage', XYZ=0.0,0.0,0.15, RADIUS=0.85, HEIGHT=1.9, SHAPE='CONE', N_PARTICLES=100000, MASS_PER_VOLUME=2.0  /
&PROP ID='needle image', SMOKEVIEW_ID='TUBE', SMOKEVIEW_PARAMETERS='L=0.05','D=0.001','RANDXYZ=1' /

&DEVC XB=-1.,1.,-1.,1.,0.,2.1, QUANTITY='MPUV', PART_ID='foliage', ID='solid mass', STATISTICS='VOLUME INTEGRAL'  /

&TAIL /
