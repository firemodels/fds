&HEAD CHID='fed_test_area', TITLE='Check FED computation in smokeview, SVN $Revision: 12143 $'/

&MESH XB=0.0,2.0,0.0,0.2,0.0,2.0, IJK=10,1,10 /

&TIME T_END=180.0, DT=0.1 /
&DUMP NFRAMES=2880 /

&SPEC ID='NITROGEN', BACKGROUND=.TRUE. /
&SPEC ID='OXYGEN' /
&SPEC ID='CARBON DIOXIDE' /
&SPEC ID='CARBON MONOXIDE'/

&OBST XB = 1.0,1.0,0.0,0.2,0.0,2.0  /
&OBST XB = 0.0,2.0,0.0,0.2,1.0,1.0  /

&INIT XB=0.0,1.0,0.0,0.2,0.0,1.0 
      MASS_FRACTION(1)=0.07,    SPEC_ID(1)='OXYGEN',
      MASS_FRACTION(2)=0.03,    SPEC_ID(2)='CARBON DIOXIDE' 
      MASS_FRACTION(3)=0.00098, SPEC_ID(3)='CARBON MONOXIDE' /

&INIT XB=1.0,2.0,0.0,0.2,0.0,1.0 
      MASS_FRACTION(1)=0.08,    SPEC_ID(1)='OXYGEN',
      MASS_FRACTION(2)=0.035,   SPEC_ID(2)='CARBON DIOXIDE' 
      MASS_FRACTION(3)=0.00008, SPEC_ID(3)='CARBON MONOXIDE' /

&INIT XB=0.0,1.0,0.0,0.2,1.0,2.0 
      MASS_FRACTION(1)=0.1,     SPEC_ID(1)='OXYGEN',
      MASS_FRACTION(2)=0.04,    SPEC_ID(2)='CARBON DIOXIDE' 
      MASS_FRACTION(3)=0.001,   SPEC_ID(3)='CARBON MONOXIDE' /

&INIT XB=1.0,2.0,0.0,0.2,1.0,2.0 
      MASS_FRACTION(1)=0.3,     SPEC_ID(1)='OXYGEN',
      MASS_FRACTION(2)=0.09,    SPEC_ID(2)='CARBON DIOXIDE' 
      MASS_FRACTION(3)=0.0009,  SPEC_ID(3)='CARBON MONOXIDE' /


DEVC ID='DEV1A', XYZ=0.5,0.5,0.6, QUANTITY='FED' /

&SLCF PBY=0.1,QUANTITY='VOLUME FRACTION' SPEC_ID='CARBON DIOXIDE' /
&SLCF PBY=0.1,QUANTITY='VOLUME FRACTION' SPEC_ID='CARBON MONOXIDE' /
&SLCF PBY=0.1,QUANTITY='VOLUME FRACTION' SPEC_ID='OXYGEN' /
&SLCF PBY=0.1,QUANTITY='VOLUME FRACTION' SPEC_ID='CARBON DIOXIDE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.1,QUANTITY='VOLUME FRACTION' SPEC_ID='CARBON MONOXIDE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.1,QUANTITY='VOLUME FRACTION' SPEC_ID='OXYGEN', CELL_CENTERED=.TRUE. /

&TAIL /
