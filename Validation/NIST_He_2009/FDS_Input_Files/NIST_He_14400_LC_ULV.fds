&DEVC XYZ=0.375,0.375,0.466, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #5' /
&DEVC XYZ=0.375,0.375,0.559, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #6' /
&DEVC XYZ=0.375,0.375,0.650, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #7' /

&DEVC QUANTITY='DUCT VELOCITY',DUCT_ID='U TOPD',ID='U TOPD_V'/
