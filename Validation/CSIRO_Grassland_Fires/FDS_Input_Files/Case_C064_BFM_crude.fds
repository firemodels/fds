&HEAD CHID='Case_C064_BFM_crude', TITLE='Australian Grassland Fire Experiment, Case C064, Boundary Fuel Model' /

&MESH IJK=20,20,20, XB=-10.0,10.0,0.0,20.0,0.0,20.0, MULT_ID='mesh' /
&MULT ID='mesh', DX=20., DY=20., DZ=20., I_LOWER=0, I_UPPER=5, J_LOWER=-3, J_UPPER=2, K_UPPER=0 /

&TIME T_END=120. /

&MISC SIMULATION_MODE='SVLES', TMPA=32. /

&WIND SPEED=4.6, Z_REF=2., DIRECTION=270., L=-500., Z_0=0.03 /

&SURF ID='IGNITION', HRRPUA=1500., COLOR='RED', TMP_FRONT=100., RAMP_Q='ignitor' /
&RAMP ID='ignitor', T= 0., F=0. /
&RAMP ID='ignitor', T= 1., F=1. /
&RAMP ID='ignitor', T= 5., F=1. /
&RAMP ID='ignitor', T= 6., F=0. /

&VENT XB=-1.6,  0.0,-25.,25.,0.,0., SURF_ID='IGNITION', XYZ=0,0,0, SPREAD_RATE=1. /
&VENT XB= 0.0,100.0,-50.,50.,0.,0., SURF_ID='wet vegetation' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&SURF ID='ROAD'
      COLOR='BEIGE'
      MATL_ID='DIRT'
      THICKNESS=0.2 /

&VENT XB=-10,110,-53,-50,0,0, SURF_ID='ROAD' /
&VENT XB=-10,110, 50, 53,0,0, SURF_ID='ROAD' /
&VENT XB=-10,110, 54, 57,0,0, SURF_ID='ROAD' /
&VENT XB= -3,  0,-60, 60,0,0, SURF_ID='ROAD' /
&VENT XB=100,103,-60, 60,0,0, SURF_ID='ROAD' /
&VENT XB=104,107,-60, 60,0,0, SURF_ID='ROAD' /

&MATL ID='DIRT'
      CONDUCTIVITY = 0.25
      SPECIFIC_HEAT = 2.
      DENSITY = 1300. /

&SURF ID                        = 'wet vegetation'
      COLOR                     = 'BROWN'
      MATL_ID(1,1:1)            = 'GENERIC VEGETATION'
      MATL_ID(2,1)              = 'DIRT'
      THICKNESS(1:2)            = 0.21,0.10
      MOISTURE_CONTENT(1)       = 0.063
      MASS_PER_VOLUME(1)        = 1.33
      DRAG_COEFFICIENT          = 2.8
      SURFACE_VOLUME_RATIO(1)   = 9770. 
      TIME_STEP_FACTOR          = 15/

&CATF OTHER_FILES='vegetation_model.txt' /

&DUMP DT_HRR=1., DT_DEVC=1., DT_SLCF=1., SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SLCF PBY=1., QUANTITY='TEMPERATURE', VECTOR=T /
&SLCF PBZ=2., QUANTITY='VELOCITY', VECTOR=T /
&SLCF PBY=1., QUANTITY='INTEGRATED INTENSITY', CELL_CENTERED=T /
&SLCF PBZ=0.1, QUANTITY='HRRPUV', CELL_CENTERED=T /

&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='GAUGE HEAT FLUX' /

&DEVC ID='x', QUANTITY='TEMPERATURE', SPATIAL_STATISTIC='MAXLOC X', XB=-10,110,-1,0,0,1 /
&DEVC ID='D', QUANTITY='FIRE DEPTH', XB=-10,110,0,0,0.1,0.1 /

&TAIL /
