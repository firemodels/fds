! Test of tunnel friction factor
!
! Case matrix
! -----------
!
! Case A: Vel = 2 m/s, Roughness = 1.E-4 m
! Case B: Vel = 2 m/s, Roughness = 1.E-1 m
! Case C: Vel = 10 m/s, Roughness = 1.E-4 m
! Case D: Vel = 10 m/s, Roughness = 1.E-1 m
! Case E: Vel = 4 m/s, Roughness = 1.E-2 m

&HEAD CHID='tunnel_pressure_drop_a_10', TITLE='LES of flow through a rough tunnel'/

&MULT ID='m1', DX=20, I_UPPER=79/ 80 mesh case
&MESH IJK=10,10,10, XB=0,20,-5,5,0,10, MULT_ID='m1'/

&TIME T_END=1600.0/
&DUMP DT_DEVC=800, DT_BNDF=800 /

&PRES TUNNEL_PRECONDITIONER=T /

&SURF ID='inflow', VEL=-2, COLOR='BLUE'/ ! Re=1.33e6
&SURF ID='rough wall',
      COLOR='GRAY',
      DEFAULT=T,
      ROUGHNESS=1.0E-4/

&VENT PBX=0, SURF_ID='inflow'/
&VENT PBX=1600, SURF_ID='OPEN'/

&SLCF PBY=0.0, QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0.0, QUANTITY='PRESSURE', CELL_CENTERED=T/

&BNDF QUANTITY='VISCOUS WALL UNITS', CELL_CENTERED=T/

&DEVC XB=800,800,-5,5,0,10, QUANTITY='U-VELOCITY', SPATIAL_STATISTIC='MEAN', ID='UBAR'/
&DEVC XB=800,800,-5,5,0,10, QUANTITY='DENSITY', SPATIAL_STATISTIC='MEAN', ID='RHO'/
&DEVC XB=800,800,-5,5,0,10, QUANTITY='MOLECULAR VISCOSITY', SPATIAL_STATISTIC='MEAN', ID='MU'/

&DEVC XB=10,10,-5,5,0,10, QUANTITY='PRESSURE', SPATIAL_STATISTIC='MEAN', ID='P10'/
