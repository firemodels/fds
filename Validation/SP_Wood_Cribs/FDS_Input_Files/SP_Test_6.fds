&VENT PBX=10.0, SURF_ID='OPEN'  /
&VENT PBX=0.0, SURF_ID='FAN' /
&SURF ID='FAN', VEL=-0.6, COLOR='INVISIBLE' /

&SURF ID='Promatect H Board', 
      DEFAULT = .TRUE.
      THICKNESS=0.015,
      COLOR='GRAY'
      MATL_ID='Promatect H' /
 
&MATL ID='Promatect H',
      DENSITY=870,
      SPECIFIC_HEAT=1.13,
      CONDUCTIVITY=0.175 /
  
&SURF ID='PINE', 
      MATL_ID = 'PINE'
      COLOR = 'BROWN'
      BACKING = 'INSULATED'
      MOISTURE_FRACTION = 0.02
      SURFACE_VOLUME_RATIO = 460.
      GEOMETRY = 'CARTESIAN'
      LENGTH = 0.01
      WIDTH = 0.01 /

&MATL ID                 = 'PINE'
      EMISSIVITY         = 0.95
      DENSITY            = 370.
      CONDUCTIVITY_RAMP  = 'wood_k'
      SPECIFIC_HEAT_RAMP = 'wood_cp'
      A                  = 1.413E5
      E                  = 90.73E3
      N_S                = 1.69
      MATL_ID            = 'char'
      NU_MATL            = 0.20
      SPEC_ID            = 'CELLULOSE'
      NU_SPEC            = 0.80
      HEAT_OF_REACTION   = 250. /

&MATL ID                 = 'char'
      EMISSIVITY         = 1.
      DENSITY            = 135.
      CONDUCTIVITY_RAMP  = 'char_k'
      SPECIFIC_HEAT_RAMP = 'char_cp'
      N_S                = 0.
      NU_O2_CHAR         = 1.65
      BETA_CHAR          = 0.2
      A                  = 430.
      E                  = 74800.
      MATL_ID            = 'ash'
      NU_MATL            = 0.02
      SPEC_ID            = 'PRODUCTS','AIR'
      NU_SPEC            = 8.18,-7.2
      HEAT_OF_REACTION   = -32000. /

&MATL ID            = 'ash'
      DENSITY       = 67.
      CONDUCTIVITY  = 0.1
      SPECIFIC_HEAT = 2.0 /

Wood k from Koufpanos 
Self-measured wood Cp
Char k correlation from Hankalin et al (1988) = wood-char and char
Char Cp Gupta et al.
&RAMP ID = 'wood_k', T = 20,  F = 0.096 /
&RAMP ID = 'wood_k', T = 800, F = 0.370 /

&RAMP ID = 'wood_cp', T = 20.,  F = 0.90 /
