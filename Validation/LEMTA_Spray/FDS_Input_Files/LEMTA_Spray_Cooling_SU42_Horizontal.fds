&HEAD CHID='LEMTA_Spray_Cooling_SU42_Horizontal', TITLE='Simulation LEMTA SU42 horizontal plate cooling experiment' / 

&TIME T_END=500 /

&MESH IJK=24,24,20, XB=-0.6,0.6,-0.6,0.6,-0.30,0.70 / 

&MISC TMPA=12.7 /

&PRES MAX_PRESSURE_ITERATIONS=30 /

&OBST XB=-0.50,0.50,-0.50,0.50,-0.002,0.00, SURF_ID='Hot metallic plate', MATL_ID='Steel', CELL_SIZE=0.05 /

&OBST XB=-0.25,0.25,-0.25,0.25,-0.25,-0.20, SURF_IDS='Radiative panel','INERT','INERT', DEVC_ID='timer' /

&DEVC ID='timer', QUANTITY='TIME', XYZ=0,0,0, SETPOINT=385., INITIAL_STATE=T /

&MATL ID='Steel'
      SPECIFIC_HEAT_RAMP = 'c_ramp'
      CONDUCTIVITY_RAMP = 'k_ramp'
      DENSITY = 7850 / 

&RAMP ID='k_ramp', T=   0., F=55 /
&RAMP ID='k_ramp', T= 100., F=52 /
&RAMP ID='k_ramp', T= 200., F=49 /
&RAMP ID='k_ramp', T= 300., F=45 /
&RAMP ID='k_ramp', T= 400., F=42 /
&RAMP ID='k_ramp', T= 600., F=34 /
&RAMP ID='k_ramp', T= 800., F=30 /
&RAMP ID='k_ramp', T=1000., F=29 / 

&RAMP ID='c_ramp', T=  50., F=0.45 /
