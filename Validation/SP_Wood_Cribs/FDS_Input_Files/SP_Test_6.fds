&HEAD CHID='SP_Test_6', TITLE='SP small-scale pallets in a tunnel' /

&MISC TMPA=22. /

&MESH XB=0.0,1.0,0.0,0.6,0.0,0.4, IJK=25,15,10, MULT_ID='mesh' /
&MULT ID='mesh', DX=1.0, I_UPPER=9 /

&TIME T_END=720. /    

&CLIP MINIMUM_DENSITY=0.05 /  Account for high temps due to charring

&DUMP DT_DEVC=5, DT_HRR=5 /

&REAC FUEL='CELLULOSE',
      SOOT_YIELD=0.01, 
      C=3.4,
      H=6.2,
      O=2.5,
      HEAT_OF_COMBUSTION=18100 /
   
&SPEC ID='WATER VAPOR' /

&SURF ID='IGNITER', HRRPUA=1000, RAMP_Q='IG', TMP_FRONT=98, COLOR='RED'  /
&RAMP ID='IG', T=0,    F=0 /
&RAMP ID='IG', T=10,   F=1 /
&RAMP ID='IG', T=75,   F=1 /
&RAMP ID='IG', T=90,   F=0 /
&OBST XB=2.84,2.88,0.28,0.32,0.040,0.080, SURF_IDS='IGNITER','IGNITER','Promatect H Board' /

&OBST XB=2.8,3.3,0.1,0.5,0.040,0.040 /
&OBST XB=3.6,4.1,0.1,0.5,0.040,0.040 /
&OBST XB=4.7,5.2,0.1,0.5,0.040,0.040 /
