&HEAD CHID='methane_dx_1p25cm', TITLE='UMD Line Burner, dx = 1.25 cm, D/dx = 4, with REIGNITION_MODEL' /

&MULT ID='m1', DX=0.425,DY=0.60,DZ=0.30, I_UPPER=1,J_UPPER=0,K_UPPER=3 / 8 meshes
&MESH IJK=34,48,24, XB=-0.425,0.0,-0.30,0.30,-0.0125,0.2875, MULT_ID='m1' /

&TIME T_BEGIN=20., T_END=40. /

&MISC FLUX_LIMITER=4, RESEARCH_MODE=.TRUE. /

&VENT MB='ZMAX',SURF_ID='OPEN'/
&VENT MB='XMAX',SURF_ID='OPEN'/
&VENT MB='XMIN',SURF_ID='OPEN'/
&VENT MB='YMAX',SURF_ID='OPEN'/
&VENT MB='YMIN',SURF_ID='OPEN'/

&SPEC ID='NITROGEN'/

&REAC FUEL='METHANE', SOOT_YIELD=0.01, RADIATIVE_FRACTION=0.18, AUTO_IGNITION_TEMPERATURE = 626.9 /

&SURF ID='BURNER', SPEC_ID(1)='METHANE', HRRPUA=2000, COLOR='RED' /

&SURF ID='COFLOW', COLOR='BLUE',
      SPEC_ID(1)='AIR', MASS_FLUX(1)=0.244614,
      SPEC_ID(2)='NITROGEN', MASS_FLUX(2)=0.089602, RAMP_MF(2)='N2 RAMP' / time varied N2 dilution

&RAMP ID='N2 RAMP', T= 0.0, F=0.0 /0.21 vol frac O2
&RAMP ID='N2 RAMP', T= 20.0, F=0.0 / 
&RAMP ID='N2 RAMP', T= 21.0, F=0.393605 / 0.181
&RAMP ID='N2 RAMP', T= 40.0, F=0.393605 / 
&RAMP ID='N2 RAMP', T= 41.0, F=0.629786 / 0.168
&RAMP ID='N2 RAMP', T= 60.0, F=0.629786 /
&RAMP ID='N2 RAMP', T= 61.0, F=0.837911 / 0.158
&RAMP ID='N2 RAMP', T= 80.0, F=0.837911 /
&RAMP ID='N2 RAMP', T= 81.0, F=1.000000 / 0.151
&RAMP ID='N2 RAMP', T= 100.0, F=1.000000 /

&OBST XB=-0.25,0.25,-0.025,0.025,-0.0125,0.0, SURF_IDS='BURNER','INERT','INERT'/

&INIT XB=-0.255,0.255,-0.030,0.030,0.00,0.050, AUTO_IGNITION_TEMPERATURE = -273.1 /

&OBST XB=-0.425,0.425,-0.30,-0.25,-0.0125,0.0 /
&OBST XB=-0.425,0.425, 0.25, 0.30,-0.0125,0.0 /

&OBST XB=-0.425,-0.375,-0.25,0.25,-0.0125,0.0 /
&OBST XB= 0.375, 0.425,-0.25,0.25,-0.0125,0.0 /

&SURF ID='CERAMIC', COLOR='IVORY', ADIABATIC=.TRUE. /
&VENT SURF_ID='CERAMIC' XB=-0.30,0.30,-0.075,0.075,-0.009,-0.009/
&VENT SURF_ID='COFLOW', XB=-0.375,0.375,-0.25,0.25,-0.010,-0.010/

&SLCF PBY=0.0, QUANTITY = 'VELOCITY', VECTOR=.TRUE./
&SLCF PBY=0.0, QUANTITY = 'TEMPERATURE', CELL_CENTERED=.TRUE./

&SLCF PBY=0.0, QUANTITY = 'AUTO IGNITION TEMPERATURE', CELL_CENTERED=.TRUE./
&SLCF PBX=0.0, QUANTITY = 'AUTO IGNITION TEMPERATURE', CELL_CENTERED=.TRUE./

&PROP ID='TC', BEAD_EMISSIVITY=0.9, BEAD_DENSITY=8665, BEAD_DIAMETER=0.0005, BEAD_SPECIFIC_HEAT=0.4855 /

&DEVC XYZ=0,0.2,0, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN' /

! centerline

&DEVC XB=0,0,0,0,0.00625,0.99375, QUANTITY='HRRPUV', POINTS=80, ID='hrrpuv' /
&DEVC XB=0,0,0,0,0.00625,0.99375, QUANTITY='HRRPUV', POINTS=80, STATISTICS='RMS', ID='hrrpuv_rms' /
&DEVC XB=0,0,0,0,0.00625,0.99375, QUANTITY='TEMPERATURE', POINTS=80, ID='tmp' /
&DEVC XB=0,0,0,0,0.00625,0.99375, QUANTITY='TEMPERATURE', POINTS=80, STATISTICS='RMS', ID='tmp_rms' /
&DEVC XB=0,0,0,0,0.00625,0.99375, QUANTITY='W-VELOCITY', POINTS=80, ID='wvel' /
&DEVC XB=0,0,0,0,0.00625,0.99375, QUANTITY='W-VELOCITY', POINTS=80, STATISTICS='RMS', ID='wvel_rms' /
&DEVC XB=0,0,0,0,0.00625,0.99375, QUANTITY='SUBGRID KINETIC ENERGY', POINTS=80, ID='ksgs' /

! profiles at z = 0.125 m

&DEVC XB=0,0,-0.24375,0.24375,0.125,0.125, POINTS=40, QUANTITY='TEMPERATURE', ID='T_125' /
&DEVC XB=0,0,-0.24375,0.24375,0.125,0.125, POINTS=40, QUANTITY='THERMOCOUPLE', PROP_ID='TC', ID='TC_125' /
&DEVC XB=0,0,-0.24375,0.24375,0.125,0.125, POINTS=40, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN', ID='XO2_125' /

! profiles at z = 0.250 m

&DEVC XB=0,0,-0.24375,0.24375,0.250,0.250, POINTS=40, QUANTITY='TEMPERATURE', ID='T_250' /
&DEVC XB=0,0,-0.24375,0.24375,0.250,0.250, POINTS=40, QUANTITY='THERMOCOUPLE', PROP_ID='TC', ID='TC_250' /
&DEVC XB=0,0,-0.24375,0.24375,0.250,0.250, POINTS=40, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN', ID='XO2_250' /

&TAIL /



























