&HVAC TYPE_ID='NODE',VENT_ID='INLET1',DUCT_ID='DUCT1',ID='INLET1'/
&HVAC TYPE_ID='NODE',VENT_ID='OUTLET1',DUCT_ID='DUCT1',ID='OUTLET1'/

&HVAC TYPE_ID='DUCT',NODE_ID='INLET1','OUTLET1',ID='DUCT1',AREA=0.01,VOLUME_FLOW=0.01,LOSS=0,0,LENGTH=10,DUCT_INTERP_TYPE='NODE2',N_CELLS=1000/

&DEVC ID='FDS Species 1',QUANTITY='NODE MASS FRACTION',SPEC_ID='SPEC1',NODE_ID='OUTLET1'/
