&HEAD CHID='Bova_4a', TITLE='Bova et al., Int. J. Wildland Fire, 25(2), 2015, Fig. 4a' /

&MESH IJK=100,100,4, XB=0,1000,0,1000,0,40 /

&TIME T_END=2160 /
&MISC LEVEL_SET_MODE=1 /
&WIND U0=4.3 /

&SLCF AGL_SLICE=1., QUANTITY='LEVEL SET VALUE' /

&SURF ID = 'SPOT IGN',VEG_LSET_IGNITE_TIME = 1,COLOR = 'RED' /
&VENT XB=240,250,490,510,0,0, SURF_ID='SPOT IGN' /

&VENT XB=300,700,300,700,0,0, SURF_ID='INERT' /

&SURF ID = 'Custom grass'
            DEFAULT = T
            VEG_LSET_ROS_00 = 0.04
            VEG_LSET_SIGMA = 11400.
            VEG_LSET_BETA = 0.0012
            VEG_LSET_HT = 0.51
            RGB=122,117,48 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&TAIL /
