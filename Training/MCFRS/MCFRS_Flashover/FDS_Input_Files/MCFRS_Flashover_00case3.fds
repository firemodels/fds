&DEVC ID='TIMER2', XYZ=0.0,0.0,0.0, QUANTITY='TIME' /
&CTRL ID='door timer2', FUNCTION_TYPE='CUSTOM', INPUT_ID='TIMER1', RAMP_ID='door times2' /
&RAMP ID='door times2', T=     0, F= 1 /
&RAMP ID='door times2', T=    59, F= 1 /
&RAMP ID='door times2', T=    61, F=-1 /
&RAMP ID='door times2', T=   404, F=-1 /
&RAMP ID='door times2', T=   406, F= 1 /
&RAMP ID='door times2', T=   599, F= 1 /
&RAMP ID='door times2', T=   601, F=-1 /
