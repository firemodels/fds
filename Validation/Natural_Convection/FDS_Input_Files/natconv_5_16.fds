&TIME T_END=9.0/

&DUMP DT_DEVC=.1/

&MISC STRATIFICATION=F, SIMULATION_MODE='LES'/

&RADI RADIATION=.FALSE./

&SPEC ID='AIR', SPECIFIC_HEAT=1., CONDUCTIVITY=0.018216, VISCOSITY=1.8216E-5, BACKGROUND=.TRUE./ ! Pr=1.00

&VENT PBX=0, SURF_ID='T1' /
&VENT PBX=.02, SURF_ID='T2' /
&VENT PBY=-.04, SURF_ID='MIRROR' /
&VENT PBY= .04, SURF_ID='MIRROR' /

&VENT PBZ=0, SURF_ID='insulated' /
&VENT PBZ=.32, SURF_ID='insulated' /

&SURF ID = 'insulated', COLOR = 'GRAY', HEAT_TRANSFER_COEFFICIENT=0./
&SURF ID = 'T1', COLOR = 'RED',  TMP_FRONT = 230., TAU_T=0.,HEAT_TRANSFER_MODEL='RAYLEIGH' /
&SURF ID = 'T2', COLOR = 'BLUE', TMP_FRONT = 20., TAU_T=0.,HEAT_TRANSFER_MODEL='RAYLEIGH' /

&INIT XB=0,.02,-.04,.04,0,.32, TEMPERATURE=125./

&BNDF QUANTITY='HEAT TRANSFER COEFFICIENT', CELL_CENTERED=T/
