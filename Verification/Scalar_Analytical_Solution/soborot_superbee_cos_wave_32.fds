&HEAD CHID='soborot_superbee_cos_wave_32', TITLE='Solid body rotation flow field, cosine wave' /

&MESH IJK=32,1,32, XB=-1,0,-0.5,0.5,0,1/

&TIME T_END=1.575745, DT=10/

&DUMP NFRAMES=10/

&MISC STRATIFICATION=F
      NOISE=F
      PERIODIC_TEST=13
      FREEZE_VELOCITY=T
      FLUX_LIMITER='SUPERBEE'
      CFL_MAX=1.0
      SIMULATION_MODE='DNS'
      /

&SPEC ID='AIR', DIFFUSIVITY=0, BACKGROUND=T /
&SPEC ID='TRACER', DIFFUSIVITY=0/

&SURF ID='inlet', SPEC_ID(1)='TRACER', MASS_FRACTION(1)=1, TAU_MF(1)=0/

&VENT XB=-0.75,-0.25,-0.5,0.5,0,0, SURF_ID='inlet', COLOR='INVISIBLE'/
&VENT XB= 1.0, 1.0,-0.5,0.5,0.0,1.0, SURF_ID='OPEN'/

&SLCF PBY=0.0, QUANTITY='VELOCITY', VECTOR=.TRUE./
&SLCF PBY=0.0, QUANTITY='MASS FRACTION', SPEC_ID='TRACER', CELL_CENTERED=T/

&DEVC XB=-0.015625,-0.984375,0,0,0.015625,0.984375, POINTS=32, TIME_HISTORY=T, TIME_AVERAGED=F,
      QUANTITY='MASS FRACTION', SPEC_ID='TRACER', ID='Y_TRACER'/

&TAIL /
