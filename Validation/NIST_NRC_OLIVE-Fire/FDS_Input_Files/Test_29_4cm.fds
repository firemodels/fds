&DEVC ID='TC-9',     XYZ= 0.40,-0.80,0.80, QUANTITY='WALL TEMPERATURE', IOR=-2 /
&DEVC ID='CH4',      XYZ=-0.60,-0.50,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='METHANE' /
&DEVC ID='SOOT',     XYZ=-0.60,-0.50,2.00, QUANTITY='MASS FRACTION',   SPEC_ID='SOOT' /
