&HEAD CHID= 'LLNL_37', TITLE= '1986 LLNL Enclosure Fire Test 37' /
&MESH IJK=30,20,30, XB=0,6,0,4,0,4.5 /
&TIME T_BEGIN=-20., T_END=500. /
&MISC TMPA=20. SURF_DEFAULT='Wall' /Fire
&RADI RADIATIVE_FRACTION=0.20 /

&REAC ID= 'Methane'
      FYI= 'C_1 H_4'
      SOOT_YIELD=0
      C=1
      H=4
      HEAT_OF_COMBUSTION=50050. /

&SURF ID= 'BURNER'
      COLOR= 'RED'
      RAMP_Q='fire'
      HRRPUA=800. /

&RAMP ID='fire', T= -20., F=0. /
&RAMP ID='fire', T=   0., F=0. /
&RAMP ID='fire', T=  10., F=1. /

&OBST XB=2.7,3.2,1.7,2.2,0.0,0.2, 

SURF_IDS='BURNER','Duct','Duct' / Construction

&MATL ID= 'Wall Aluminum Silicate'
   	FYI= 'Al_2 O_3 Si O_2 Refractory'
   	CONDUCTIVITY=0.39
   	DENSITY=1440.
   	SPECIFIC_HEAT=1.0 /

&MATL ID= 'Ceiling Aluminum Silicate'
	FYI= 'Al_2 O_3 Si O_2 Refractory'
	CONDUCTIVITY=0.63
	DENSITY=1920.
	SPECIFIC_HEAT=1.0 /

&MATL ID= 'Marinite'
	CONDUCTIVITY=0.12
	DENSITY=700.
	SPECIFIC_HEAT=1.12 /

&SURF ID= 'Ceiling'
	COLOR='TAN'
     MATL_ID= 'Ceiling Aluminum Silicate'
     	THICKNESS=0.1 /

&SURF ID= 'Drop Ceiling'
	MATL_ID= 'Marinite'
	COLOR= 'GRAY'
	THICKNESS=0.019 /

&SURF ID= 'Wall',
     MATL_ID= 'Wall Aluminum Silicate'
     COLOR = 'BEIGE'
     THICKNESS=0.1 /


&OBST XB=0.0,6.0,0.0,4.0,3.0,3.0, SURF_ID='Drop Ceiling' /
&HOLE XB=5.2,5.6,1.8,2.2,2.9,3.1 /
&VENT XB=0.0,6.0,0.0,4.0,4.5,4.5, SURF_ID='Ceiling' / Ceiling liner
&VENT XB=0.0,6.0,0.0,4.0,0.0,0.0, SURF_ID='Ceiling' / Floor liner

Ventilation
&ZONE XB=0,6,0,4,0,4.5, LEAK_AREA(0)=0.018 /

&MATL ID= 'Steel'
      FYI='NUREG-1805'
      CONDUCTIVITY=54.
      DENSITY=7850.
      SPECIFIC_HEAT=0.465 /

&SURF ID='Duct'
      COLOR='BLACK'
      MATL_ID= 'Steel'
      THICKNESS=0.002 /

&SURF ID='Inlet'
      COLOR='GREEN'
      LEAK_PATH=1,0 /

&SURF ID='EXHAUST'
      COLOR='BLUE'
      VOLUME_FLUX=0.1 /

&OBST XB=0.6,1.0,0.0,0.5,0.6,0.6, SURF_ID='Duct' / Inlet duct work
