&HEAD CHID='FAA_Polymers_PEEK_90', TITLE='PEEK at 90 kW/m2, 5 mm thick' /

&MESH IJK=20,20,50, XB=-0.10,0.10,-0.10,0.10,-0.03,0.47 /  

&TIME T_END=800. / 

&REAC FUEL='PEEK GAS', C=6., H=6., O=1., SOOT_YIELD=0.02, HEAT_OF_COMBUSTION=17700. / 

&MATL ID='PEEK'
      ABSORPTION_COEFFICIENT=1690.
      N_REACTIONS=1
      A(1) = 1.0E32
      E(1) = 5.57E5
      EMISSIVITY=0.90
      DENSITY=1300.
      SPEC_ID='PEEK GAS'
      NU_SPEC=0.38
      MATL_ID='CHAR 1'
      NU_MATL=0.62
      HEAT_OF_REACTION=350.
      HEAT_OF_COMBUSTION=16000.
      CONDUCTIVITY = 0.28
      SPECIFIC_HEAT = 2.05 /

&MATL ID='CHAR 1'
      ABSORPTION_COEFFICIENT=81000.
      N_REACTIONS=1
      A(1) = 1.0E3
      E(1) = 8.9E4
      EMISSIVITY=1.0
      DENSITY=810.
      SPEC_ID='PEEK GAS'
      NU_SPEC=0.12
      MATL_ID='CHAR 2'
      NU_MATL=0.88
      HEAT_OF_REACTION=0.
      HEAT_OF_COMBUSTION=27000.
      CONDUCTIVITY = 0.36
      SPECIFIC_HEAT = 0.24 /

&MATL ID='CHAR 2'
      ABSORPTION_COEFFICIENT=71000.
      EMISSIVITY=1.0
      DENSITY=710.
      CONDUCTIVITY=0.36
      SPECIFIC_HEAT=0.27 /

&MATL ID='Kaowool'
      CONDUCTIVITY = 0.08
      SPECIFIC_HEAT = 0.80
      DENSITY = 48. /

&SURF ID='SAMPLE'
      COLOR='BLACK'
      MATL_ID='PEEK','Kaowool'
      THICKNESS=0.0039,0.015
      EXTERNAL_FLUX=90. / 

&OBST XB=-0.05,0.05,-0.05,0.05,-0.02,0.00, SURF_IDS='SAMPLE','INERT','INERT' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMIN', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DUMP DT_DEVC=2., SIG_FIGS=4, SIG_FIGS_EXP=2 /

&DEVC XB=-0.10,0.10,-0.10,0.10,-0.03,0.47, QUANTITY='HRRPUV', ID='HRR', STATISTICS='VOLUME INTEGRAL', CONVERSION_FACTOR=100. /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='WALL TEMPERATURE', ID='temp' /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='BURNING RATE',     ID='MLR' /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='WALL THICKNESS',   ID='thick' /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='GAUGE HEAT FLUX',  ID='flux' /

&SLCF PBY=0., QUANTITY='TEMPERATURE' /
&SLCF PBY=0., QUANTITY='HRRPUV' /

&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='GAUGE HEAT FLUX' /
&BNDF QUANTITY='BURNING RATE' /

&TAIL / 


