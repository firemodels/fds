&HEAD CHID='jet_vreman_dx10cm', TITLE='Centerline decay of a turbulent jet' /    
	
&MESH IJK=200,48,48,  XB= 0.0,20.0,-2.4,2.4,-2.4,2.4 /

&TIME T_END=100. /

&DUMP DT_DEVC=10. /

&MISC TURBULENCE_MODEL='VREMAN', STRATIFICATION=.FALSE. /

&SURF ID='jet', VEL=-2.0, COLOR='INVISIBLE' /

&VENT XB=0.0,0.0, -.4,.4, -.4,.4, SURF_ID='jet' /

&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /
&VENT MB='ZMIN', SURF_ID='OPEN' /

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=.TRUE. /

&DEVC XB= 0.4,20,0,0,0,0, QUANTITY='U-VELOCITY', ID='VEL1', POINTS=50 /

&TAIL /

