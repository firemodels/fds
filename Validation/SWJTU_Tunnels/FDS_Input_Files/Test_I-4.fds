&HEAD CHID='Test_I-4', TITLE='Southwest Jiaotong University scale tunnel fire tests' /

&MESH XB=0.0,0.325,0.0,0.45,0.0,0.23, IJK=43,60,30, MULT_ID='mesh1' /
&MULT ID='mesh1', DX=0.325, I_UPPER=63 /

&MISC TMPA=11.6 /

&TIME T_END=800. /

&DUMP NFRAMES=800, DT_HRR=5., DT_DEVC=5. /

&COMB RADIATIVE_FRACTION=0., N_SIMPLE_CHEMISTRY_REACTIONS=2 /
&REAC FUEL='PROPANE', AIT_EXCLUSION_ZONE=9.90,10.10,0.12,0.32,0.03,0.04, AUTO_IGNITION_TEMPERATURE=450. /

&VENT PBX= 0.00, SURF_ID='OPEN' /
&VENT PBX=20.80, SURF_ID='OPEN' /
&VENT PBY= 0.00, SURF_ID='GLASS PLATE' /
&VENT PBY= 0.45, SURF_ID='GLASS PLATE' /

&MATL ID            = 'STEEL'
      SPECIFIC_HEAT = 0.465
      CONDUCTIVITY  = 54.
      DENSITY       = 7833. /

&MATL ID            = 'GLASS'
      SPECIFIC_HEAT = 0.8
      CONDUCTIVITY  = 1. 
      DENSITY       = 2500. /

&MATL ID            = 'SAND'
      SPECIFIC_HEAT = 0.8
      CONDUCTIVITY  = 2. 
      DENSITY       = 1600. /

&SURF ID            = 'STEEL PLATE'
      MATL_ID       = 'STEEL'
      DEFAULT       = .TRUE.
      ROUGHNESS     = 0.000100
      THICKNESS     = 0.002
      COLOR         = 'GRAY 40' /

&SURF ID            = 'GLASS PLATE'
      MATL_ID       = 'GLASS'
      THICKNESS     = 0.006
      COLOR         = 'LIGHT BLUE' /


&SURF ID='FIRE', HRRPUA=133., COLOR='ORANGE', MATL_ID='SAND', THICKNESS=0.03 /
&OBST XB= 9.90,10.10,0.12,0.32,0.00,0.03, SURF_IDS='FIRE','STEEL PLATE','STEEL PLATE' /
&OBST XB= 9.45,10.55,0.00,0.45,0.18,0.19, SURF_ID='STEEL PLATE' /
&OBST XB= 1.00, 1.00,0.00,0.45,0.18,0.23, SURF_ID='STEEL PLATE' /
&OBST XB= 4.00, 4.00,0.00,0.45,0.18,0.23, SURF_ID='STEEL PLATE' /
&OBST XB= 6.00, 6.00,0.00,0.45,0.18,0.23, SURF_ID='STEEL PLATE' /
&OBST XB=14.00,14.00,0.00,0.45,0.18,0.23, SURF_ID='STEEL PLATE' /
&OBST XB=16.00,16.00,0.00,0.45,0.18,0.23, SURF_ID='STEEL PLATE' /
&OBST XB=19.00,19.00,0.00,0.45,0.18,0.23, SURF_ID='STEEL PLATE' /

&SLCF PBY=0.23, QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
&SLCF PBY=0.23, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.23, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.23, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.23, QUANTITY='MASS FRACTION', SPEC_ID='SOOT', CELL_CENTERED=.TRUE. /

&DEVC ID='X_O2-1',  XYZ=10.2,0.22,0.115, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN' /
&DEVC ID='X_CO2-1', XYZ=10.2,0.22,0.115, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE' /
&DEVC ID='X_CO-1',  XYZ=10.2,0.22,0.115, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE' /
&DEVC ID='X_O2-2',  XYZ=15.0,0.22,0.115, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN' /
&DEVC ID='X_CO2-2', XYZ=15.0,0.22,0.115, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE' /
&DEVC ID='X_CO-2',  XYZ=15.0,0.22,0.115, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE' /

&TAIL /
