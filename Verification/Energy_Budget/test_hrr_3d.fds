&HEAD CHID='test_hrr_3d', TITLE='Comparison of HRR column between 2D cylindrical and 3D'/

&MESH IJK=40,40,40, XB=-2,2,-2,2,0,4/

&TIME T_END=10/

&DUMP DT_HRR=0.1 /

&REAC FUEL='METHANE'/

&SURF ID='burner', HRRPUA=200, COLOR='RED'/

&MULT ID='cube array', DX=0.1,DY=0.1, I_UPPER=11,J_UPPER=11/

&OBST XB=-0.5,-0.4,-0.5,-0.4,0,0.2
      MULT_ID='cube array'
      SURF_IDS='burner','INERT','INERT'
      SHAPE='CYLINDER'
      RADIUS=0.5
      HEIGHT=0.2
      XYZ=0,0,0
      ORIENTATION=0,0,1/

&VENT MB='XMIN', SURF_ID='OPEN'/
&VENT MB='XMAX', SURF_ID='OPEN'/
&VENT MB='YMIN', SURF_ID='OPEN'/
&VENT MB='YMAX', SURF_ID='OPEN'/
&VENT MB='ZMIN', SURF_ID='OPEN'/
&VENT MB='ZMAX', SURF_ID='OPEN'/

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0, QUANTITY='TEMPERATURE', CELL_CENTERED=T/

&DEVC XB=-2,2,-2,2,0,4,
      QUANTITY='MASS FLUX', SPEC_ID='METHANE',
      SPATIAL_STATISTIC='SURFACE INTEGRAL', SURF_ID='burner', ID='MDOT METHANE'/

&TAIL /
