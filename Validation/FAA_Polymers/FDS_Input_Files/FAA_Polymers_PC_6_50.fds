&HEAD CHID='FAA_Polymers_PC_6_50', TITLE='Bisphenol A Polycarbonate at 50 kW/m2, 5.4 mm thick' /

&MESH IJK=20,20,50, XB=-0.10,0.10,-0.10,0.10,-0.03,0.47 /  

&TIME T_END=600. / 

&REAC FUEL='BISPHENOL A', C=3., H=8., O=2., SOOT_YIELD=0.02 / 

&MATL ID='PC'
      ABSORPTION_COEFFICIENT=1770.
      N_REACTIONS=1
      A(1) = 1.9E18
      E(1) = 2.95E5
      EMISSIVITY=0.90
      DENSITY=1180.
      SPEC_ID='BISPHENOL A'
      NU_SPEC=0.79
      MATL_ID='CHAR'
      NU_MATL=0.21
      HEAT_OF_REACTION=830.
      HEAT_OF_COMBUSTION=21504.
      CONDUCTIVITY = 0.22
      SPECIFIC_HEAT = 1.9 /

&MATL ID='CHAR'
      EMISSIVITY=0.85
      DENSITY=248.
      CONDUCTIVITY=0.37
      SPECIFIC_HEAT=1.72 /

&MATL ID='Kaowool'
      CONDUCTIVITY = 0.08
      SPECIFIC_HEAT = 0.80
      DENSITY = 48. /

&SURF ID='SAMPLE'
      COLOR='BLACK'
      MATL_ID='PC','Kaowool'
      THICKNESS=0.0054,0.015
      STRETCH_FACTOR=1.
      EXTERNAL_FLUX=50. / 

&OBST XB=-0.05,0.05,-0.05,0.05,-0.02,0.00, SURF_IDS='SAMPLE','INERT','INERT' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMIN', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DUMP DT_DEVC=2., SIG_FIGS=4 /

&DEVC XB=-0.10,0.10,-0.10,0.10,-0.03,0.47, QUANTITY='HRRPUV', ID='HRR', STATISTICS='VOLUME INTEGRAL', CONVERSION_FACTOR=100. /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='WALL TEMPERATURE', ID='temp' /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='BURNING RATE',     ID='MLR' /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='WALL THICKNESS',   ID='thick' /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='GAUGE HEAT FLUX',  ID='flux' /

&SLCF PBY=0., QUANTITY='TEMPERATURE' /
&SLCF PBY=0., QUANTITY='HRRPUV' /

&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='GAUGE HEAT FLUX' /
&BNDF QUANTITY='BURNING RATE' /

&TAIL / 


