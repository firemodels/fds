&DEVC XB=0.0,0.0,0.0,0.0,2.25,339.75, QUANTITY='HRRPUV', DX=72., DY=72., DZ=2.25, SPATIAL_STATISTIC='VOLUME INTEGRAL', POINTS=76, Z_ID='Height', ID='HRR' /
&CTRL ID='pct95', FUNCTION_TYPE='PERCENTILE', INPUT_ID='HRR', PERCENTILE=0.95 /
&CTRL ID='pct97', FUNCTION_TYPE='PERCENTILE', INPUT_ID='HRR', PERCENTILE=0.97 /
&CTRL ID='pct99', FUNCTION_TYPE='PERCENTILE', INPUT_ID='HRR', PERCENTILE=0.99 /
&DEVC ID='Lf_95', XYZ=0,0,0, QUANTITY='CONTROL VALUE', CTRL_ID='pct95', TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=168., UNITS='m' /
&DEVC ID='Lf_97', XYZ=0,0,0, QUANTITY='CONTROL VALUE', CTRL_ID='pct97', TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=168., UNITS='m' /
&DEVC ID='Lf_99', XYZ=0,0,0, QUANTITY='CONTROL VALUE', CTRL_ID='pct99', TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=168., UNITS='m' /

&TAIL /

