&HEAD CHID='Qs=10000_RI=10', TITLE='Flame Height Test, Q*=10000' /

&MESH IJK=33,33,80, XB=-72.0,72.0,-72.0,72.0,-18.0,342. /
&TIME T_END=336. /

&REAC FUEL='PROPANE', 
      SOOT_YIELD=0.015 /

&SURF ID='burner', HRRPUA=15127250., COLOR='RED' /  
&OBST XB=-0.5,0.5,-0.5,0.5,-5.0,0.0, SURF_IDS='burner','INERT','INERT', THICKEN=.TRUE. /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMIN', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /
