&HEAD CHID='SP_AST_Heptane_1p1', TITLE='SP Adiabatic Surface Temperature Validation, Column Test'/

&MESH IJK=20,20,80, XB=-1.0,1.0,-1.0,1.0,0.0,8.0 /
&TIME T_END=1200. /

&REAC FUEL='N-HEPTANE', SOOT_YIELD=0.037 /

&ZONE XB=-0.10, 0.10,-0.10, 0.10, 0.40, 6.40 /

&SURF ID='BURNER', HRRPUA=2275., RAMP_Q='fire', TMP_FRONT=100., COLOR='ORANGE' / 
&RAMP ID='fire', T=   0., F=0. /
&RAMP ID='fire', T= 120., F=1. /
&RAMP ID='fire', T= 780., F=1. /
&RAMP ID='fire', T= 900., F=0. /

&OBST XB=-0.50, 0.50,-0.50, 0.50, 0.00, 0.20, SURF_IDS='BURNER','STEEL PLATE','STEEL PLATE' /

&OBST XB=-0.10,-0.10,-0.10, 0.10, 0.40, 6.40, SURF_ID='STEEL PLATE' /
&OBST XB= 0.10, 0.10,-0.10, 0.10, 0.40, 6.40, SURF_ID='STEEL PLATE' /
&OBST XB=-0.10, 0.10,-0.10,-0.10, 0.40, 6.40, SURF_ID='STEEL PLATE' /
&OBST XB=-0.10, 0.10, 0.10, 0.10, 0.40, 6.40, SURF_ID='STEEL PLATE' /
&OBST XB=-0.10, 0.10,-0.10, 0.10, 6.40, 6.40, SURF_ID='STUFFING' /
&OBST XB=-0.10, 0.10,-0.10, 0.10, 0.40, 0.40, SURF_ID='STEEL PLATE' /

&OBST XB= 0.00, 0.10,-0.20,-0.20, 1.40, 1.50, SURF_ID6='INERT','INERT','PLATE FRONT','PLATE BACK','INERT','INERT' /
&OBST XB= 0.00, 0.10,-0.20,-0.20, 2.40, 2.50, SURF_ID6='INERT','INERT','PLATE FRONT','PLATE BACK','INERT','INERT' /
&OBST XB= 0.00, 0.10,-0.20,-0.20, 3.40, 3.50, SURF_ID6='INERT','INERT','PLATE FRONT','PLATE BACK','INERT','INERT' /
&OBST XB= 0.00, 0.10,-0.20,-0.20, 4.40, 4.50, SURF_ID6='INERT','INERT','PLATE FRONT','PLATE BACK','INERT','INERT' /
&OBST XB= 0.00, 0.10,-0.20,-0.20, 5.40, 5.50, SURF_ID6='INERT','INERT','PLATE FRONT','PLATE BACK','INERT','INERT' /

&SURF ID='STEEL PLATE' 
      MATL_ID='STEEL'
      THICKNESS=0.010
      BACKING='EXPOSED'
      COLOR='BLACK' /

&SURF ID='PLATE FRONT' 
      MATL_ID='STEEL','INSULATION'
      BACKING='EXPOSED'
      THICKNESS=0.0007,0.01
      COLOR='BLACK' /

&SURF ID='PLATE BACK' 
      MATL_ID='INSULATION','STEEL'
      BACKING='EXPOSED'
      THICKNESS=0.01,0.0007
      COLOR='PINK' /

&SURF ID='STUFFING'
      MATL_ID='INSULATION'
      BACKING='EXPOSED'
      THICKNESS=0.05
      COLOR='PINK' /

&VENT MB='XMIN', SURF_ID='OPEN'/
&VENT MB='XMAX', SURF_ID='OPEN'/
&VENT MB='YMIN', SURF_ID='OPEN'/
&VENT MB='YMAX', SURF_ID='OPEN'/
&VENT MB='ZMAX', SURF_ID='OPEN'/

&MATL ID            = 'STEEL'
      SPECIFIC_HEAT = 0.5      
      CONDUCTIVITY  = 48.      
      DENSITY       = 7800. /

&MATL ID            = 'INSULATION' 
      DENSITY       = 200.
      CONDUCTIVITY  = 0.1
      SPECIFIC_HEAT = 1. /

&DUMP DT_DEVC=15., DT_HRR=15. /

&DEVC ID='PT-1', XYZ= 0.01,-0.21, 1.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='PT-2', XYZ= 0.01,-0.21, 2.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='PT-3', XYZ= 0.01,-0.21, 3.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='PT-4', XYZ= 0.01,-0.21, 4.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='PT-5', XYZ= 0.01,-0.21, 5.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /

&DEVC ID='AST-1', XYZ= 0.01,-0.21, 1.45, IOR=-2, QUANTITY='ADIABATIC SURFACE TEMPERATURE'  /
&DEVC ID='AST-2', XYZ= 0.01,-0.21, 2.45, IOR=-2, QUANTITY='ADIABATIC SURFACE TEMPERATURE'  /
&DEVC ID='AST-3', XYZ= 0.01,-0.21, 3.45, IOR=-2, QUANTITY='ADIABATIC SURFACE TEMPERATURE'  /
&DEVC ID='AST-4', XYZ= 0.01,-0.21, 4.45, IOR=-2, QUANTITY='ADIABATIC SURFACE TEMPERATURE'  /
&DEVC ID='AST-5', XYZ= 0.01,-0.21, 5.45, IOR=-2, QUANTITY='ADIABATIC SURFACE TEMPERATURE'  /

&DEVC ID='Gas-1', XYZ=-0.05,-0.20, 1.45, QUANTITY='THERMOCOUPLE'  /
&DEVC ID='Gas-2', XYZ=-0.05,-0.20, 2.45, QUANTITY='THERMOCOUPLE'  /
&DEVC ID='Gas-3', XYZ=-0.05,-0.20, 3.45, QUANTITY='THERMOCOUPLE'  /
&DEVC ID='Gas-4', XYZ=-0.05,-0.20, 4.45, QUANTITY='THERMOCOUPLE'  /
&DEVC ID='Gas-5', XYZ=-0.05,-0.20, 5.45, QUANTITY='THERMOCOUPLE'  /

&DEVC ID='Steel-1', XYZ=-0.05,-0.10, 1.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='Steel-2', XYZ=-0.05,-0.10, 2.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='Steel-3', XYZ=-0.05,-0.10, 3.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='Steel-4', XYZ=-0.05,-0.10, 4.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='Steel-5', XYZ=-0.05,-0.10, 5.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /

&SLCF PBY=0.0,QUANTITY='TEMPERATURE',VECTOR=.TRUE./

&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='ADIABATIC SURFACE TEMPERATURE' /
&BNDF QUANTITY='GAUGE HEAT FLUX' /

&TAIL /

