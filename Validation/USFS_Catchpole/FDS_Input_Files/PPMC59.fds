&HEAD CHID='PPMC59', TITLE='Catchpole simulation using Lagrangian particles' /

&TIME T_END=300.0 /

&WIND SPEED=1.79 /

&OBST XB=-1.0,-0.9,-1.5,1.5,0.0,0.051 /  Baffle upstream of fire

&OBST XB= 0.0, 8.0,-0.5,-0.5, 0.0, 0.051 /  Rails on both sides of vegetation, matching height of vegetation
&OBST XB= 0.0, 8.0, 0.5, 0.5, 0.0, 0.051 /

&CATF OTHER_FILES='common_inputs.txt' /

&SURF ID                   = 'Pine Needles'
      MATL_ID              = 'Dry Pine Needles'
      MOISTURE_CONTENT     = 0.071
      SURFACE_VOLUME_RATIO = 5710
      LENGTH               = 0.02
      GEOMETRY             = 'CYLINDRICAL' /

&PART ID             = 'Pine Needles' 
      DRAG_COEFFICIENT = 2.8
      COLOR          = 'GREEN'
      SURF_ID        = 'Pine Needles' 
      QUANTITIES     = 'PARTICLE TEMPERATURE','PARTICLE MASS'
      STATIC         = .TRUE. /

&INIT PART_ID              = 'Pine Needles'
      XB                   = 0.0,8.0,-0.5,0.5,0,0.051
      N_PARTICLES_PER_CELL = 1
      CELL_CENTERED        = .TRUE.
      MASS_PER_VOLUME      = 10.2
      DRY                  = T /

&SURF ID='IGN FIRE', HRRPUA=800., RAMP_Q='RAMPFIRE', COLOR='RED' /

&RAMP ID='RAMPFIRE',T= 0., F=0.0 /
&RAMP ID='RAMPFIRE',T= 1., F=1.0 /
&RAMP ID='RAMPFIRE',T=20.0, F=1.0 /
&RAMP ID='RAMPFIRE',T=21.0, F=0.0 /

&VENT XB=-0.05,0.05,-0.5,0.5,0,0, SURF_ID='IGN FIRE' /

&DEVC ID='x', QUANTITY='WALL TEMPERATURE', PART_ID='Pine Needles', SPATIAL_STATISTIC='MAXLOC X', XB=0,8,-0.1,0.1,0,0.203/

&DEVC ID='t', XYZ=0,0,0, QUANTITY='TIME', OUTPUT=F, SETPOINT=21.0/
&DEVC ID='HRR', QUANTITY='HRRPUV', XB=-1.,9.,-1.5,1.5,0.,3., SPATIAL_STATISTIC='VOLUME INTEGRAL',
      OUTPUT=F, SETPOINT=0.1, LATCH=F, TRIP_DIRECTION=-1/

&CTRL ID='kill', FUNCTION_TYPE='ALL', INPUT_ID='t','HRR'/
&CTRL FUNCTION_TYPE='KILL', INPUT_ID='kill' /

&TAIL /

