&HEAD CHID='Case_C064_LS', TITLE='Australian Grassland Fire Experiment, Case C064' /

&MESH IJK=24,24,8, XB=-10.0,110.0,-60.0,60.0,0.0,40.0 /

&TIME T_END=120. /

&MISC SIMULATION_MODE='SVLES', TMPA=32., LEVEL_SET_MODE=4 /

&WIND SPEED=4.6, Z_REF=2., DIRECTION=270., L=-500., Z_0=0.03 /

&REAC FUEL='FUEL VAPOR', C=3.4, H=6.2, O=2.5, SOOT_YIELD=0.02, HEAT_OF_COMBUSTION=17700. /

&SURF ID='IGNITION', VEG_LSET_IGNITE_TIME=0. /
&SURF ID='LINE', VEG_LSET_ROS_00 = 1./

&VENT XB=-5.0,0.0, -5., 5.,0.,0., SURF_ID='IGNITION' /
&VENT XB=-5.0,0.0,-25.,-5.,0.,0., SURF_ID='LINE' /
&VENT XB=-5.0,0.0,  5.,25.,0.,0., SURF_ID='LINE' /
&VENT XB= 0.0,100.0,-50.,50.,0.,0., SURF_ID='Short Grass' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&SURF ID='ROAD', COLOR='BEIGE' /

&VENT XB=-10,110,-53,-50,0,0, SURF_ID='ROAD' /
&VENT XB=-10,110, 50, 53,0,0, SURF_ID='ROAD' /
&VENT XB=-10,110, 54, 57,0,0, SURF_ID='ROAD' /
&VENT XB= -3,  0,-60, 60,0,0, SURF_ID='ROAD' /
&VENT XB=100,103,-60, 60,0,0, SURF_ID='ROAD' /
&VENT XB=104,107,-60, 60,0,0, SURF_ID='ROAD' /

&SURF ID                        = 'Short Grass'
      VEG_LSET_FUEL_INDEX       = 1
      VEG_LSET_M1               = 0.063 /

&DUMP DT_HRR=1., DT_DEVC=1., DT_SLCF=1., SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SLCF PBY=1., QUANTITY='TEMPERATURE', VECTOR=T /
&SLCF PBZ=2., QUANTITY='VELOCITY', VECTOR=T /
&SLCF AGL_SLICE=0.1, QUANTITY='LEVEL SET VALUE' /
&SLCF PBZ=0.1, QUANTITY='HRRPUV', CELL_CENTERED=T /

&BNDF QUANTITY='BURNING RATE' /

&DEVC ID='x', QUANTITY='BURNING RATE', SPATIAL_STATISTIC='MAXLOC X', XB=-10,110,-5,0,0,5 /
&DEVC ID='D', QUANTITY='FIRE DEPTH', XB=-10,110,0,0,0.1,0.1 /

&TAIL /
