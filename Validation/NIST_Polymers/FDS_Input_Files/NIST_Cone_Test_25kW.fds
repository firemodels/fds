&HEAD CHID='NIST_Cone_Test_25kW', TITLE='Black PMMA at 25 kW/m2 in Cone Calorimeter' /

&MESH IJK=10,10,25, XB=-0.10,0.00,-0.10,0.00,-0.03,0.22, MULT_ID='mesh' /
&MULT ID='mesh', DX=0.10, DY=0.10, DZ=0.25, I_UPPER=1, J_UPPER=1, K_UPPER=1 /

&TIME T_END=0/600. /

&MATL ID                = 'KAOWOOL'
      DENSITY           = 128.
      SPECIFIC_HEAT     = 1.1
      CONDUCTIVITY_RAMP = 'k_KAOWOOL' /
&RAMP ID='k_KAOWOOL', T=  20., F=0.06 /
&RAMP ID='k_KAOWOOL', T= 260., F=0.06 /
&RAMP ID='k_KAOWOOL', T= 538., F=0.12 /
&RAMP ID='k_KAOWOOL', T= 816., F=0.21 /
&RAMP ID='k_KAOWOOL', T=1093., F=0.30 /

&CATF OTHER_FILES='pmma_properties.txt' /

&SURF ID='PMMA SLAB'
      COLOR='BLACK'
      STRETCH_FACTOR=1.
      MATL_ID='BLACK PMMA','KAOWOOL'
      THICKNESS=0.0060,0.0254 /

&OBST XB=-0.05,0.05,-0.05,0.05,-0.02,0.00, SURF_IDS='PMMA SLAB','INERT','INERT' /

&SURF ID='CONE', TMP_FRONT=640, COLOR='ORANGE' /

&MULT ID='south', DXB=0.01,-.01,0.01,0.01,0.01,0.01, N_LOWER=0, N_UPPER=5 /
&MULT ID='north', DXB=0.01,-.01,-.01,-.01,0.01,0.01, N_LOWER=0, N_UPPER=5 /
&MULT ID='east',  DXB=-.01,-.01,0.01,-.01,0.01,0.01, N_LOWER=0, N_UPPER=5 /
&MULT ID='west',  DXB=0.01,0.01,0.01,-.01,0.01,0.01, N_LOWER=0, N_UPPER=5 /

&OBST XB=-0.09, 0.09,-0.09,-0.08,0.05,0.06, MULT_ID='south', SURF_ID6='INERT','INERT','INERT','CONE','CONE','INERT' /
&OBST XB=-0.09, 0.09, 0.08, 0.09,0.05,0.06, MULT_ID='north', SURF_ID6='INERT','INERT','CONE','INERT','CONE','INERT' /
&OBST XB=-0.09,-0.08,-0.09, 0.09,0.05,0.06, MULT_ID='west',  SURF_ID6='INERT','CONE','INERT','INERT','CONE','INERT' /
&OBST XB= 0.08, 0.09,-0.09, 0.09,0.05,0.06, MULT_ID='east',  SURF_ID6='CONE','INERT','INERT','INERT','CONE','INERT' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMIN', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DUMP DT_DEVC=2., SIG_FIGS=4 /

&DEVC XB=-0.10,0.10,-0.10,0.10,-0.03,0.47, QUANTITY='HRRPUV', ID='HRR', SPATIAL_STATISTIC='VOLUME INTEGRAL', CONVERSION_FACTOR=100. /

&SLCF PBY=0., QUANTITY='TEMPERATURE' /
&SLCF PBY=0., QUANTITY='HRRPUV' /

&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='GAUGE HEAT FLUX' /
&BNDF QUANTITY='BURNING RATE' /

&TAIL /


