&HEAD CHID='terrain_test', TITLE='terrain test' /
&MISC TERRAIN_CASE=.TRUE./ 
&TIME T_END=60 / 

&MESH IJK=50,50,50, XB=0,50,0,50,50,100 /

&INIT XB=0,50,0,50,50,60 TEMPERATURE=300.0 /
&INIT XB=0,50,0,50,60,70 TEMPERATURE=350.0 /
&INIT XB=0,50,0,50,70,80 TEMPERATURE=400.0 /
&INIT XB=0,50,0,50,80,90 TEMPERATURE=450.0 /
&INIT XB=0,50,0,50,90,100 TEMPERATURE=500.0 /

&OBST XB=20,30,0,50, 50, 60/
&OBST XB=23,27,20,30, 60, 70,NOTERRAIN=.TRUE. /

&DUMP DT_SLCF=0.1 /

&SLCF PBZ=85, QUANTITY='TEMPERATURE' /
&SLCF AGL_SLICE=35, QUANTITY='TEMPERATURE' /

&TAIL /
