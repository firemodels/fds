&HEAD CHID='mass_heat_wall_device_test_2', TITLE='Test of MASS FLOW WALL and HEAT FLOW WALL DEVC quantities on -IOR surfaces'/

&MESH ID='MESH', IJK=80,80,40, XB=00.00,12.00,00.00,12.00,10.00,16.00/

&TIME T_END=60.0/

&DUMP MASS_FILE=.TRUE./

&SPEC ID='AIR', SPECIFIC_HEAT=1., BACKGROUND=.TRUE./

&MISC TMPA=100/

&PRES VELOCITY_TOLERANCE=0.001, MAX_PRESSURE_ITERATIONS=100/

&RADI RADIATION=.FALSE./

&SURF ID='ADIABATIC', COLOR='GRAY 80', ADIABATIC=.TRUE., DEFAULT=.TRUE./
&SURF ID='Exhaust', MASS_FLUX_TOTAL=1.0/

&DEVC ID='MF vent', QUANTITY='MASS FLOW WALL', XB=11.99,11.99,08.00,10.00,13.00,15.00, IOR=-1/
&DEVC ID='MF open', QUANTITY='MASS FLOW WALL', XB=06.00,10.00,02.00,06.00,15.99,15.99, IOR=-3/

&DEVC ID='HF vent', QUANTITY='HEAT FLOW WALL', XB=11.99,11.99,08.00,10.00,13.00,15.00, IOR=-1/
&DEVC ID='HF open', QUANTITY='HEAT FLOW WALL', XB=06.00,10.00,02.00,06.00,15.99,15.99, IOR=-3/

&VENT SURF_ID='Exhaust', XB=12.00,12.00,08.00,10.00,13.00,15.00, COLOR='BLACK'/
&VENT SURF_ID='OPEN', XB=06.00,10.00,02.00,06.00,16.00,16.00/

&SLCF QUANTITY='TEMPERATURE', VECTOR=.TRUE., PBY=4/
&SLCF QUANTITY='VELOCITY', VECTOR=.TRUE., PBY=4/
&SLCF QUANTITY='PRESSURE', VECTOR=.TRUE., PBY=4/
&SLCF QUANTITY='H', VECTOR=.TRUE., PBY=4/
&SLCF QUANTITY='TEMPERATURE', VECTOR=.TRUE., PBY=9/
&SLCF QUANTITY='VELOCITY', VECTOR=.TRUE., PBY=9/
&SLCF QUANTITY='PRESSURE', VECTOR=.TRUE., PBY=9/
&SLCF QUANTITY='H', VECTOR=.TRUE., PBY=9/

&TAIL /
