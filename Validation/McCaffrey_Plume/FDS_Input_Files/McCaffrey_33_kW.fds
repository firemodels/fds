&HEAD CHID='McCaffrey_33_kW', TITLE='McCaffrey, NBSIR 79-1910, 33 kW Natural Gas' /

&MESH IJK=60,60,200, XB=-0.75,0.75,-0.75,0.75,-0.25,4.75 /

&TIME T_END=30. /

&SURF ID='burner', HRRPUA=367., TMP_FRONT=100., COLOR='RED' /
&OBST XB=-.15,0.15,-.15,0.15,-.10,0.00,SURF_IDS='burner','INERT','INERT' /

&REAC FUEL='METHANE'
      C=1.
      H=4.
      CO_YIELD=0.0
      SOOT_YIELD=0.0 /

&RADI RADIATIVE_FRACTION=0.20 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMIN', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DEVC ID='temp', XB=0.00,0.00,0.00,0.00,0.0125,4.7375, POINTS=190, QUANTITY='TEMPERATURE', Z_ID='Height' /
&DEVC ID='velo', XB=0.00,0.00,0.00,0.00,0.0125,4.7375, POINTS=190, QUANTITY='W-VELOCITY', HIDE_COORDINATES=.TRUE. /

&SLCF PBY=0.0,QUANTITY='TEMPERATURE',VECTOR=.TRUE. /
&SLCF PBY=0.0,QUANTITY='HRRPUV'  /

&TAIL /

