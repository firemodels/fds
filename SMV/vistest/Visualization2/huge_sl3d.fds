&HEAD CHID='huge_sl3d', TITLE='generate large 3d slice file for SMV debug'/
&MESH IJK=128,128,128, XB=0,1,0,1,0,1/
&REAC CO_YIELD=0.2,SOOT_YIELD=0.01,FUEL='PROPANE'/
&TIME T_END=15, DT=.02, LOCK_TIME_STEP=.TRUE./
&DUMP DT_SL3D=.02/
&MISC NOISE=.FALSE./
&SLCF XB=0.0,6.4,0.0,8.0,0.0,4.8, QUANTITY='TEMPERATURE' / 3D slice
&SLCF XB=0.0,6.4,0.0,8.0,0.0,4.8, QUANTITY='DENSITY',SPEC_ID='SOOT'/
&TAIL/
