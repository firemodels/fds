&HEAD CHID='Ulster_SBI_60_kW', TITLE='University of Ulster SBI heat flux measurements' /

&MESH IJK=30,30,100, XB=0.00,0.60,0.00,0.60,0.00,2.00 /

&TIME T_END=60. /

&REAC SOOT_YIELD=0.015, FUEL='PROPANE' /

&SURF ID='burner', HRRPUA=1786., TMP_FRONT=220., COLOR='TAN' /

&VENT XB=0.04,0.08,0.04,0.28,0.00,0.00,SURF_ID='burner' /
&VENT XB=0.08,0.12,0.04,0.24,0.00,0.00,SURF_ID='burner' /
&VENT XB=0.12,0.16,0.04,0.20,0.00,0.00,SURF_ID='burner' /
&VENT XB=0.16,0.20,0.04,0.16,0.00,0.00,SURF_ID='burner' /
&VENT XB=0.20,0.24,0.04,0.12,0.00,0.00,SURF_ID='burner' /
&VENT XB=0.24,0.28,0.04,0.08,0.00,0.00,SURF_ID='burner' /

&MATL ID            = 'FIBER MATERIAL'
      FYI           = 'Only a guess'
      CONDUCTIVITY  = 0.045
      SPECIFIC_HEAT = 2.0
      DENSITY       = 250. /

&MATL ID            = 'CONCRETE'
      FYI           = 'Drysdale, Intro to Fire Dynamics'
      SPECIFIC_HEAT = 0.90
      DENSITY       = 2000.
      EMISSIVITY    = 0.95
      CONDUCTIVITY  = 1.0  /

&SURF ID         = 'FLOOR'
      MATL_ID    = 'CONCRETE'
      THICKNESS  = 0.1
      COLOR      = 'GRAY' /

&SURF ID         = 'FIBER BOARD'
      DEFAULT    = .TRUE.
      MATL_ID    = 'FIBER MATERIAL'
      COLOR      = 'ANTIQUE WHITE'
      THICKNESS  = 0.025 /

&VENT XB = 0.5,0.6,0.0,0.6,0.0,0.0 SURF_ID = 'OPEN' /
&VENT XB = 0.0,0.5,0.5,0.6,0.0,0.0 SURF_ID = 'OPEN' /
&VENT MB='ZMIN', SURF_ID='FLOOR' /
&VENT MB='ZMAX', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /

&DEVC ID='Left 29',  QUANTITY='GAUGE HEAT FLUX', XB=0.29,0.29,0.0,0.0,0.01,1.99, POINTS=100, IOR=2, Z_ID='Height' /
&DEVC ID='Left 16',  QUANTITY='GAUGE HEAT FLUX', XB=0.16,0.16,0.0,0.0,0.01,1.99, POINTS=100, IOR=2, HIDE_COORDINATES=.TRUE. /
&DEVC ID='Left 3',   QUANTITY='GAUGE HEAT FLUX', XB=0.03,0.03,0.0,0.0,0.01,1.99, POINTS=100, IOR=2, HIDE_COORDINATES=.TRUE. /
&DEVC ID='Right 3',  QUANTITY='GAUGE HEAT FLUX', XB=0.0,0.0,0.03,0.03,0.01,1.99, POINTS=100, IOR=1, HIDE_COORDINATES=.TRUE. /
&DEVC ID='Right 16', QUANTITY='GAUGE HEAT FLUX', XB=0.0,0.0,0.16,0.16,0.01,1.99, POINTS=100, IOR=1, HIDE_COORDINATES=.TRUE. /
&DEVC ID='Right 29', QUANTITY='GAUGE HEAT FLUX', XB=0.0,0.0,0.29,0.29,0.01,1.99, POINTS=100, IOR=1, HIDE_COORDINATES=.TRUE. /

&SLCF PBY=0.01,QUANTITY='TEMPERATURE',VECTOR=.TRUE. /
&SLCF PBY=0.01,QUANTITY='HRRPUV'  /
&SLCF PBY=0.01,QUANTITY='INTEGRATED INTENSITY'  /
&SLCF PBY=0.01,QUANTITY='ABSORPTION COEFFICIENT'  /
&SLCF PBY=0.01,QUANTITY='RADIATION LOSS'  /

&BNDF QUANTITY='GAUGE HEAT FLUX'  /
&BNDF QUANTITY='WALL TEMPERATURE'  /

&TAIL /

