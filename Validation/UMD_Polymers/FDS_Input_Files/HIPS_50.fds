&HEAD CHID='HIPS_50', TITLE='HIPS Gasification at 50 kW/m2' /

&MESH IJK=3,3,4, XB=-0.15,0.15,-0.15,0.15,0.0,0.4 / 

&MISC Y_O2_INFTY=0.0001, SOLID_PHASE_ONLY=.TRUE., TMPA = 87. /

&TIME T_END=700, WALL_INCREMENT=1, DT=0.1 /

&REAC FUEL='CB GAS', C=1., H=4., SOOT_YIELD=0. /

&MATL ID='HIPS'
      ABSORPTION_COEFFICIENT=2250.
      N_REACTIONS=1
      A(1) = 1.70E20
      E(1) = 3.01E5
      EMISSIVITY=0.95
      DENSITY= 1060.
      SPEC_ID='CB GAS'
      NU_SPEC= 0.957
      MATL_ID='HIPS char'
      NU_MATL=0.043
      CONDUCTIVITY_RAMP ='k_HIPS'
      SPECIFIC_HEAT_RAMP = 'cp_HIPS'
      HEAT_OF_REACTION = 689. /

&MATL ID='HIPS char'
      ABSORPTION_COEFFICIENT=100000000.
      EMISSIVITY=0.95
      DENSITY=1060
      CONDUCTIVITY_RAMP = 'k_HIPS'
      SPECIFIC_HEAT_RAMP = 'cp_HIPS' /

&RAMP ID='k_HIPS', T= 27., F=0.130 /
&RAMP ID='k_HIPS', T=597., F=0.187 /

&RAMP ID='cp_HIPS', T= 27., F=1.61 /
&RAMP ID='cp_HIPS', T=597., F=3.55 /

&SURF ID='top'
      COLOR='BLACK'
      MATL_ID ='HIPS'
      THICKNESS= 0.006
      STRETCH_FACTOR=1.
      EXTERNAL_FLUX=50.
      TMP_GAS_BACK=37., TMP_INNER=37.
      HEAT_TRANSFER_COEFFICIENT=5. /

&VENT XB=-.05,0.05,-.05,0.05,0.0,0.0, SURF_ID='top' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DUMP DT_DEVC=10. /
&DEVC ID='MLR', XYZ=0.0,0.0,0.0, QUANTITY='BURNING RATE', IOR=3 /

&TAIL / 
