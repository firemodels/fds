&HEAD CHID='Case_F19', TITLE='Australian Grassland Fire Experiment, Case F19' /

&MESH IJK=80,80,40, XB=-20.0,20.0,0.0,40.0,0.0,20.0, MULT_ID='mesh' /
&MULT ID='mesh', DX=40., DY=40., DZ=20., I_LOWER=0, I_UPPER=5, J_LOWER=-3, J_UPPER=2, K_UPPER=0 /

&TIME T_END=180. / 

&MISC TMPA=34. /

&WIND SPEED=4.8, DIRECTION=270., L=-500., Z_0=0.03 /

&REAC FUEL='CELLULOSE', C=6, H=10, O=5, SOOT_YIELD=0.02, HEAT_OF_COMBUSTION=15600. /

&SPEC ID='WATER VAPOR' /

&SURF ID='IGNITION', HRRPUA=1460., COLOR='RED', TMP_FRONT=100., RAMP_Q='ignitor' /
&RAMP ID='ignitor', T= 0., F=0. /
&RAMP ID='ignitor', T= 1., F=1. /
&RAMP ID='ignitor', T=11., F=1. /
&RAMP ID='ignitor', T=12., F=0. /

&VENT XB=0.0,3.3,-87.,87.,0.,0., SURF_ID='IGNITION', XYZ=0,0,0, SPREAD_RATE=1.56 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&MATL ID='DIRT'
      CONDUCTIVITY = 0.25
      SPECIFIC_HEAT = 2.
      DENSITY = 1300. /

&SURF ID='GROUND' 
      DEFAULT=.TRUE. 
      MATL_ID='DIRT'
      THICKNESS=0.2 /

&SURF ID                        = 'wet vegetation'
      MATL_ID                   = 'DRY GRASS'
      MOISTURE_FRACTION         = 0.058
      SURFACE_VOLUME_RATIO      = 12240.
      LENGTH                    = 0.20
      GEOMETRY                  = 'CYLINDRICAL' /
      
&MATL ID                 = 'DRY GRASS'
      DENSITY            = 512.
      CONDUCTIVITY       = 0.1
      SPECIFIC_HEAT_RAMP = 'c_v'
      A                  = 36300.
      E                  = 60300.
      MATL_ID            = 'CHAR'
      NU_MATL            = 0.2
      SPEC_ID            = 'CELLULOSE'
      NU_SPEC            = 0.8
      HEAT_OF_REACTION   = 416. /

&RAMP ID='c_v', T= 20., F=1.184 /
&RAMP ID='c_v', T=800., F=4.070 /

&MATL ID                 = 'CHAR'
      DENSITY            = 300.
      CONDUCTIVITY       = 0.052
      SPECIFIC_HEAT_RAMP = 'cp_char'
      N_S                = 0.
      NU_O2_CHAR         = 1.65
      BETA_CHAR          = 0.2
      A                  = 430.
      E                  = 74800.
      SPEC_ID            = 'PRODUCTS','AIR'
      NU_SPEC            = 8.18,-7.2
      MATL_ID            = 'ASH'
      NU_MATL            = 0.02
      HEAT_OF_REACTION   = -32000. /

&RAMP ID='cp_char', T=   27, F=0.715 /
&RAMP ID='cp_char', T= 1724, F=2.04 /

&MATL ID            = 'ASH'
      DENSITY       = 67.
      CONDUCTIVITY  = 0.1
      SPECIFIC_HEAT = 2.0 /

&PART ID='blade of grass'
      SAMPLING_FACTOR=20 
      SURF_ID='wet vegetation' 
      PROP_ID='grass image' 
      DRAG_COEFFICIENT=2.8
      SHAPE_FACTOR = 0.15
      QUANTITIES='PARTICLE TEMPERATURE','PARTICLE MASS' 
      STATIC=.TRUE. 
      COLOR='BROWN' /

&INIT PART_ID='blade of grass', XB=0.,200.,-100.,100.,0.00,0.49, N_PARTICLES_PER_CELL=1, CELL_CENTERED=.TRUE., PACKING_RATIO=0.0012 /

&PROP ID='grass image', SMOKEVIEW_ID='TUBE', SMOKEVIEW_PARAMETERS='L=0.20','D=0.00016','DIRZ=1' /

&DUMP DT_HRR=1., DT_DEVC=1., SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SLCF PBY=1., QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
&SLCF PBZ=2., QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBY=1., QUANTITY='INTEGRATED INTENSITY', CELL_CENTERED=.TRUE. /

&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='GAUGE HEAT FLUX' /

&DEVC ID='x', QUANTITY='TEMPERATURE', SPATIAL_STATISTIC='MAXLOC X', XB=-20,220,-1,0,0,1 /

&TAIL /
