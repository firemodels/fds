&HEAD CHID='Riahi_Gasoline', TITLE='Validation of soot deposition in hood experiments, SVN $Revision$'/

&MESH IJK=60,60,90, XB=0.0,0.6,0.0,0.6,0.1,1.0 /

&TIME T_END=300.0 /

&SPEC ID = 'NITROGEN',        LUMPED_COMPONENT_ONLY = .TRUE. /
&SPEC ID = 'OXYGEN',          LUMPED_COMPONENT_ONLY = .TRUE. /
&SPEC ID = 'CARBON DIOXIDE',  LUMPED_COMPONENT_ONLY = .TRUE. /
&SPEC ID = 'WATER VAPOR',     LUMPED_COMPONENT_ONLY = .TRUE. /
&SPEC ID = 'OCTANE'           FORMULA='C8H18'                /
&SPEC ID = 'SOOT',            AEROSOL=.TRUE.                 /

&SPEC ID='AIR', SPEC_ID='OXYGEN','NITROGEN',
      VOLUME_FRACTION=1.000,3.7619, BACKGROUND = .TRUE. /

&SPEC ID='PRODUCTS', SPEC_ID='CARBON DIOXIDE','WATER VAPOR','NITROGEN',
      VOLUME_FRACTION=7.2392,9,44.1615 /

&REAC FUEL='OCTANE', HEAT_OF_COMBUSTION=43540.,
      SPEC_ID_NU='OCTANE','AIR','PRODUCTS','SOOT',
      NU=-1.,-11.739155,1,0.7608 /

&MATL ID                 = 'INSULATION'
      FYI                = 'Fiberfrax Duraboard HD, 0.5 inch'
      CONDUCTIVITY_RAMP  = 'k_insulation'
      SPECIFIC_HEAT      = 1.13
      DENSITY            = 416. /
&RAMP ID='k_insulation', T= 250., F=0.084 /
&RAMP ID='k_insulation', T= 500., F=0.116 /
&RAMP ID='k_insulation', T= 750., F=0.164 /
&RAMP ID='k_insulation', T=1000., F=0.228 /
&RAMP ID='k_insulation', T=1250., F=0.309 /

&SURF ID         = 'FIBER BOARD'
      MATL_ID    = 'INSULATION'
      THICKNESS  = 0.0127
      COLOR      = 'SNOW 3'
      DEFAULT    = .TRUE. /

cVENT MB='XMIN', SURF_ID='OPEN' /
cVENT MB='XMAX', SURF_ID='OPEN' /
cVENT MB='YMIN', SURF_ID='OPEN' /
cVENT MB='YMAX', SURF_ID='OPEN' /
cVENT MB='ZMIN', SURF_ID='OPEN' /
cVENT MB='ZMAX', SURF_ID='OPEN' /

&OBST XB=0.0,0.0,0.0,0.6,0.1,1.0, SURF_ID='FIBER BOARD' /
&OBST XB=0.6,0.6,0.0,0.6,0.1,1.0, SURF_ID='FIBER BOARD' /
&OBST XB=0.0,0.6,0.0,0.0,0.1,1.0, SURF_ID='FIBER BOARD' /
&OBST XB=0.0,0.6,0.6,0.6,0.1,1.0, SURF_ID='FIBER BOARD' /
&OBST XB=0.0,0.6,0.0,0.6,1.0,1.0, SURF_ID='FIBER BOARD' /

&OBST XB=0.26,0.34,0.26,0.34,0.10,0.10, SURF_IDS = 'FUEL','INERT','INERT' /
&SURF ID='FUEL', HRRPUA=203.125 /

&BNDF QUANTITY='SURFACE DEPOSITION', SPEC_ID='SOOT' /

&TAIL /
