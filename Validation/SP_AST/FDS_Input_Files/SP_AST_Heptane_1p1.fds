&HEAD CHID='SP_AST_Heptane_1p1', TITLE='SP Adiabatic Surface Temperature Validation, Column Test'/

&MESH IJK=32,32,80, XB=-1.6,1.6,-1.6,1.6,0.0,8.0 /
&TIME TIME_SHRINK_FACTOR=10., T_END=1200. /

&REAC FUEL='N-HEPTANE', SOOT_YIELD=0.037 /

&SURF ID='BURNER', HRRPUA=2275., RAMP_Q='fire', TMP_FRONT=100., COLOR='ORANGE' / 
&RAMP ID='fire', T=   0., F=0. /
&RAMP ID='fire', T= 120., F=1. /
&RAMP ID='fire', T= 780., F=1. /
&RAMP ID='fire', T= 900., F=0. /

&OBST XB=-0.50, 0.50,-0.50, 0.50, 0.00, 0.20, SURF_IDS='BURNER','STEEL PLATE','STEEL PLATE' /

&OBST XB=-0.10, 0.10,-0.10, 0.10, 0.40, 6.40, SURF_ID='STEEL PLATE' /

&VENT XB= 0.00, 0.10,-0.10,-0.10, 1.40, 1.50, SURF_ID='PLATE FRONT', IOR=-2 /
&VENT XB= 0.00, 0.10,-0.10,-0.10, 2.40, 2.50, SURF_ID='PLATE FRONT', IOR=-2 /
&VENT XB= 0.00, 0.10,-0.10,-0.10, 3.40, 3.50, SURF_ID='PLATE FRONT', IOR=-2 /
&VENT XB= 0.00, 0.10,-0.10,-0.10, 4.40, 4.50, SURF_ID='PLATE FRONT', IOR=-2 /
&VENT XB= 0.00, 0.10,-0.10,-0.10, 5.40, 5.50, SURF_ID='PLATE FRONT', IOR=-2 /

&SURF ID='STEEL PLATE' 
      MATL_ID='STEEL'
      THICKNESS=0.010
      COLOR='BLACK' /

&SURF ID='PLATE FRONT' 
      MATL_ID='INCONEL','INSULATION'
      THICKNESS=0.0007,0.02
      COLOR='GRAY 30' /

&VENT MB='XMIN', SURF_ID='OPEN'/
&VENT MB='XMAX', SURF_ID='OPEN'/
&VENT MB='YMIN', SURF_ID='OPEN'/
&VENT MB='YMAX', SURF_ID='OPEN'/
&VENT MB='ZMAX', SURF_ID='OPEN'/

&MATL ID            = 'STEEL'
      SPECIFIC_HEAT = 0.465      
      CONDUCTIVITY  = 54.      
      DENSITY       = 7833. /

&MATL ID='INCONEL'
      EMISSIVITY=0.85
      DENSITY=8470.
      CONDUCTIVITY_RAMP='k_inc'
      SPECIFIC_HEAT_RAMP='c_inc' /

&RAMP ID='k_inc', T= 20., F=14.9 /
&RAMP ID='k_inc', T=100., F=15.9 /
&RAMP ID='k_inc', T=200., F=17.3 /
&RAMP ID='k_inc', T=300., F=19.0 /
&RAMP ID='k_inc', T=400., F=20.5 /
&RAMP ID='k_inc', T=500., F=22.1 /
&RAMP ID='k_inc', T=600., F=23.9 /
&RAMP ID='k_inc', T=700., F=25.7 /
&RAMP ID='k_inc', T=800., F=27.5 /

&RAMP ID='c_inc', T= 20., F=0.444 /
&RAMP ID='c_inc', T=100., F=0.465 /
&RAMP ID='c_inc', T=200., F=0.486 /
&RAMP ID='c_inc', T=300., F=0.502 /
&RAMP ID='c_inc', T=400., F=0.519 /
&RAMP ID='c_inc', T=500., F=0.536 /
&RAMP ID='c_inc', T=600., F=0.578 /
&RAMP ID='c_inc', T=700., F=0.595 /
&RAMP ID='c_inc', T=800., F=0.611 /
&RAMP ID='c_inc', T=900., F=0.628 /

&MATL ID            = 'INSULATION' 
      DENSITY       = 280.
      CONDUCTIVITY  = 0.1
      SPECIFIC_HEAT = 1. /

&DUMP SIG_FIGS=4, SIG_FIGS_EXP=2, DT_DEVC=30., DT_HRR=30. /

&DEVC ID='PT-1', XYZ= 0.05,-0.11, 1.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='PT-2', XYZ= 0.05,-0.11, 2.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='PT-3', XYZ= 0.05,-0.11, 3.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='PT-4', XYZ= 0.05,-0.11, 4.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='PT-5', XYZ= 0.05,-0.11, 5.45, IOR=-2, QUANTITY='WALL TEMPERATURE'  /

&DEVC ID='AST-1', XYZ= 0.05,-0.11, 1.45, IOR=-2, QUANTITY='ADIABATIC SURFACE TEMPERATURE'  /
&DEVC ID='AST-2', XYZ= 0.05,-0.11, 2.45, IOR=-2, QUANTITY='ADIABATIC SURFACE TEMPERATURE'  /
&DEVC ID='AST-3', XYZ= 0.05,-0.11, 3.45, IOR=-2, QUANTITY='ADIABATIC SURFACE TEMPERATURE'  /
&DEVC ID='AST-4', XYZ= 0.05,-0.11, 4.45, IOR=-2, QUANTITY='ADIABATIC SURFACE TEMPERATURE'  /
&DEVC ID='AST-5', XYZ= 0.05,-0.11, 5.45, IOR=-2, QUANTITY='ADIABATIC SURFACE TEMPERATURE'  /

&DEVC ID='Gas-1', XYZ= 0.05,-0.15, 1.45, QUANTITY='THERMOCOUPLE'  /
&DEVC ID='Gas-2', XYZ= 0.05,-0.15, 2.45, QUANTITY='THERMOCOUPLE'  /
&DEVC ID='Gas-3', XYZ= 0.05,-0.15, 3.45, QUANTITY='THERMOCOUPLE'  /
&DEVC ID='Gas-4', XYZ= 0.05,-0.15, 4.45, QUANTITY='THERMOCOUPLE'  /
&DEVC ID='Gas-5', XYZ= 0.05,-0.15, 5.45, QUANTITY='THERMOCOUPLE'  /

&DEVC ID='Steel-1', XYZ=-0.11,-0.05, 1.45, IOR=-1, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='Steel-2', XYZ=-0.11,-0.05, 2.45, IOR=-1, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='Steel-3', XYZ=-0.11,-0.05, 3.45, IOR=-1, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='Steel-4', XYZ=-0.11,-0.05, 4.45, IOR=-1, QUANTITY='WALL TEMPERATURE'  /
&DEVC ID='Steel-5', XYZ=-0.11,-0.05, 5.45, IOR=-1, QUANTITY='WALL TEMPERATURE'  /

&SLCF PBY=0.0,QUANTITY='TEMPERATURE',CELL_CENTERED=.TRUE. /

&BNDF QUANTITY='WALL TEMPERATURE',CELL_CENTERED=.TRUE. /
&BNDF QUANTITY='ADIABATIC SURFACE TEMPERATURE',CELL_CENTERED=.TRUE. /
&BNDF QUANTITY='GAUGE HEAT FLUX',CELL_CENTERED=.TRUE. /

&TAIL /

