&HEAD CHID='z0=p02_dpdx=-100_N16', TITLE='LES of flow through a rough channel' /

&MESH IJK=128,16,16, XB=0,16,-1,1,0,2 /

&TIME T_END=20.0 /

&MISC FORCE_VECTOR(1)=100.,STRATIFICATION=.FALSE. /
&DUMP NFRAMES=100, SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SURF ID='rough wall', DEFAULT=.TRUE., COLOR='GRAY', ROUGHNESS=0.02 /

&VENT MB='XMIN', SURF_ID='PERIODIC' /
&VENT MB='XMAX', SURF_ID='PERIODIC' /

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBX=8, QUANTITY='VELOCITY', VECTOR=.TRUE. /

&DEVC XB=8.0,8.0, -1,1, 0.0,2.0, QUANTITY='U-VELOCITY', STATISTICS='MEAN' /
&DEVC XYZ=8.0,0.0,1.0, QUANTITY='U-VELOCITY' /
&DEVC XYZ=8.0,0.0,1.0, QUANTITY='VISCOSITY' /
&DEVC XYZ=8.0,0.0,1.0, QUANTITY='DENSITY' /

&TAIL /
