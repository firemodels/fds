&HEAD CHID='Needle_TGA_H', TITLE='TGA for pine needles' / 

&MESH IJK=3,1,4, XB=-2,2,-0.5,0.5,0,1 / 

&TIME T_END=1200., WALL_INCREMENT=1, DT=0.2 /

&MISC TMPA=28., SOLID_PHASE_ONLY=F /
&RAMP ID = 'T_RAMP' T =    0., F = 0.000 /
&RAMP ID = 'T_RAMP' T =   77., F = 0.063 /
&RAMP ID = 'T_RAMP' T =  250., F = 0.079 /
&RAMP ID = 'T_RAMP' T =  304., F = 0.116 /
&RAMP ID = 'T_RAMP' T =  546., F = 0.116 /
&RAMP ID = 'T_RAMP' T = 1129., F = 1.000 /

&RADI RADIATION=.FALSE. /

&SPEC ID='NITROGEN', MASS_FRACTION_0=0.77, BACKGROUND=T /
&SPEC ID='OXYGEN', MASS_FRACTION_0=0.23 /
&SPEC ID='FUEL VAPOR' / 
&SPEC ID='WATER VAPOR' / 
&SPEC ID='CARBON DIOXIDE' / 

&VENT XB=-1,1,-0.5,0.5,0.0,0.0, SURF_ID='SAMPLE' /

&SURF ID             = 'SAMPLE'
      COLOR          = 'RED'
      BACKING        = 'INSULATED' 
      TMP_GAS_FRONT  = 700.
      RAMP_TMP_GAS_FRONT='T_RAMP'
      THICKNESS      = 0.00002
      HEAT_TRANSFER_COEFFICIENT = 1000.
      MOISTURE_CONTENT = 1.33 
      MATL_ID(1,1:3) = 'comp 1','comp 2','comp 3'
      MATL_MASS_FRACTION(1,1:3) = 0.25,0.50,0.25 /

&CATF OTHER_FILES='Needle_TGA_Model.txt','Needle_TGA_Output.txt' /

&TAIL /
