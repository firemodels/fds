&HEAD CHID='FM_Parallel_Panel_4', TITLE='FM Parallel Panel Test, Propane, 30 kW' /

&MESH IJK=40,24,160, XB=-0.40,0.40,-0.24,0.24,-0.30,2.90 /

&TIME T_END=180.0 /

&REAC FUEL                 = 'PROPANE'			
      RADIATIVE_FRACTION = 0.29
      FYI                = 'C3H8' 
      SOOT_YIELD         = 0.024 /

&SURF ID='BURNER', HRRPUA=167., COLOR='RED' /

&MATL ID            = 'MARINITE'
      EMISSIVITY    = 0.9
      DENSITY       = 737.
      SPECIFIC_HEAT = 1.2
      CONDUCTIVITY  = 0.12 /

&SURF ID            = 'MARINITE BOARD'
      MATL_ID       = 'MARINITE'
      THICKNESS     = 0.024
      BACKING       = 'INSULATED'  
      COLOR         = 'GRAY' /

&MATL ID            = 'STEEL'
      EMISSIVITY    = 0.9
      DENSITY       = 7800.
      SPECIFIC_HEAT = 0.465        
      CONDUCTIVITY  = 43. /

&SURF ID            = 'SHEET METAL'
      MATL_ID       = 'STEEL'
      THICKNESS     = 0.002
      BACKING       = 'EXPOSED'
      COLOR         = 'BLACK' /

&OBST XB = -0.30,  0.30, -0.14,  0.16, -0.30, 0.00, SURF_IDS = 'BURNER','SHEET METAL','SHEET METAL' /
&OBST XB = -0.30, -0.28, -0.16, -0.14, -0.30, 2.44, SURF_ID = 'SHEET METAL' /
&OBST XB =  0.28,  0.30, -0.16, -0.14, -0.30, 2.44, SURF_ID = 'SHEET METAL' /
&OBST XB = -0.30, -0.28,  0.16,  0.18, -0.30, 2.44, SURF_ID = 'SHEET METAL' /
&OBST XB =  0.28,  0.30,  0.16,  0.18, -0.30, 2.44, SURF_ID = 'SHEET METAL' /
&OBST XB = -0.28,  0.28,  0.16,  0.18, -0.30, 2.44, SURF_ID = 'MARINITE BOARD'    /
&OBST XB = -0.28,  0.28, -0.16, -0.14, -0.30, 2.44, SURF_ID = 'MARINITE BOARD'    /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DEVC ID='Flux', XB=0.0,0.0,0.16,0.16,0.01,2.43, POINTS=122, Z_ID='Height',           QUANTITY='GAUGE HEAT FLUX',  IOR=-2 /
&DEVC ID='Temp', XB=0.0,0.0,0.16,0.16,0.01,2.43, POINTS=122, HIDE_COORDINATES=.TRUE., QUANTITY='WALL TEMPERATURE', IOR=-2 /

&BNDF QUANTITY='GAUGE HEAT FLUX'  /

&DUMP SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SLCF PBX=0.00 QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
 
&TAIL /
