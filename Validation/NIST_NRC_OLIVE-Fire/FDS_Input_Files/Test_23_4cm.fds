&HVAC ID='Upper Vent', TYPE_ID='LEAK', VENT_ID='Inside Upper Vent', VENT2_ID='Outside Upper Vent', AREA=0.022,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /
&VENT XB=-0.88,-0.44,-0.48,-0.48,0.32,0.52, ID='Inside Lower Vent',  SURF_ID='STEEL PLATE' /
&VENT XB=-0.88,-0.44,-0.52,-0.52,0.32,0.52, ID='Outside Lower Vent', SURF_ID='STEEL PLATE' /
&HVAC ID='Lower Vent', TYPE_ID='LEAK', VENT_ID='Inside Lower Vent', VENT2_ID='Outside Lower Vent', AREA=0.011,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /

&VENT XB= 0.96, 1.04, 0.08, 0.16,2.28,2.28, ID='Inside Roof Vent',  SURF_ID='STEEL PLATE' /
&VENT XB= 0.96, 1.04, 0.08, 0.16,2.32,2.32, ID='Outside Roof Vent', SURF_ID='STEEL PLATE' /
&HVAC ID='Roof Vent', TYPE_ID='LEAK', VENT_ID='Inside Roof Vent', VENT2_ID='Outside Roof Vent', AREA=0.008,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /

 SURF ID='BLOW', COLOR='BLUE', VOLUME_FLOW=-0.125, TAU_V=200. /
 VENT XB=-0.08, 0.08,-0.08,0.08, 0.0, 0.0, SURF_ID='BLOW' /
 DEVC ID='Vdot', QUANTITY='NORMAL VELOCITY', XB=-0.08, 0.08,-0.08,0.08, 0.0, 0.0, SPATIAL_STATISTIC='SURFACE INTEGRAL',
      CONVERSION_FACTOR=3600. /

&DUMP DT_DEVC=10., DT_HRR=10. /

&SLCF PBY=0.03, QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
&SLCF PBY=0.03, QUANTITY='PRESSURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE', CELL_CENTERED=.TRUE. /
