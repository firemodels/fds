&HVAC TYPE_ID='DUCT',NODE_ID='INLET1','MID1',ID='DUCT1',AREA=0.01,VOLUME_FLOW=0.01,LOSS=0,0,LENGTH=2.5,DUCT_INTERP_TYPE='NODE2',N_CELLS=250/
