! Reference: A. Markan, P. B. Sunderland, J. G. Quintiere, J. L. de Ris, D. P. Stocker, H. R. Baum.
! A Burning Rate Emulator (BRE) for study of condensed fuel burning in microgravity. Comb. and Flame,
! 192:272-282, 2018.
!
! See Markan et al. 2018, Table 3, for a key to the Test No. series.
!
! Stoichiometric mixture fraction for pure C2H4 is Z_stoich = 0.0637.
! Stoichiometric mixture fraction for 50% C2H4/50% N2 is Z_stoich = 0.120.

&HEAD CHID='BRE25_Test_14_1mm', TITLE='UMD Burning Rate Emulator, Markan et al. 2018'/

&MESH IJK=160,1,188, XB=0,0.160,-0.5,0.5,-0.028,0.160, CYLINDRICAL=T/

&TIME T_END=5./

&DUMP DT_SLCF=0.1, DT_DEVC=0.1 /

&MISC SIMULATION_MODE='LES', GVEC(3)=-9.8E-5, P_INF=101325, STRATIFICATION=F
      Y_O2_INFTY=0.3287, Y_CO2_INFTY=0., HUMIDITY=0., TMPA=25, CONSTANT_SPECIFIC_HEAT_RATIO=F/

&COMB SUPPRESSION=F, RAMP_ZETA_0='zeta_0 ramp' /

&RAMP ID='zeta_0 ramp', T=0, F=1/
&RAMP ID='zeta_0 ramp', T=1, F=0/
&RAMP ID='zeta_0 ramp', T=5, F=0/

&RADI RADIATION=T, WIDE_BAND_MODEL=T, NUMBER_RADIATION_ANGLES=16 /

&REAC FUEL='50% C2H4'/

&SPEC ID='50% C2H4',
      VOLUME_FRACTION(1)=0.5, SPEC_ID(1)='ETHYLENE',
      VOLUME_FRACTION(2)=0.5, SPEC_ID(2)='NITROGEN'/

&SPEC ID='ETHYLENE', LUMPED_COMPONENT_ONLY=T/
&SPEC ID='NITROGEN', LUMPED_COMPONENT_ONLY=T/

&SURF ID='burner', TAU_MF=0.1, MASS_FLUX=6.89E-03, SPEC_ID='50% C2H4', TMP_FRONT=35.0, COLOR='RED', RAMP_T='T_S_ramp'/

&OBST XB=0,0.0125,-0.5,0.5,-0.028,0, SURF_IDS='burner','INERT','INERT'/

&VENT DB='ZMAX', SURF_ID='OPEN'/
&VENT DB='XMAX', SURF_ID='OPEN'/

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0, QUANTITY='TEMPERATURE', CELL_CENTERED=T/
&SLCF PBY=0, QUANTITY='MIXTURE FRACTION', CELL_CENTERED=T/
&SLCF PBY=0, QUANTITY='INTEGRATED INTENSITY', CELL_CENTERED=T/

&DEVC XB=0.001,0.001,0,0,0,0.160, STATISTICS_START=4.9, POINTS=160, QUANTITY='MIXTURE FRACTION', ID='Z'/
&DEVC XB=0.001,0.001,0,0,0,0.160, STATISTICS_START=4.9, POINTS=160, QUANTITY='TEMPERATURE', ID='T', HIDE_COORDINATES=T/

&DEVC XYZ=0.00000,0,0.0001, IOR=3, QUANTITY='CONVECTIVE HEAT FLUX', ID='qconv0mm'/
&DEVC XYZ=0.00825,0,0.0001, IOR=3, QUANTITY='CONVECTIVE HEAT FLUX', ID='qconv8mm'/

&DEVC XYZ=0.00000,0,0.0001, IOR=3, QUANTITY='RADIATIVE HEAT FLUX', ID='qrad0mm'/
&DEVC XYZ=0.00825,0,0.0001, IOR=3, QUANTITY='RADIATIVE HEAT FLUX', ID='qrad8mm'/

&DEVC XYZ=0.00000,0,0.0001, IOR=3, QUANTITY='NET HEAT FLUX', ID='qnet0mm'/
&DEVC XYZ=0.00825,0,0.0001, IOR=3, QUANTITY='NET HEAT FLUX', ID='qnet8mm'/

&DEVC XYZ=0.00000,0,0.0001, IOR=3, QUANTITY='GAUGE HEAT FLUX', ID='qgauge0mm', PROP_ID='hfp'/
&DEVC XYZ=0.00825,0,0.0001, IOR=3, QUANTITY='GAUGE HEAT FLUX', ID='qgauge8mm', PROP_ID='hfp'/
&PROP ID='hfp', GAUGE_TEMPERATURE=31.5, GAUGE_EMISSIVITY=0.97 /




&RAMP ID='T_S_ramp', T=0.000, F=0.000000/
&RAMP ID='T_S_ramp', T=0.500, F=0.632149/
&RAMP ID='T_S_ramp', T=1.000, F=0.864704/
&RAMP ID='T_S_ramp', T=1.500, F=0.950256/
&RAMP ID='T_S_ramp', T=2.000, F=0.981729/
&RAMP ID='T_S_ramp', T=2.500, F=0.993307/
&RAMP ID='T_S_ramp', T=3.000, F=0.997567/
&RAMP ID='T_S_ramp', T=3.500, F=0.999133/
&RAMP ID='T_S_ramp', T=4.000, F=0.999710/
&RAMP ID='T_S_ramp', T=4.500, F=0.999922/
&RAMP ID='T_S_ramp', T=5.000, F=1.000000/

&TAIL /
