&TIME TWFIN=1600.0 /
&DUMP NFRAMES=8000, DT_DEVC=5.0, DT_HRR=5.0 /
&MISC SURF_DEFAULT='METAL WALL'/

***** starter fire

&OBST XB= 1.5, 2.1, 7.9, 8.5, 1.1, 1.6,   COLOR='BLACK' /  metal drum
&VENT XB= 1.5, 2.1, 7.9, 8.5, 1.6, 1.6 SURF_ID='FIRE'/
&SURF ID='FIRE', HRRPUA=1389.0 / 500KW

&DEVC ID='TIMER1', XYZ=0.0,0.0,0.0, QUANTITY='TIME' /
&CTRL ID='door timer1', FUNCTION_TYPE='CUSTOM', INPUT_ID='TIMER1', RAMP_ID='door times1' /
&RAMP ID='door times1', T=     0, F= 1 /
&RAMP ID='door times1', T=    59, F= 1 /
&RAMP ID='door times1', T=    61, F=-1 /
