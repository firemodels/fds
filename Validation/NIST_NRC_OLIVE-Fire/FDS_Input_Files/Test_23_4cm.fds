&SLCF PBY=0.03, QUANTITY='MASS FRACTION', SPEC_ID='SOOT', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='HRRPUV', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.03, QUANTITY='PRESSURE', CELL_CENTERED=.TRUE. /

&BNDF QUANTITY='WALL TEMPERATURE', CELL_CENTERED=.TRUE. /

&DEVC ID='HRR_ideal', XB=-0.96,-0.64,0.12,0.44,0.56,0.64, QUANTITY='MASS FLUX', SPEC_ID='METHANE',
      SPATIAL_STATISTIC='SURFACE INTEGRAL', CONVERSION_FACTOR=50000, UNITS='kW' /
&DEVC ID='HRR_actual',XB=-2.00,2.00,-2.00,2.000,0.00,3.00, QUANTITY='HRRPUV',
      SPATIAL_STATISTIC='VOLUME INTEGRAL', TEMPORAL_STATISTIC='SMOOTHED', SMOOTHING_FACTOR=0.99 /
&DEVC ID='O2',       XYZ=-0.96,-0.20,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='OXYGEN' /
&DEVC ID='CO2',      XYZ=-0.96,-0.20,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='CARBON DIOXIDE' /
&DEVC ID='CO',       XYZ=-0.96,-0.20,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='CARBON MONOXIDE' /
&DEVC ID='TC-1',     XYZ=-0.96,-0.20,2.00, QUANTITY='THERMOCOUPLE' /
&DEVC ID='TC-2',     XYZ=-0.96,-0.20,1.40, QUANTITY='THERMOCOUPLE' /
&DEVC ID='TC-3',     XYZ=-0.96,-0.20,0.50, QUANTITY='THERMOCOUPLE' /
&DEVC ID='TC-4',     XYZ= 0.96,-0.20,2.00, QUANTITY='THERMOCOUPLE' /
&DEVC ID='TC-5',     XYZ= 0.96,-0.20,1.40, QUANTITY='THERMOCOUPLE' /
&DEVC ID='TC-6',     XYZ= 0.96,-0.20,0.50, QUANTITY='THERMOCOUPLE' /
&DEVC ID='CH4',      XYZ=-0.96,-0.20,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='METHANE' /
&DEVC ID='SOOT',     XYZ=-0.96,-0.20,2.00, QUANTITY='MASS FRACTION',   SPEC_ID='SOOT' /
&DEVC ID='PRESSURE', XYZ=-0.96,-0.20,2.00, QUANTITY='PRESSURE' /

&TAIL/

