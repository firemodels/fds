&RAMP ID='c_ramp', T= 800., F=0.90 /
&RAMP ID='c_ramp', T=1000., F=0.50 /
&RAMP ID='c_ramp', T=1200., F=0.52 /

&SURF ID='Hot metallic plate', COLOR='BLACK', HT3D=T /
 
&SURF ID = 'Radiative panel' 
      NET_HEAT_FLUX = 112
      RAMP_Q = 'NHFRAMP' /

&RAMP ID='NHFRAMP', T=  0.0, F=0.0 /
&RAMP ID='NHFRAMP', T= 32.7, F=0.0 /
&RAMP ID='NHFRAMP', T= 32.8, F=1.0 /
&RAMP ID='NHFRAMP', T=384.3, F=1.0 / 
&RAMP ID='NHFRAMP', T=384.4, F=0.0 /

&SPEC ID='WATER VAPOR', MASS_EXTINCTION_COEFFICIENT=500 /

&SM3D QUANTITY='DENSITY', SPEC_ID='WATER VAPOR' /

&PART ID='Watermist', SAMPLING_FACTOR=1, SPEC_ID='WATER VAPOR', DIAMETER=188, GAMMA_D=3.3, QUANTITIES='PARTICLE TEMPERATURE','PARTICLE DIAMETER', HORIZONTAL_VELOCITY=2. /

&PROP ID='sprinkler',
      PART_ID='Watermist',
      OFFSET=0.02, 
      SPRAY_ANGLE=0,21,
      FLOW_RATE=4.6,
      PARTICLE_VELOCITY=70,
      PARTICLES_PER_SECOND=5000 /
 
&DUMP DT_DEVC=4. /

&DEVC ID='SPR', XYZ=0.0,0.0,0.5, ORIENTATION=0,0,-1, PROP_ID='sprinkler', QUANTITY='TIME', SETPOINT=383.4 /

&VENT DB='XMIN', SURF_ID='OPEN' /
&VENT DB='XMAX', SURF_ID='OPEN' /
&VENT DB='YMIN', SURF_ID='OPEN' /
&VENT DB='YMAX', SURF_ID='OPEN' /
&VENT DB='ZMIN', SURF_ID='OPEN' /
&VENT DB='ZMAX', SURF_ID='OPEN' /

&SLCF PBZ=-0.001, QUANTITY='TEMPERATURE' /
&SLCF PBZ=0.001, QUANTITY='TEMPERATURE' / 
&SLCF PBY=0, QUANTITY='TEMPERATURE' /
&SLCF PBY=0, QUANTITY='W-VELOCITY' /

