      DT_PART=1000000.0,
      DT_HRR= 1000000.0,
      DT_SLCF=1.0,
      DT_BNDF=1000000.0,
      DT_PL3D=1000000.0,
      DT_ISOF=1000000.0 /

&MISC TMPA=20.0, 
      BACKGROUND_SPECIES=’AIR’,
      NOISE=.FALSE.,
      RADIATION=.FALSE. /


&REAC ID         = 'POLYURETHANE'
      SOOT_YIELD = 0.0
      CO_YIELD   = 0.0
      N          = 1.0
      C          = 6.3
      H          = 7.1
      O          = 2.1  /

&SPEC ID='CARBON DIOXIDE' /
&SPEC ID='CARBON MONOXIDE' /
