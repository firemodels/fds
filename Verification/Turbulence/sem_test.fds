&HEAD CHID='sem_test', TITLE='Turbulent statistics in the boundary layer' /

&MESH IJK=60,36,70, XB=0.00,0.585,0.00,0.37,0.00,0.70, MULT_ID='mesh' /
&MULT ID='mesh', DX=0.585, I_UPPER=3 /

&TIME T_END=600. /

&SURF ID='INLET', VEL=-1., COLOR='INVISIBLE' /
&SURF ID='SLIP', FREE_SLIP=.TRUE., COLOR='INVISIBLE' /

&VENT PBX=0.,    SURF_ID = 'INLET', N_EDDY=100, L_EDDY=0.05, VEL_RMS=0.1 /
&VENT PBX=2.34,  SURF_ID = 'OPEN'/
&VENT PBY=0.,    SURF_ID = 'PERIODIC'/
&VENT PBY=0.37,  SURF_ID = 'PERIODIC'/
&VENT MB='ZMAX', SURF_ID = 'SLIP' /

&SLCF PBY=0.18, QUANTITY='VELOCITY', VECTOR=.TRUE. /

&DEVC XB=1.50,1.50,0.20,0.20,0.005,0.695, QUANTITY='U-VELOCITY', ID='U',     POINTS=70, Z_ID='z'  /
&DEVC XB=1.50,1.50,0.20,0.20,0.005,0.695, QUANTITY='CELL U',     ID='Urms',  POINTS=70, STATISTICS='RMS', HIDE_COORDINATES=.TRUE.  /
&DEVC XB=1.50,1.50,0.20,0.20,0.005,0.695, QUANTITY='CELL V',     ID='Vrms',  POINTS=70, STATISTICS='RMS', HIDE_COORDINATES=.TRUE.  /
&DEVC XB=1.50,1.50,0.20,0.20,0.005,0.695, QUANTITY='CELL W',     ID='Wrms',  POINTS=70, STATISTICS='RMS', HIDE_COORDINATES=.TRUE.  /
&DEVC XB=1.50,1.50,0.20,0.20,0.005,0.695, QUANTITY='CELL U', QUANTITY2='CELL W', ID='UpWp',  POINTS=70, STATISTICS='COV', HIDE_COORDINATES=.TRUE.  /

&TAIL /

