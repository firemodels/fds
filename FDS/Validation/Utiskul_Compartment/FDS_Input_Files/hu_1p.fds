&HEAD CHID='hu_1p',TITLE='Hu et al. IAFSS (2005) compartment fire Case 1 prescribed mass loss rate'/

&MESH IJK=36,40,40, XB=-0.2,0.16,-0.2,0.2,0.0,0.4/ main box

&MESH IJK=50,40,50, XB=.16,.21,-.2,.2,0.35,.40/ top inside
&MESH IJK=50,40,50, XB=.16,.21,-.2,.2,0.00,.05/ bot inside
&MESH IJK= 5,40,30, XB=.16,.21,-.2,.2,0.05,.35/ mid inside  (min 4 cells in a given direction for Poisson solver)

&TIME T_END=350./

&DUMP DT_DEVC=1./

&MISC CHECK_VN=.TRUE./

&REAC FUEL       = 'N-HEPTANE'
      SOOT_YIELD = 0.037/

&OBST XB=0.20,0.21,-0.2,0.2,0.03,0.37,SURF_ID='INSULATION'/

&VENT XB=0.21,0.21,-0.2,0.2,0.37,0.4,ID='INLET1',SURF_ID='OPEN',COLOR='BLUE'/
&VENT XB=0.21,0.21,-0.2,0.2,0.0,0.03,ID='INLET2',SURF_ID='OPEN',COLOR='GREEN'/

&VENT XB=-0.0475,0.0475,-0.0475,0.0475,0.00,0.00, XYZ=0,0,0, RADIUS=0.0475, SPREAD_RATE=1000., SURF_ID='FIRE', COLOR='RED'/

&SURF ID='FIRE', SPEC_ID(1)='N-HEPTANE', MASS_FLUX(1)=0.02, RAMP_MF(1)='case1'/

&RAMP ID='case1' T=0, F=0.0/
&RAMP ID='case1' T=10, F=0.6/
&RAMP ID='case1' T=70, F=1.0/
&RAMP ID='case1' T=250, F=1.0/
&RAMP ID='case1' T=280, F=0.75/
&RAMP ID='case1' T=350, F=0.0/

&SURF ID        = 'INSULATION'
      MATL_ID   = 'CERAMIC FIBER'
      COLOR     = 'GRAY'
      THICKNESS = 0.020
      BACKING   = 'EXPOSED'
      DEFAULT   = .TRUE. /

&MATL ID                 = 'CERAMIC FIBER'
      CONDUCTIVITY_RAMP  = 'k_fiber'
      SPECIFIC_HEAT      = 1.04
      DENSITY            =  128.
      EMISSIVITY         = 0.97 /
&RAMP ID='k_fiber',T= 20.,F=0.09 /
&RAMP ID='k_fiber',T=300.,F=0.09 /
&RAMP ID='k_fiber',T=600.,F=0.17 /
&RAMP ID='k_fiber',T=900.,F=0.25 /

&SLCF PBY=0., QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE./
&SLCF PBY=0., QUANTITY='VELOCITY', VECTOR=.TRUE./

&BNDF QUANTITY='YPLUS'/

Flow at vent

&DEVC XB=0.205,0.205,-0.2,0.2,0.37,0.4, QUANTITY='MASS FLOW', ID='M1'/
&DEVC XB=0.205,0.205,-0.2,0.2,0.0,0.03, QUANTITY='MASS FLOW', ID='M2'/

