&HEAD CHID='NIST_He_3600_UC_ULV',TITLE='Helium release experiment 3600 seconds, Upper Center Burner, Upper Lower Vents'/

&MESH IJK=64,60,30, XB=-0.1, 1.50, 0.0, 1.50, 0.0, 0.75 /

&TIME T_END=32460./ 3600 sec. release and an equilibrium phase
&MISC TMPA=21. /
&SPEC ID='HELIUM'/

&MATL ID = 'PLEXIGLASS',
   CONDUCTIVITY = 0.48, 
   SPECIFIC_HEAT = 0.84, 
   DENSITY = 1440./

&SURF ID = 'PLEXIGLASS', 
   DEFAULT=.TRUE., 
   MATL_ID='PLEXIGLASS', 
   THICKNESS = 0.013 /

&OBST XB= 0.0,0.0,0.0,1.5,0.0,0.75, SURF_ID='PLEXIGLASS'/ Front Wall
&HOLE XB=-0.1,0.1,0.75,0.7715,0.0254,0.0469 / HOLE SIZE 2.15 cm x 2.15 cm, nominal
&HOLE XB=-0.1,0.1,0.75,0.7715,0.7031,0.7246 / HOLE SIZE 2.15 cm x 2.15 cm, nominal


&VENT XB=-0.1, -0.1, 0.0, 1.50, 0.0, 0.75, SURF_ID='OPEN'/ Open side for venting


&SURF ID='HELIUM', SPEC_ID(1)='HELIUM', MASS_FLUX(1)=0.040, RAMP_MF(1)='LEAKRAMP'/ Vent Area =  .001024 sq m, Flow rate = 14.92 lpm, Density = 0.166 kg/m3 at 294 K
(14.92 l/min *1/60 min/sec *.001 m3/L *.166 kg/m3  * 1/.001024 m2= .040 kg/m2/s)
&RAMP ID='LEAKRAMP',T=0.0,F=0.0 / 
&RAMP ID='LEAKRAMP',T=0.1,F=1.0 /
&RAMP ID='LEAKRAMP',T=3600.0,F=1.0 /
&RAMP ID='LEAKRAMP',T=3600.1,F=0.0 /

&OBST XB=0.734,0.766,0.734,0.766,0.0,0.725,COLOR='RED' / Fischer Burner, center upper location
&VENT XB=0.734,0.766,0.734,0.766,0.725,0.725, SURF_ID='HELIUM', COLOR='BLUE' / Helium Vent, center upper location

&SLCF PBX = 0.375, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',VECTOR=.TRUE./

&DEVC XYZ=0.375,0.375,0.093, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #1' /
&DEVC XYZ=0.375,0.375,0.185, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #2' /
&DEVC XYZ=0.375,0.375,0.276, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #3' /
&DEVC XYZ=0.375,0.375,0.372, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #4' /
&DEVC XYZ=0.375,0.375,0.466, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #5' /
&DEVC XYZ=0.375,0.375,0.559, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #6' /
&DEVC XYZ=0.375,0.375,0.650, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #7' /

&TAIL /
