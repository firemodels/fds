&HEAD CHID='bucket_test_1', TITLE='Single Sprinkler Test Case' /

&MESH IJK=50,50,25, XB=-5.0,5.0,-5.0,5.0,0.0,5.0 /
&RADI RADIATION=.FALSE./
&TIME DT=0.05, T_END=40. /
&SPEC ID='WATER VAPOR'/
&PART ID='water drops', SPEC_ID='WATER VAPOR', QUANTITIES(1:3)='PARTICLE DIAMETER','PARTICLE TEMPERATURE','PARTICLE AGE',
      DIAMETER=750., SAMPLING_FACTOR=1 /

&PROP ID='K-11', QUANTITY='SPRINKLER LINK TEMPERATURE', OFFSET=0.10, PART_ID='water drops', 
      FLOW_RATE=180., FLOW_RAMP='spray', PARTICLE_VELOCITY=10., SPRAY_ANGLE=30.,80., SMOKEVIEW_ID='sprinkler_upright' /
&RAMP ID='spray',T= 0., F=0. /
&RAMP ID='spray',T= 1., F=1. /
&RAMP ID='spray',T=30., F=1. /
&RAMP ID='spray',T=31., F=0. /

&DEVC XYZ=0.0,0.0,4.9, PROP_ID='K-11', ID='Spr_1', QUANTITY='TIME', SETPOINT=0.  /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /

&SLCF PBY=0.0,QUANTITY='TEMPERATURE' /
&SLCF PBY=0.0,QUANTITY='MASS FRACTION',SPEC_ID='WATER VAPOR' /
&SLCF PBY=0.0,QUANTITY='MPUV', PART_ID='water drops' /
&SLCF PBZ=0.1,QUANTITY='PARTICLE FLUX Z', PART_ID='water drops' /
&SLCF PBZ=2.5,QUANTITY='PARTICLE FLUX Z', PART_ID='water drops' /

&DEVC ID='Mass',  XB=-5.0,5.0,-5.0,5.0,0.0,0.1, QUANTITY='AMPUA', PART_ID='water drops', STATISTICS='SURFACE INTEGRAL'  /

&TAIL /
