&HEAD CHID='HVAC_mass_transport_1',TITLE='HVAC mass transport, single duct' /

&MESH IJK=21,20,20, XB=0.,1.05,0.,1.,0.,1./

&MISC STRATIFICATION=.FALSE.,HVAC_MASS_TRANSPORT=.TRUE.,TAU_DEFAULT=0. /
&RADI RADIATION=.FALSE./
&TIME T_END=20. /

&OBST XB=0.5,0.55,0.,1.,0.,1./

&ZONE XB=0.,0.5,0.,1.,0.,1./
&ZONE XB=0.55,1.05,0.,1.,0.,1./

&SPEC ID='SPEC1',SPECIFIC_HEAT=1.,BACKGROUND=.TRUE.,MW=24.055144069133224/
&SPEC ID='SPEC2',SPECIFIC_HEAT=1.,MW=24.055144069133224/

&VENT MB='XMIN',SURF_ID='OPEN'/
&VENT MB='XMAX',SURF_ID='OPEN'/

&INIT XB=0,0.5,0.,1.,0.,1.,SPEC_ID='SPEC1',DENSITY=1.0/
&INIT XB=0.55,1.05,0.,1.,0.,1.,SPEC_ID='SPEC2',MASS_FRACTION(1)=1.,DENSITY=1.0/

&VENT XB=0.5,0.5,0.45,0.55,0.45,0.55,ID='INLET1',SURF_ID='HVAC',COLOR='RED'/
