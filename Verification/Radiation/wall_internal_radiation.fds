&MESH IJK = 5,3,4, XB = 0.0,0.5,-0.15,0.15,0.0,0.4 /

&TIME T_END=0.0001, WALL_INCREMENT = 1, DT = 0.0001/
&MISC TMPA = -272.15, Y_CO2_INFTY=0.,HUMIDITY=0./

&SURF ID='TAU_0.01'
      MATL_ID = 'MAT_0.1'
      STRETCH_FACTOR = 1.0
      THICKNESS=0.1
      HEAT_TRANSFER_COEFFICIENT=0.0
      TMP_INNER = 1000. /
&SURF ID='TAU_0.1'
      MATL_ID = 'MAT_1.0'
      STRETCH_FACTOR = 1.0
