&HEAD CHID='vis_test2', TITLE='Sandia 1m high flow rate methane pool fire (Test 17), 6 cm resolution SVN $Revision$' /

&MULT ID='mesh array', DX=1.5,DY=1.5,DZ=1.0, I_UPPER=1,J_UPPER=1,K_UPPER=3 /
&MESH IJK=96,96,128, XB=-1.5,1.5,-1.5,1.5,-.0625,3.9375/

&DUMP NFRAMES=100 DT_SL3D=0.033333 /

&TIME T_END=10. /
&MISC TMPA=5.
      P_INF=81100./

&REAC FUEL='METHANE'
      SOOT_YIELD=0.01
      HEAT_OF_COMBUSTION=50350.
      CRITICAL_FLAME_TEMPERATURE=1207/

&SURF ID='POOL',MASS_FLUX(1)=0.066,SPEC_ID(1)='METHANE',TMP_FRONT=1. /
&SURF ID='PLATE',COLOR='GRAY',DEFAULT=.TRUE. /

&VENT XB=-0.5,0.5,-0.5,0.5,-.0625,-.0625,COLOR='BLUE',SURF_ID='POOL'/

&VENT XB=-1.5,-.5,-1.5,1.5,-.0625,-.0625,SURF_ID='OPEN'/
&VENT XB=.5,1.5,-1.5,1.5,-.0625,-.0625,SURF_ID='OPEN'/
&VENT XB=-1.5,1.5,-1.5,-.5,-.0625,-.0625,SURF_ID='OPEN'/
&VENT XB=-1.5,1.5,.5,1.5,-.0625,-.0625,SURF_ID='OPEN'/

&VENT MB='ZMAX',SURF_ID='OPEN'/
&VENT MB='YMIN',SURF_ID='OPEN'/
&VENT MB='YMAX',SURF_ID='OPEN'/
&VENT MB='XMIN',SURF_ID='OPEN'/
&VENT MB='XMAX',SURF_ID='OPEN'/

&SLCF PBY=-.0625, QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBY=-.0625, QUANTITY='DENSITY' /
&SLCF PBY=-.0625, QUANTITY='TEMPERATURE' /
&SLCF PBY=-.0625, QUANTITY='HRRPUV' /
&SLCF PBY=-.0625, QUANTITY='MASS FRACTION', SPEC_ID='METHANE' /
&SLCF PBY=-.0625, QUANTITY='TURBULENCE RESOLUTION' /
&SLCF PBY=-.0625, QUANTITY='WAVELET ERROR', QUANTITY2='MASS FRACTION', SPEC_ID='METHANE' /
&SLCF XB=-1.5,1.5,-1.5,1.5,-0.0625,3.9375, QUANTITY='TEMPERATURE' / 3D slice
&SLCF XB=-1.5,1.5,-1.5,1.5,-0.0625,3.9375, QUANTITY='DENSITY',SPEC_ID='SOOT' / 3D slice
&SLCF XB=-1.5,1.5,-1.5,1.5,-0.0625,3.9375, QUANTITY='SCALAR RESOLUTION', QUANTITY2='TEMPERATURE' /

Tieszen et al., Comb. and Flame (2002), Figure 6
&DEVC XYZ = 0.0,0.0,0.505, QUANTITY = 'W-VELOCITY', ID='W005'/
&DEVC XYZ = 0.5,0.0,0.505, QUANTITY = 'W-VELOCITY', ID='W505'/
&DEVC XYZ = 0.0,0.0,2.0, QUANTITY = 'W-VELOCITY', ID='W0020'/
&DEVC XYZ = 0.5,0.0,2.0, QUANTITY = 'W-VELOCITY', ID='W5020'/


&TAIL /
