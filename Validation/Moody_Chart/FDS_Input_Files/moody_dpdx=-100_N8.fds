&HEAD CHID='moody_dpdx=-100_N8', TITLE='LES of flow through a square channel' /

&MESH IJK=64,8,8, XB=0,8,-.5,.5,0,1 /

&TIME T_END=20.0 /

&WIND FORCE_VECTOR(1)=100.,STRATIFICATION=.FALSE./
&DUMP NFRAMES=100, SIG_FIGS=4, SIG_FIGS_EXP=2 /

&VENT MB='XMIN', SURF_ID='PERIODIC' /
&VENT MB='XMAX', SURF_ID='PERIODIC' /

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBX=4, QUANTITY='VELOCITY', VECTOR=.TRUE. /

&SLCF PBY=0, QUANTITY='H',         CELL_CENTERED=.TRUE. /
&SLCF PBY=0, QUANTITY='DENSITY',   CELL_CENTERED=.TRUE. /
&SLCF PBY=0, QUANTITY='VISCOSITY', CELL_CENTERED=.TRUE. /

&DEVC XB=4.0,4.0, -0.5,0.5, 0.0,1.0, QUANTITY='U-VELOCITY', SPATIAL_STATISTIC='AREA INTEGRAL' /
&DEVC XYZ=4.0,0.0,0.5, QUANTITY='U-VELOCITY' /
&DEVC XYZ=4.0,0.0,0.5, QUANTITY='VISCOSITY' /
&DEVC XYZ=4.0,0.0,0.5, QUANTITY='DENSITY' /

&TAIL /
