      QUANTITY='TIME'
      SETPOINT=10.0
      PROP_ID='p-ROSIN-RAMMLER'
      ID='n-ROSIN-RAMMLER'
      INITIAL_STATE=T /

! Sprinkler properties

&PROP ID='p-USER'
      OFFSET=0.08
      PART_ID='USER'
      FLOW_RATE=0.347
      PARTICLE_VELOCITY=.05
      PARTICLES_PER_SECOND=500
      SPRAY_ANGLE=0.0 38 /

