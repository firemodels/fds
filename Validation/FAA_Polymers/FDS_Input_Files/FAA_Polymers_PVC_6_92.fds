&HEAD CHID='FAA_Polymers_PVC_6_92', TITLE='PVC at 75 kW/m2, 3 mm thick' /

&MESH IJK=20,20,50, XB=-0.10,0.10,-0.10,0.10,-0.03,0.47 /  

&TIME T_END=800. / 

&REAC FUEL='PVC GAS', C=2., H=3., N=2., SOOT_YIELD=0.02 / 

&MATL ID='PVC'
      ABSORPTION_COEFFICIENT=2145.
      N_REACTIONS=1
      A(1) = 1.4E33
      E(1) = 3.67E5
      EMISSIVITY=0.90
      DENSITY=1430.
      SPEC_ID='PVC GAS'
      NU_SPEC=0.56
      MATL_ID='CHAR 1'
      NU_MATL=0.44
      HEAT_OF_REACTION=170.
      HEAT_OF_COMBUSTION=2025.
      CONDUCTIVITY = 0.17
      SPECIFIC_HEAT = 1.55 /

&MATL ID='CHAR 1'
      ABSORPTION_COEFFICIENT=2453.
      N_REACTIONS=1
      A(1) = 3.5E12
      E(1) = 2.07E5
      EMISSIVITY=0.90
      DENSITY=629.
      SPEC_ID='PVC GAS'
      NU_SPEC=0.53
      MATL_ID='CHAR 2'
      NU_MATL=0.47
      HEAT_OF_REACTION=1200.
      HEAT_OF_COMBUSTION=27375.
      CONDUCTIVITY = 0.17
      SPECIFIC_HEAT = 1.55 /

&MATL ID='CHAR 2'
      ABSORPTION_COEFFICIENT=29600.
      EMISSIVITY=0.85
      DENSITY=296.
      CONDUCTIVITY=0.26
      SPECIFIC_HEAT=1.72 /

&MATL ID='Kaowool'
      CONDUCTIVITY = 0.08
      SPECIFIC_HEAT = 0.80
      DENSITY = 48. /

&SURF ID='SAMPLE'
      COLOR='BLACK'
      MATL_ID='PVC','Kaowool'
      THICKNESS=0.006,0.015
      STRETCH_FACTOR=1.
      EXTERNAL_FLUX=92. / 

&OBST XB=-0.05,0.05,-0.05,0.05,-0.02,0.00, SURF_IDS='SAMPLE','INERT','INERT' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMIN', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DUMP DT_DEVC=2., SIG_FIGS=4 /

&DEVC XB=-0.10,0.10,-0.10,0.10,-0.03,0.47, QUANTITY='HRRPUV', ID='HRR', SPATIAL_STATISTIC='VOLUME INTEGRAL', CONVERSION_FACTOR=100. /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='WALL TEMPERATURE', ID='temp' /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='BURNING RATE',     ID='MLR' /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='WALL THICKNESS',   ID='thick' /
&DEVC XYZ=0.0,0.0,0.0, IOR=3, QUANTITY='GAUGE HEAT FLUX',  ID='flux' /

&SLCF PBY=0., QUANTITY='TEMPERATURE' /
&SLCF PBY=0., QUANTITY='HRRPUV' /
&SLCF PBY=0., QUANTITY='MASS FRACTION', SPEC_ID='PVC GAS' /

&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='GAUGE HEAT FLUX' /
&BNDF QUANTITY='BURNING RATE' /

&TAIL / 


