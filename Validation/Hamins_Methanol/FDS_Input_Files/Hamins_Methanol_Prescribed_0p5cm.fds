&HEAD CHID='Hamins_Methanol_Prescribed_0p5cm', TITLE='NIST 30 cm Methanol Pool Fire'

&MESH XB=-0.32,-0.16,-0.32,-0.16,0.0,0.16,IJK=32,32,32, MULT_ID='ARRAY'/
&MULT ID='ARRAY', DX=0.16,DY=0.16,DZ=0.16,I_UPPER=3,J_UPPER=3,K_UPPER=7/ 128 meshes

&TIME T_END=15/

&DUMP DT_DEVC_LINE=10/

&MISC FLUX_LIMITER=4, RESEARCH_MODE=T, SUPPRESSION=F/

&REAC FUEL               = 'METHANOL'
      FYI                = 'Methanol CH3OH'
      CO_YIELD           = 0.001
      SOOT_YIELD         = 0.001
      RADIATIVE_FRACTION = 0.17 /

! 0.92e-3 kg/s / (pi*.15^2) = 0.013015 kg/m^2s (A. Hamins personal communication)
! 64.7 C is boiling point of methanol at 1 atm
&SURF ID='POOL'
      SPEC_ID='METHANOL'
      MASS_FLUX = 0.013015
      TMP_FRONT=64.7
      FREE_SLIP=T/

&VENT XB=-0.15,0.15,-0.15,0.15,0,0, XYZ=0,0,0.0001, RADIUS=0.15, COLOR='GRAY', SURF_ID='POOL', SPREAD_RATE=1000/

&VENT MB='ZMIN',SURF_ID='OPEN'/
&VENT MB='ZMAX',SURF_ID='OPEN'/
&VENT MB='YMIN',SURF_ID='OPEN'/
&VENT MB='YMAX',SURF_ID='OPEN'/
&VENT MB='XMIN',SURF_ID='OPEN'/
&VENT MB='XMAX',SURF_ID='OPEN'/

&DEVC XB=0,0.15,0.0,0.0,0.038,0.038, QUANTITY='TEMPERATURE', ID='T-03', POINTS=15, X_ID='r' /
&DEVC XB=0,0.15,0.0,0.0,0.308,0.308, QUANTITY='TEMPERATURE', ID='T-30', POINTS=15, HIDE_COORDINATES=.TRUE. /
&DEVC XB=0,0.15,0.0,0.0,0.608,0.608, QUANTITY='TEMPERATURE', ID='T-60', POINTS=15, HIDE_COORDINATES=.TRUE. /

&DEVC XB=0,0,0,0,0,0.7, QUANTITY='TEMPERATURE', ID='T-CL', POINTS=70 /

&DEVC XB=0,0.32,0.0,0.0,0.0,0.0, QUANTITY='INCIDENT HEAT FLUX', ID='QPPRAD', POINTS=32, IOR=+3 /

&SLCF PBY=0.0001, QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBY=0.0001, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.0001, QUANTITY='MASS FRACTION', SPEC_ID='METHANOL', CELL_CENTERED=.TRUE. /
&SLCF PBY=0.0001, QUANTITY='EXTINCTION', CELL_CENTERED=.TRUE. /

&TAIL/