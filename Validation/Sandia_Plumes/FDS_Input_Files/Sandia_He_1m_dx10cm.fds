&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, QUANTITY='MASS FRACTION', SPEC_ID='PLUME FLUID MIXTURE', ID='YMIXp4', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, QUANTITY='MASS FRACTION', SPEC_ID='PLUME FLUID MIXTURE', ID='YMIXp6', POINTS=16, HIDE_COORDINATES=.TRUE. /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, QUANTITY='CELL W', ID='Wp2_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, QUANTITY='CELL W', ID='Wp4_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, QUANTITY='CELL W', ID='Wp6_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, QUANTITY='CELL U', ID='Up2_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, QUANTITY='CELL U', ID='Up4_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, QUANTITY='CELL U', ID='Up6_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, QUANTITY='MASS FRACTION', SPEC_ID='HELIUM', ID='YHEp2_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, QUANTITY='MASS FRACTION', SPEC_ID='HELIUM', ID='YHEp4_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, QUANTITY='MASS FRACTION', SPEC_ID='HELIUM', ID='YHEp6_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, QUANTITY='MASS FRACTION', SPEC_ID='PLUME FLUID MIXTURE', ID='YMIXp2_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, QUANTITY='MASS FRACTION', SPEC_ID='PLUME FLUID MIXTURE', ID='YMIXp4_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, QUANTITY='MASS FRACTION', SPEC_ID='PLUME FLUID MIXTURE', ID='YMIXp6_rms', STATISTICS='RMS', POINTS=16, HIDE_COORDINATES=.TRUE. /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, QUANTITY='TOTAL MASS FLUX Z', SPEC_ID='PLUME FLUID MIXTURE', ID='MFLUXZp2', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, QUANTITY='TOTAL MASS FLUX Z', SPEC_ID='PLUME FLUID MIXTURE', ID='MFLUXZp4', POINTS=16, HIDE_COORDINATES=.TRUE. /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, QUANTITY='TOTAL MASS FLUX Z', SPEC_ID='PLUME FLUID MIXTURE', ID='MFLUXZp6', POINTS=16, HIDE_COORDINATES=.TRUE. /

&TAIL /
