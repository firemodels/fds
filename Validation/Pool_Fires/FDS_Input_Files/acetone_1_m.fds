&HEAD CHID='acetone_1_m', TITLE='Single pan of burning acetone' /

&MESH IJK=32,32,96, XB=-0.8,0.8,-0.8,0.8,0.0,4.8 /

&MISC NEW_EVAP=.TRUE. /

&TIME T_END=300. / 

&REAC FUEL               = 'ACETONE VAPOR'
      FYI                = 'Acetone, C_3 H_6 O_1'
      HEAT_OF_COMBUSTION = 30509.64
      O                  = 1.
      C                  = 3.
      H                  = 6.
      SOOT_YIELD         = 0.014 /

&MATL ID                     = 'ACETONE LIQUID'
      EMISSIVITY             = 1.
      NU_SPEC                = 1.
      SPEC_ID                = 'ACETONE VAPOR'
      HEAT_OF_REACTION       = 501.03
      CONDUCTIVITY           = 0.18
      SPECIFIC_HEAT          = 1.6529
      DENSITY                = 792.5
      ABSORPTION_COEFFICIENT = 40.
      BOILING_TEMPERATURE    = 56.3 /
	  
&MATL ID            = 'STEEL'
      EMISSIVITY    = 1.0
      DENSITY       = 7850.
      CONDUCTIVITY  = 45.8
      SPECIFIC_HEAT = 0.46 /

&SURF ID        = 'ACETONE POOL'
      COLOR     = 'YELLOW'
      MATL_ID   = 'ACETONE LIQUID'
      THICKNESS = 0.1 /

&SURF ID        = 'STEEL SHEET'
      COLOR     = 'BLACK'
      MATL_ID   = 'STEEL'
      BACKING   = 'EXPOSED'
      THICKNESS = 0.003 /

&VENT MB='XMIN', SURF_ID = 'OPEN' /
&VENT MB='XMAX', SURF_ID = 'OPEN' /
&VENT MB='YMIN', SURF_ID = 'OPEN' /
&VENT MB='YMAX', SURF_ID = 'OPEN' /
&VENT MB='ZMAX', SURF_ID = 'OPEN' /

&OBST XB=-.50,0.50,-.50,0.50,0.00,0.05, SURF_IDS='ACETONE POOL','STEEL SHEET','STEEL SHEET' /
&OBST XB=-.50,-.50,-.50,0.50,0.00,0.10, SURF_ID='STEEL SHEET' /
&OBST XB=0.50,0.50,-.50,0.50,0.00,0.10, SURF_ID='STEEL SHEET' /
&OBST XB=-.50,0.50,-.50,-.50,0.00,0.10, SURF_ID='STEEL SHEET' /
&OBST XB=-.50,0.50,0.50,0.50,0.00,0.10, SURF_ID='STEEL SHEET' /

&DUMP DT_HRR=1., SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SLCF PBY=0.0, QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
&SLCF PBY=0.0, QUANTITY='HRRPUV' /

&BNDF QUANTITY='BURNING RATE' /
&BNDF QUANTITY='GAUGE HEAT FLUX' /
&BNDF QUANTITY='WALL TEMPERATURE' /

&TAIL /


