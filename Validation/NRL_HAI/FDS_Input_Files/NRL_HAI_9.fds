&VENT MB='XMIN', SURF_ID='GYPSUM BOARD' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMIN', SURF_ID='FLOOR' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DEVC ID='Tot_Flux', QUANTITY='GAUGE HEAT FLUX', XB=0.0,0.0,0.0,0.0,0.02,7.00, POINTS=93, IOR=1 /

&DEVC ID='Tot_Flux 1m' QUANTITY = 'GAUGE HEAT FLUX' XYZ = 0,0,1, IOR=1 /
&DEVC ID='Wall temp 1m' QUANTITY = 'WALL TEMPERATURE' XYZ = 0,0,1, IOR=1 /
&DEVC ID='Conv 1m' QUANTITY = 'CONVECTIVE HEAT FLUX' XYZ = 0,0,1, IOR=1 /
&DEVC ID='Rad 1m' QUANTITY = 'RADIATIVE HEAT FLUX' XYZ = 0,0,1, IOR=1 /

&SLCF PBY=0.0,QUANTITY='TEMPERATURE',VECTOR=.TRUE. /
&SLCF PBY=0.0,QUANTITY='HRRPUV'  /
&SLCF PBY=0.0,QUANTITY='INTEGRATED INTENSITY'  /
&SLCF PBY=0.0,QUANTITY='ABSORPTION COEFFICIENT'  /
