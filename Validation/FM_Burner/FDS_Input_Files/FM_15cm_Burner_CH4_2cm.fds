&HEAD CHID='FM_15cm_Burner_CH4_2cm', TITLE='LOI test for FM 15 cm burner'/

&MULT ID='m1', DX=0.60,DY=0.60,DZ=0.60,
      I_UPPER=1,J_UPPER=1,K_UPPER=2 /  12 meshes
&MESH IJK=30,30,30, XB=-0.60,-0.00,-0.60,-0.00,-0.370,0.230, MULT_ID='m1' /

&TIME T_END=65/

&DUMP DT_DEVC=1, DT_HRR=1 /

&MISC SIMULATION_MODE='LES' /

&SPEC ID='METHANE'/
&SPEC ID='ETHYLENE'/
&SPEC ID='NITROGEN'/
&SPEC ID='OXYGEN', LUMPED_COMPONENT_ONLY=T/
&SPEC ID='CARBON DIOXIDE', LUMPED_COMPONENT_ONLY=T/
&SPEC ID='WATER VAPOR', LUMPED_COMPONENT_ONLY=T/
&SPEC ID='CARBON MONOXIDE', LUMPED_COMPONENT_ONLY=T/
&SPEC ID='SOOT', LUMPED_COMPONENT_ONLY=T/

&SPEC ID='AIR', SPEC_ID(1)='NITROGEN',       VOLUME_FRACTION(1)=7.825727E-01
                SPEC_ID(2)='OXYGEN',         VOLUME_FRACTION(2)=2.075588E-01
                SPEC_ID(3)='CARBON DIOXIDE', VOLUME_FRACTION(3)=3.864118E-04
                SPEC_ID(4)='WATER VAPOR',    VOLUME_FRACTION(4)=9.482037E-03
                BACKGROUND=T/

&SPEC ID='METH PROD', SPEC_ID(1)='NITROGEN',       VOLUME_FRACTION(1)=7.089938E-01
                      SPEC_ID(2)='CARBON DIOXIDE', VOLUME_FRACTION(2)=9.437196E-02
                      SPEC_ID(3)='WATER VAPOR',    VOLUME_FRACTION(3)=1.966343E-01/

&SPEC ID='INT PROD', SPEC_ID(1)='NITROGEN',        VOLUME_FRACTION(1)=6.480618E-01
                     SPEC_ID(2)='CARBON DIOXIDE',  VOLUME_FRACTION(2)=3.199942E-04
                     SPEC_ID(3)='WATER VAPOR',     VOLUME_FRACTION(3)=2.370296E-01
                     SPEC_ID(4)='CARBON MONOXIDE', VOLUME_FRACTION(4)=1.145887E-01/

&SPEC ID='ETH PROD', SPEC_ID(1)='NITROGEN',        VOLUME_FRACTION(1)=7.242772E-01
                     SPEC_ID(2)='CARBON DIOXIDE',  VOLUME_FRACTION(2)=1.256631E-01
                     SPEC_ID(3)='WATER VAPOR',     VOLUME_FRACTION(3)=1.414929E-01
                     SPEC_ID(4)='CARBON MONOXIDE', VOLUME_FRACTION(4)=8.664945E-04
                     SPEC_ID(5)='SOOT',            VOLUME_FRACTION(5)=7.700307E-03/

&REAC FUEL='ETHYLENE'
      SPEC_ID_NU = 'ETHYLENE','AIR','ETH PROD'
      NU=-1,-13.906722,15.026044
      PRIORITY=1
      AUTO_IGNITION_TEMPERATURE=-273
      CRITICAL_FLAME_TEMPERATURE=1337
      RADIATIVE_FRACTION=0.34 /

&REAC FUEL='METHANE'
      SPEC_ID_NU = 'METHANE','AIR','INT PROD'
      NU=-1,-7.226867,8.726867
      PRIORITY=2
      AUTO_IGNITION_TEMPERATURE=600
      CRITICAL_FLAME_TEMPERATURE=1507 /

&REAC FUEL='INT PROD'
      SPEC_ID_NU = 'INT PROD','AIR','METH PROD'
      NU=-1,-0.276039,1.218745
      PRIORITY=3
      AUTO_IGNITION_TEMPERATURE=600
      CRITICAL_FLAME_TEMPERATURE=1507 /

&SURF ID='side wall', COLOR='LIGHT BLUE', TRANSPARENCY=0.5, DEFAULT=T/
&SURF ID='wall', COLOR='GRAY'/
https://material-properties.org/sand-density-heat-capacity-thermal-conductivity/
&MATL ID='sand', DENSITY=1500, CONDUCTIVITY=0.25, SPECIFIC_HEAT=0.83/
&SURF ID='burner', MASS_FLUX(1)=0.011019529971700, SPEC_ID(1)='METHANE', COLOR='RED'
      HEAT_TRANSFER_COEFFICIENT_BACK=1000., TMP_BACK=25., THICKNESS=0.0254, MATL_ID='sand' / D* = 0.152 m (10 kW), HRRPUA=551.1 kW/m2
&SURF ID='coflow', COLOR='BLUE',
      SPEC_ID(1)='AIR',      MASS_FLUX(1)=0.223, RAMP_MF(1)='O2 RAMP'
      SPEC_ID(2)='NITROGEN', MASS_FLUX(2)=0.360, RAMP_MF(2)='N2 RAMP' / 21 to 8 vol % O2

&RAMP ID='O2 RAMP', T= 0.0, F=1.00 /
&RAMP ID='O2 RAMP', T=65.0, F=1.00 /

&RAMP ID='N2 RAMP', T= 0.0, F=0.00 /
&RAMP ID='N2 RAMP', T= 5.0, F=0.00 /
&RAMP ID='N2 RAMP', T=65.0, F=1.00 /

&MULT ID='cube array', DX=0.02,DY=0.02, I_UPPER=9,J_UPPER=9 /

&OBST XB=-0.10,-0.08,-0.10,-0.08,-0.254,0.0000
      MULT_ID='cube array'
      SURF_IDS='burner','wall','wall'
      SHAPE='CYLINDER'
      RADIUS=0.076
      HEIGHT=0.254
      XYZ=0,0,-0.254 /

&PART ID='ignitor', SAMPLING_FACTOR=1, SURF_ID='pilot', STATIC=.TRUE./
&SURF ID='pilot', MASS_FLUX(1)=0.002196663186903, SPEC_ID(1)='ETHYLENE', RADIUS=0.0047, GEOMETRY='SPHERICAL' / HRRPUA=100. kW/m2
&INIT PART_ID='ignitor', XYZ=0.0,0.0,0.01, RADIUS=0.08, SHAPE='RING', N_PARTICLES=36, UNIFORM=T/

&VENT DB='ZMAX', SURF_ID='OPEN'/
&VENT PBZ=-0.370, SURF_ID='coflow'/

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0, QUANTITY='TEMPERATURE', CELL_CENTERED=T/
&SLCF PBY=0, QUANTITY='MASS FRACTION', SPEC_ID='METHANE' CELL_CENTERED=T/
&SLCF PBY=0, QUANTITY='MASS FRACTION', SPEC_ID='OXYGEN' CELL_CENTERED=T/
&SLCF PBY=0, QUANTITY='MASS FRACTION', SPEC_ID='CARBON MONOXIDE' CELL_CENTERED=T/
&SLCF PBY=0, QUANTITY='MASS FRACTION', SPEC_ID='SOOT' CELL_CENTERED=T/
&SLCF PBY=0, QUANTITY='AUTO IGNITION TEMPERATURE', CELL_CENTERED=T/
&SLCF PBY=0, QUANTITY='HRRPUV', CELL_CENTERED=T/

&DEVC ID='XO2', XYZ=0.152,0,-.149, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN', TIME_AVERAGED=F/
&DEVC ID='TBURNER', XYZ=0,0,0.001, IOR=3, QUANTITY='WALL TEMPERATURE'/

&TAIL /
