      C=1.
      H=4.
      SOOT_YIELD=0.01 /

&MATL ID            = 'GYPSUM'
      FYI           = 'NBSIR 88-3752'
      CONDUCTIVITY  = 0.17
      SPECIFIC_HEAT = 1.09
      DENSITY       =  930. /

&MATL ID            = 'CONCRETE' 
