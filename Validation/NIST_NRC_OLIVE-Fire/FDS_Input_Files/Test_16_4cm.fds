&TIME T_END=900., TIME_SHRINK_FACTOR=1 /

&MATL ID            = 'STEEL'
      SPECIFIC_HEAT = 0.465
      CONDUCTIVITY  = 54.
      DENSITY       = 7833. /

&SURF ID            = 'STEEL PLATE'
      MATL_ID       = 'STEEL'
      DEFAULT       = .TRUE.
      THICKNESS     = 0.003
      COLOR         = 'GRAY 40' /

&PRES VELOCITY_TOLERANCE=0.002, MAX_PRESSURE_ITERATIONS=50 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /


&REAC FUEL='METHANE', RADIATIVE_FRACTION=0., N_SIMPLE_CHEMISTRY_REACTIONS=2, AUTO_IGNITION_TEMPERATURE=540.   AIT_EXCLUSION_ZONE=-0.16, 0.16, 0.20, 0.52,0.50,0.56 /

&SURF ID='FIRE', HRRPUA=488.3, COLOR='ORANGE', TMP_FRONT=300., RAMP_Q='fire ramp' /
