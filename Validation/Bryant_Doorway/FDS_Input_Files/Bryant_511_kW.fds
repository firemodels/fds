&HEAD CHID='Bryant_511_kW', TITLE='Rodney Bryant Velocity Measurements in a Doorway' /

&MESH IJK=100,48,54, XB=-1.25,3.75,-1.20,1.20,-0.3,2.40 /

&TIME T_END=1800., TIME_SHRINK_FACTOR=10. /

&MISC TMPA=21.5 /

&REAC FUEL='METHANE'                                            
      FYI='Methane, C H_4'                                    
      SOOT_YIELD=0.001 /                 



&SURF ID='BURNER', HRRPUA=5678., COLOR='ORANGE' /

&MATL ID            = 'GYPSUM'
      CONDUCTIVITY  = 0.16
      SPECIFIC_HEAT = 0.9
      DENSITY       = 790. /

&MATL ID            = 'STEEL'
      CONDUCTIVITY  = 45. 
      SPECIFIC_HEAT = 0.8
      DENSITY       = 8000.  /

&SURF ID            = 'GYPSUM BOARD', DEFAULT=.TRUE.
      COLOR         = 'BEIGE'
      MATL_ID       = 'GYPSUM'
      THICKNESS     = 0.0254 /

&SURF ID            = 'STEEL PLATE'
      COLOR         = 'BLACK'
      MATL_ID       = 'STEEL'
      THICKNESS     = 0.00635 /


&OBST XB= 1.80, 2.10,-0.15, 0.15, 0.20, 0.30, SURF_IDS='BURNER','STEEL PLATE','STEEL PLATE' / 

&OBST XB=-0.00, 0.15,-1.20, 1.20, 0.00, 2.40 / Door Wall
&HOLE XB=-0.05, 0.20,-0.40, 0.40, 0.00, 1.95 / Doorway

&OBST XB= 0.00, 3.75,-1.20, 1.20,-0.30, 0.00 / Floor

&OBST XB=-0.15, 0.00,-0.40,-0.40, 0.00, 1.95 / Door Threshold
&OBST XB=-0.15, 0.00, 0.40, 0.40, 0.00, 1.95 / 
&OBST XB=-0.15, 0.00,-0.40, 0.40, 0.00, 0.00 / 
&OBST XB=-0.15, 0.00,-0.40, 0.40, 1.95, 1.95 / 

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT XB=-1.25, 0.00,-1.20, 1.20, 2.40, 2.40, SURF_ID='OPEN' /
&VENT XB=-1.25, 0.00,-1.20,-1.20,-0.30, 2.40, SURF_ID='OPEN' /
&VENT XB=-1.25, 0.00, 1.20, 1.20,-0.30, 2.40, SURF_ID='OPEN' /

&DUMP DT_HRR=30., DT_DEVC=30., SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SLCF PBY=0.00, QUANTITY='TEMPERATURE', VECTOR=.TRUE. /

&DEVC ID='Room_TC', XB=0.62,0.62,-.81,-.81,0.025,2.375, QUANTITY='THERMOCOUPLE',           POINTS=48, Z_ID='Room_z' /
&DEVC ID='Door_TC', XB=0.00,0.00,0.00,0.00,0.025,1.925, QUANTITY='THERMOCOUPLE',           POINTS=39, Z_ID='Door_z' /
&DEVC ID='Door_q',  XB=0.00,0.00,0.00,0.00,0.025,1.925, QUANTITY='VELOCITY', VELO_INDEX=1, POINTS=39, HIDE_COORDINATES=.TRUE.  /
&DEVC ID='Door_u',  XB=0.00,0.00,0.00,0.00,0.025,1.925, QUANTITY='U-VELOCITY',             POINTS=39, HIDE_COORDINATES=.TRUE.  /
&DEVC ID='Door_v',  XB=0.00,0.00,0.00,0.00,0.025,1.295, QUANTITY='V-VELOCITY',             POINTS=39, HIDE_COORDINATES=.TRUE.  /
&DEVC ID='Door_w',  XB=0.00,0.00,0.00,0.00,0.025,1.925, QUANTITY='W-VELOCITY',             POINTS=39, HIDE_COORDINATES=.TRUE.  /

&DEVC XB= 0.00, 0.00,-0.40, 0.40, 0.00, 1.95, QUANTITY='VOLUME FLOW',   ID='Vol_Flow' /
&DEVC XB= 0.00, 0.00,-0.40, 0.40, 0.00, 1.95, QUANTITY='MASS FLOW',     ID='Mass_Flow' /
&DEVC XB= 0.00, 0.00,-0.40, 0.40, 0.00, 1.95, QUANTITY='HEAT FLOW',     ID='Heat_Flow' /
&DEVC XB= 0.00, 0.00,-0.40, 0.40, 0.00, 1.95, QUANTITY='VOLUME FLOW +', ID='Vol_Flow_In' /
&DEVC XB= 0.00, 0.00,-0.40, 0.40, 0.00, 1.95, QUANTITY='MASS FLOW +',   ID='Mass_Flow_In' /
&DEVC XB= 0.00, 0.00,-0.40, 0.40, 0.00, 1.95, QUANTITY='HEAT FLOW +',   ID='Heat_Flow_In' /
&DEVC XB= 0.00, 0.00,-0.40, 0.40, 0.00, 1.95, QUANTITY='VOLUME FLOW -', ID='Vol_Flow_Out' /
&DEVC XB= 0.00, 0.00,-0.40, 0.40, 0.00, 1.95, QUANTITY='MASS FLOW -',   ID='Mass_Flow_Out' /
&DEVC XB= 0.00, 0.00,-0.40, 0.40, 0.00, 1.95, QUANTITY='HEAT FLOW -',   ID='Heat_Flow_Out' /

&TAIL /


