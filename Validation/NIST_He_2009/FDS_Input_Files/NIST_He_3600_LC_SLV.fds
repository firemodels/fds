&HEAD CHID='NIST_He_3600_LC_SLV',TITLE='Helium release experiment 1 h, Lower Center Burner, Single Large Vent'/

&MESH IJK=60,60,30, XB=0.0, 1.50, 0.0, 1.50, 0.0, 0.75 /

&TIME T_BEGIN=-0.1,T_END=7200. / 1 h release and an equilibrium phase
&MISC TMPA=21. /
&SPEC ID='HELIUM', MASS_EXTINCTION_COEFFICIENT=500. /

&DUMP DT_DEVC=60., DT_HRR=60., SIG_FIGS=4, SIG_FIGS_EXP=2 /

&VENT XB = 0.0,0.0,0.725,0.775,0.375,0.400,SURF_ID='HVAC',ID='TOP',COLOR='GREEN'/
&HVAC ID='TOP',TYPE_ID='NODE',DUCT_ID='TOPD',VENT_ID='TOP'/
&HVAC ID='TOPA',TYPE_ID='NODE',DUCT_ID='TOPD',AMBIENT=.TRUE.,XYZ=0.0,0.75,0.3875/
&HVAC ID='TOPD',TYPE_ID='DUCT',NODE_ID='TOP','TOPA',AREA=0.00046513, LENGTH=0.0127,LOSS=2.488,2.488/

&VENT XB = 0.0,0.0,0.725,0.775,0.350,0.375,SURF_ID='HVAC',ID='BOTTOM',COLOR='RED'/
&HVAC ID='BOTTOM',TYPE_ID='NODE',DUCT_ID='BOTTOMD',VENT_ID='BOTTOM'/
&HVAC ID='BOTTOMA',TYPE_ID='NODE',DUCT_ID='BOTTOMD',AMBIENT=.TRUE.,XYZ=0.0,0.75,0.3625/
&HVAC ID='BOTTOMD',TYPE_ID='DUCT',NODE_ID='BOTTOM','BOTTOMA',AREA=0.00046513, LENGTH=0.0127,LOSS=2.488,2.488/

&SURF ID='HELIUM', SPEC_ID(1)='HELIUM', MASS_FLUX(1)=0.040, RAMP_MF(1)='LEAKRAMP'/

Vent Area =  .001024 sq m, Flow rate = 14.92 lpm, Density = 0.166 kg/m3 at 294 K
(14.92 l/min *1/60 min/sec *.001 m3/L *.166 kg/m3  * 1/.001024 m2= .040 kg/m2/s)

&RAMP ID='LEAKRAMP',T=0.0,F=0.0 / 
&RAMP ID='LEAKRAMP',T=0.1,F=1.0 /
&RAMP ID='LEAKRAMP',T=3600.0,F=1.0 /
&RAMP ID='LEAKRAMP',T=3600.1,F=0.0 /

&OBST XB=0.734,0.766,0.734,0.766,0.0,0.21,COLOR='RED' / Fischer Burner, center lower location
&VENT XB=0.734,0.766,0.734,0.766,0.21,0.21, SURF_ID='HELIUM' / Helium Vent, center lower location

&SLCF PBY=0.75, QUANTITY='VELOCITY', VECTOR=.TRUE./
&SLCF PBY=0.75, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM', CELL_CENTERED=.TRUE./
&SLCF PBY=0.75, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE./

&DEVC XYZ=0.375,0.375,0.093, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #1' /
&DEVC XYZ=0.375,0.375,0.185, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #2' /
&DEVC XYZ=0.375,0.375,0.276, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #3' /
&DEVC XYZ=0.375,0.375,0.372, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #4' /
&DEVC XYZ=0.375,0.375,0.466, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #5' /
&DEVC XYZ=0.375,0.375,0.559, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #6' /
&DEVC XYZ=0.375,0.375,0.650, QUANTITY='VOLUME FRACTION', SPEC_ID='HELIUM',UNITS='%', CONVERSION_FACTOR=100., ID='Sensor #7' /

&DEVC QUANTITY='DUCT VELOCITY',DUCT_ID='TOPD',ID='TOPD_V'/
&DEVC QUANTITY='DUCT VELOCITY',DUCT_ID='BOTTOMD',ID='BOTTOMD_V'/

&TAIL /
