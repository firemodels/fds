&HEAD CHID='bucket_test_2', TITLE='Customized sprinkler test case' /

&MESH IJK=50,50,25, XB=-5.0,5.0,-5.0,5.0,0.0,5.0 /

&TIME T_END=15. /

&SPEC ID='WATER VAPOR'/
&PART ID='water drops', SPEC_ID='WATER VAPOR', QUANTITIES(1)='PARTICLE DIAMETER', DIAMETER=1750. /

&PROP ID='K-11', QUANTITY='SPRINKLER LINK TEMPERATURE', PARTICLE_VELOCITY=5., PART_ID='water drops', 
      FLOW_RATE=60., SPRAY_PATTERN_TABLE='TABLE1', SMOKEVIEW_ID='sprinkler_upright', PARTICLES_PER_SECOND=10000 /

&TABL ID='TABLE1',TABLE_DATA=30,31,  0,  1,5,0.2 /
&TABL ID='TABLE1',TABLE_DATA=30,31,179,180,5,0.8 /

&DEVC XYZ=0.0,0.0,4.9, PROP_ID='K-11', ID='Spr_1', QUANTITY='TIME', SETPOINT=5., INITIAL_STATE=.TRUE. /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /

&DEVC ID='Mass', QUANTITY='AMPUA', PART_ID='water drops', SPATIAL_STATISTIC='SURFACE INTEGRAL', XB=-5,5,-5,5,0,0 /
&BNDF QUANTITY='AMPUA', PART_ID='water drops' /  Accumulated Mass Per Unit Area, i.e., the Buckets

&TAIL /
