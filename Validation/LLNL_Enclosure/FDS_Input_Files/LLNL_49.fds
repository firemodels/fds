&HEAD CHID= 'LLNL_49', TITLE= '1986 LLNL Enclosure Fire Test 49'/
&MESH IJK=40,27,30, XB=0,6,0,4,0,4.5 /
&TIME T_BEGIN=-600., T_END=2000. /
&MISC SIMULATION_MODE='SVLES', P_INF=99300., TMPA=28. /
&DUMP SIG_FIGS=4, SIG_FIGS_EXP=2, DT_DEVC=60., DT_HRR=60./


&REAC FUEL='METHANE' /





&SURF ID= 'BURNER'
      COLOR= 'RED'
      RAMP_Q='fire'
      HRRPUA=1000. /

&RAMP ID='fire', T= -20., F=0. /
&RAMP ID='fire', T=   0., F=0. /
&RAMP ID='fire', T=  30., F=1. /

&OBST XB=2.7,3.2,1.8,2.2,0.3,0.5, SURF_IDS='BURNER','Duct','Duct' /

&MATL ID= 'Wall Aluminum Silicate'
   	FYI= 'Al_2 O_3 Si O_2 Refractory'
   	CONDUCTIVITY=0.39
   	DENSITY=1440.
   	SPECIFIC_HEAT=1.0 /

&MATL ID= 'Ceiling Aluminum Silicate'
	FYI= 'Al_2 O_3 Si O_2 Refractory'
	CONDUCTIVITY=0.63
	DENSITY=1920.
	SPECIFIC_HEAT=1.0 /

&MATL ID= 'Marinite'
	CONDUCTIVITY=0.12
	DENSITY=700.
	SPECIFIC_HEAT=1.12 /

&SURF ID= 'Ceiling'
	COLOR='TAN'
     MATL_ID= 'Ceiling Aluminum Silicate'
     	THICKNESS=0.1 /

&SURF ID= 'Drop Ceiling'
	MATL_ID= 'Marinite'
	COLOR= 'GRAY'
	THICKNESS=0.019 /

&SURF ID= 'Wall', DEFAULT=.TRUE.
     MATL_ID= 'Wall Aluminum Silicate'
     COLOR = 'BEIGE'
     LEAK_PATH=0,1     
     THICKNESS=0.1 /


&OBST XB=0.0,6.0,0.0,4.0,3.0,3.0, SURF_ID='Drop Ceiling' /
&HOLE XB=5.2,5.6,1.8,2.2,2.9,3.1 /
&VENT XB=0.0,6.0,0.0,4.0,4.5,4.5, SURF_ID='Ceiling' / Ceiling liner
&VENT XB=0.0,6.0,0.0,4.0,0.0,0.0, SURF_ID='Ceiling' / Floor liner

Ventilation
&ZONE XYZ=3,2,2, LEAK_AREA(0)=0.0033, LEAK_REFERENCE_PRESSURE=50., LEAK_PRESSURE_EXPONENT=0.6311 /

&MATL ID= 'Steel'
      FYI='NUREG-1805'
      CONDUCTIVITY=54.
      DENSITY=7850.
      SPECIFIC_HEAT=0.465 /

&SURF ID='Duct'
      COLOR='BLACK'
      MATL_ID= 'Steel'
      THICKNESS=0.002 /

&SURF ID='EXHAUST', COLOR='BLUE', MASS_FLUX_TOTAL=0.433, RAMP_V='exhaust ramp' /
&RAMP ID='exhaust ramp', T=   0., F=1.00 /
&RAMP ID='exhaust ramp', T= 500., F=0.95 /

&OBST XB=0.7,1.0,0.0,0.5,0.6,0.9, SURF_ID='Duct' / Inlet duct work
&OBST XB=0.7,1.0,0.1,0.5,0.9,3.45, SURF_ID='Duct' /
&OBST XB=0.7,1.0,0.5,2.2,3.15,3.45, SURF_ID='Duct' /
&OBST XB=0.7,1.0,1.8,2.2,3.0,3.15, SURF_ID='Duct' /
&VENT XB=0.7,1.0,1.8,2.2,3.0,3.0, SURF_ID='HVAC',ID='INLET',COLOR='GREEN'/
&HVAC ID='INLET A',TYPE_ID='NODE',DUCT_ID='INLET DUCT',AMBIENT=.TRUE.,XYZ(3)=0.3 /
&HVAC ID='INLET B',TYPE_ID='NODE',DUCT_ID='INLET DUCT',VENT_ID='INLET'/
&HVAC ID='INLET DUCT',TYPE_ID='DUCT',NODE_ID='INLET A','INLET B',DIAMETER=0.30,LOSS=25.6,25.6,LENGTH=7/

 VENT XB=4.4,5.2,0.0,0.0,0.0,2.0, SURF_ID='OPEN' / Door (closed in forced ventilation tests)
&VENT XB=6.0,6.0,1.6,2.4,3.3,3.9, SURF_ID='EXHAUST' / 

Output

&DEVC XYZ=1.5,2.0,0.15, QUANTITY='THERMOCOUPLE', ID ='East 1',OUTPUT=.FALSE./
&DEVC XYZ=1.5,2.0,0.45, QUANTITY='THERMOCOUPLE', ID ='East 2',OUTPUT=.FALSE./
