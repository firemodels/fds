&PROP ID='Sprinkler 1',
      QUANTITY='SPRINKLER LINK TEMPERATURE',
      ACTIVATION_TEMPERATURE=68.0,
      RTI=95.0,
      C_FACTOR=0.4,
      OFFSET=0.02/

&PROP ID='Sprinkler 2',
      QUANTITY='SPRINKLER LINK TEMPERATURE',
      ACTIVATION_TEMPERATURE=68.0,
      RTI=36.0,
      C_FACTOR=0.4,
      OFFSET=0.02/

