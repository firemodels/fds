&HEAD CHID='weak_scaling_glmat_128', TITLE='General purpose input file to test FDS timings' /

&MULT ID='mesh multiplier', DX=1.0, DY=1.0, DZ=1.0, I_LOWER=1, I_UPPER=128, J_LOWER=1, J_UPPER=1, K_LOWER=1, K_UPPER=1 /

&MESH IJK=50,50,50, XB=0.0,1.0,0.0,1.0,0.0,1.0, MULT_ID='mesh multiplier' /

&TIME T_END=10.0, DT=0.1 /

&DUMP NFRAMES=10, SUPPRESS_DIAGNOSTICS=.TRUE., VELOCITY_ERROR_FILE=.TRUE. /

&PRES SOLVER='GLMAT' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMIN'  SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

 DEVC ID='RAM', QUANTITY='RAM', XYZ=1.1,1.1,1.1 /

&TAIL /
