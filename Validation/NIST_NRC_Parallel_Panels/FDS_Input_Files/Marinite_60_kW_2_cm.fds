&HEAD CHID='Marinite_60_kW_2_cm', TITLE='Parallel Panel Test, Marinite only, Propane, 60 kW' /

&MESH IJK=15,20,20, XB=-0.60,-0.30,-0.40,0.00,-0.30,0.10, MULT_ID='mesh' /
&MULT ID='mesh', DX=0.3, DY=0.4, DZ=0.4, I_UPPER=3, J_UPPER=1, K_UPPER=7 /

&TIME T_END=100.0 /

&REAC ID='PROPANE', FUEL='PROPANE', SOOT_YIELD=0.024, RADIATIVE_FRACTION=0.29 /

&MATL ID = 'MINERAL WOOL'
      CONDUCTIVITY = 0.035
      SPECIFIC_HEAT = 0.8
      DENSITY = 70. /

&SURF ID='BURNER', SPEC_ID='PROPANE', HRRPUA=333.3, COLOR='RED', RAMP_Q='fire', MATL_ID='MINERAL WOOL', THICKNESS=0.05 /
&RAMP ID='fire', T=  0., F=0.00 /
&RAMP ID='fire', T= 20., F=0.37 /
&RAMP ID='fire', T= 40., F=0.83 /
&RAMP ID='fire', T= 60., F=0.98 /
&RAMP ID='fire', T= 80., F=1.05 /

&SURF ID='WOOL', COLOR='BEIGE', MATL_ID='MINERAL WOOL', THICKNESS=0.05 /

&SURF ID = 'SAMPLE FRONT'
      COLOR  = 'BEIGE'
      MATL_ID  = 'MARINITE'
      THICKNESS = 0.0254 /

&SURF ID = 'SAMPLE BACK'
      COLOR  = 'BEIGE'
      MATL_ID  = 'MARINITE'
      THICKNESS = 0.0254 /

&MATL ID            = 'MARINITE'
      EMISSIVITY    = 0.9
      DENSITY       = 737.
      SPECIFIC_HEAT = 1.2
      CONDUCTIVITY  = 0.12 /

&MATL ID            = 'STEEL'
      EMISSIVITY    = 0.9
      DENSITY       = 7800.
      SPECIFIC_HEAT = 0.465        
      CONDUCTIVITY  = 43. /

&SURF ID            = 'STEEL'
      MATL_ID       = 'STEEL'
      THICKNESS     = 0.003
      COLOR         = 'BLACK' /

&OBST XB = -0.30,  0.30, -0.14,  0.16, -0.30,-0.00, SURF_IDS = 'BURNER','STEEL','STEEL' /
&OBST XB = -0.30,  0.30,  0.16,  0.18,  0.00, 2.44, SURF_ID6 = 'STEEL','STEEL','SAMPLE FRONT','SAMPLE BACK','STEEL','STEEL'  /
&OBST XB = -0.30,  0.30, -0.16, -0.14,  0.00, 2.44, SURF_ID6 = 'STEEL','STEEL','SAMPLE BACK','SAMPLE FRONT','STEEL','STEEL'  /

&VENT MB='XMIN', SURF_ID='OPEN', DYNAMIC_PRESSURE= 0.05, PRESSURE_RAMP='noise' / 
&VENT MB='XMAX', SURF_ID='OPEN', DYNAMIC_PRESSURE=-0.05, PRESSURE_RAMP='noise' / 
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DEVC ID='t', QUANTITY='TIME', XYZ=0,0,0 /
&CTRL ID='2*pi*t/10', FUNCTION_TYPE='MULTIPLY', INPUT_ID='CONSTANT','t', CONSTANT=0.62831853 /
&CTRL ID='sin(2*pi*t/10)', FUNCTION_TYPE='SIN', INPUT_ID='2*pi*t/10' /
&RAMP ID='noise', T=-1, F=-1, CTRL_ID='sin(2*pi*t/10)' /
&RAMP ID='noise', T= 1, F= 1 /

&DEVC ID='Flux', XB=0.0,0.0,0.16,0.16,0.01,2.43, POINTS=50, Z_ID='Height',           QUANTITY='GAUGE HEAT FLUX',  IOR=-2, TIME_HISTORY=.TRUE. /
&DEVC ID='Temp', XB=0.0,0.0,0.16,0.16,0.01,2.43, POINTS=50, HIDE_COORDINATES=.TRUE., QUANTITY='WALL TEMPERATURE', IOR=-2, TIME_HISTORY=.TRUE. /
&DEVC ID='HF_y-25z20' , XYZ=-0.25,0.16,0.20, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y-15z20' , XYZ=-0.15,0.16,0.20, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y0z20'   , XYZ= 0.00,0.16,0.20, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y15z20'  , XYZ= 0.15,0.16,0.20, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y25z20'  , XYZ= 0.25,0.16,0.20, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y-25z50' , XYZ=-0.25,0.16,0.50, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y-15z50' , XYZ=-0.15,0.16,0.50, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y0z50'   , XYZ= 0.00,0.16,0.50, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y15z50'  , XYZ= 0.15,0.16,0.50, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y25z50'  , XYZ= 0.25,0.16,0.50, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y-25z75' , XYZ=-0.25,0.16,0.75, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y-15z75' , XYZ=-0.15,0.16,0.75, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y0z75'   , XYZ= 0.00,0.16,0.75, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y15z75'  , XYZ= 0.15,0.16,0.75, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y25z75'  , XYZ= 0.25,0.16,0.75, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y-25z100', XYZ=-0.25,0.16,1.00, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y-15z100', XYZ=-0.15,0.16,1.00, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y0z100'  , XYZ= 0.00,0.16,1.00, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y15z100' , XYZ= 0.15,0.16,1.00, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /
&DEVC ID='HF_y25z100' , XYZ= 0.25,0.16,1.00, QUANTITY='GAUGE HEAT FLUX', IOR=-2, TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=80. /

&BNDF QUANTITY='GAUGE HEAT FLUX', CELL_CENTERED=T  /
&BNDF QUANTITY='BURNING RATE', CELL_CENTERED=T  /
&BNDF QUANTITY='WALL TEMPERATURE', CELL_CENTERED=T  /
&BNDF QUANTITY='RADIATIVE HEAT FLUX', CELL_CENTERED=T  /
&BNDF QUANTITY='CONVECTIVE HEAT FLUX', CELL_CENTERED=T  /
&BNDF QUANTITY='HEAT TRANSFER COEFFICIENT', CELL_CENTERED=T  /

&DUMP DT_BNDF=1., DT_DEVC=2., DT_HRR=2., SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SLCF PBX=0.00 QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
&SLCF PBY=0.00 QUANTITY='VELOCITY', VECTOR=.TRUE. /

&ISOF QUANTITY='HRRPUV', VALUE=200 /
 
&TAIL /
