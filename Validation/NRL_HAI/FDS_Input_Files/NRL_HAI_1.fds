&HEAD CHID='NRL_HAI_1', TITLE='Back, Beyler, DiNenno and Tatem Wall Heat Flux Measurements, Test 1' /

&MESH IJK=32,32,96, XB=0.00,0.96,-0.48,0.48,-0.09,2.79 /

&TIME T_END=60. /

&REAC SOOT_YIELD=0.015 FUEL='PROPANE' /

&SURF ID='burner', HRRPUA=676., TMP_FRONT=50., COLOR='TAN' /
&OBST XB=0.00,0.28,-.14,0.14,-.10,0.00,SURF_IDS='burner','STEEL WALL','STEEL WALL' /

&MATL ID            = 'GYPSUM PLASTER'
      FYI           = 'Quintiere, Fire Behavior'
      CONDUCTIVITY  = 0.48
      SPECIFIC_HEAT = 0.84
      DENSITY       = 1440. /
&MATL ID            = 'STEEL'
      FYI           = 'Drysdale, Intro to Fire Dynamics'
      EMISSIVITY    = 0.95
      SPECIFIC_HEAT = 0.46
      CONDUCTIVITY  = 45.8
      DENSITY       = 7850. /
&MATL ID            = 'CONCRETE'
      FYI           = 'Drysdale, Intro to Fire Dynamics'
      SPECIFIC_HEAT = 0.90
      DENSITY       = 2000.
      EMISSIVITY    = 0.95
      CONDUCTIVITY  = 1.0  /

&SURF ID         = 'FLOOR'
      MATL_ID    = 'CONCRETE'
      THICKNESS  = 0.1
      COLOR      = 'GRAY' /
&SURF ID         = 'STEEL WALL'
      MATL_ID    = 'STEEL'
      THICKNESS  = 0.002
      COLOR      = 'BLACK' /
&SURF ID            = 'GYPSUM BOARD'
      MATL_ID       = 'GYPSUM PLASTER'
      COLOR         = 'ANTIQUE WHITE'
      THICKNESS     = 0.0127 /

&VENT MB='XMIN', SURF_ID='GYPSUM BOARD' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMIN', SURF_ID='FLOOR' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DEVC ID='Tot_Flux', QUANTITY='GAUGE HEAT FLUX', XB=0.0,0.0,0.0,0.0,0.015,2.775, POINTS=93, IOR=1 /

&SLCF PBY=0.0,QUANTITY='TEMPERATURE',VECTOR=.TRUE. /
&SLCF PBY=0.0,QUANTITY='HRRPUV'  /
&SLCF PBY=0.0,QUANTITY='INTEGRATED INTENSITY'  /
&SLCF PBY=0.0,QUANTITY='ABSORPTION COEFFICIENT'  /
&SLCF PBY=0.0,QUANTITY='RADIATION LOSS'  /

&DUMP SIG_FIGS=4, SIG_FIGS_EXP=2 /

&BNDF QUANTITY='GAUGE HEAT FLUX'  /

&TAIL /

