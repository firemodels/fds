&HEAD CHID='Test_19_4cm', TITLE='Methane burner in a cabinet' /

&MESH IJK=12,12,12, XB=-0.48,0.0,-0.48,0.0,0.0,0.48, MULT_ID='mesh' /
&MULT ID='mesh', DX=0.48, DY=0.48, DZ=0.48, I_LOWER=-1, I_UPPER=2, J_LOWER=0, J_UPPER=1, K_UPPER=4 /

&TIME T_END=1500., TIME_SHRINK_FACTOR=1 /

&MATL ID            = 'STEEL'
      SPECIFIC_HEAT = 0.465
      CONDUCTIVITY  = 54.
      DENSITY       = 7833. /

&SURF ID            = 'STEEL PLATE'
      MATL_ID       = 'STEEL'
      DEFAULT       = .TRUE.
      THICKNESS     = 0.003
      COLOR         = 'GRAY 40' /

&PRES VELOCITY_TOLERANCE=0.002, MAX_PRESSURE_ITERATIONS=50 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /


&REAC FUEL='METHANE', RADIATIVE_FRACTION=0., N_SIMPLE_CHEMISTRY_REACTIONS=2, AUTO_IGNITION_TEMPERATURE=540.   AIT_EXCLUSION_ZONE=0.44,0.60,-0.16, 0.00,0.24,0.28 /

&SURF ID='FIRE', HRRPUA=977., COLOR='ORANGE', TMP_FRONT=300., RAMP_Q='fire ramp' /
&RAMP ID='fire ramp', T=   0., F=0.000 /
&RAMP ID='fire ramp', T=  10., F=1.000 /
&RAMP ID='fire ramp', T= 260., F=1.000 /
&RAMP ID='fire ramp', T= 270., F=2.000 /
&RAMP ID='fire ramp', T= 450., F=2.000 /
&RAMP ID='fire ramp', T= 460., F=3.000 /
&RAMP ID='fire ramp', T= 590., F=3.000 /
&RAMP ID='fire ramp', T= 600., F=4.000 /
&RAMP ID='fire ramp', T= 800., F=4.000 /
&RAMP ID='fire ramp', T= 810., F=6.000 /
&RAMP ID='fire ramp', T=1000., F=6.000 /
&RAMP ID='fire ramp', T=1010., F=8.000 /
&RAMP ID='fire ramp', T=1270., F=8.000 /
&RAMP ID='fire ramp', T=1280., F=0.000 /

&OBST XB=0.44,0.60,-0.16, 0.00,0.16,0.24, SURF_IDS='FIRE','STEEL PLATE','STEEL PLATE' /

&OBST XB=-0.80, 0.80,-0.28,-0.24, 0.00, 2.28 / Front
&OBST XB=-0.80, 0.80, 0.24, 0.28, 0.00, 2.28 / Back
&OBST XB=-0.80,-0.76,-0.24, 0.24, 0.00, 2.28 / Left
&OBST XB= 0.76, 0.80,-0.24, 0.24, 0.00, 2.28 / Right
&OBST XB=-0.80, 0.80,-0.24, 0.24, 2.28, 2.32 / Top

&VENT XB=-0.76, 0.76,-0.24,-0.24,0.00,1.00, ID='Inside Lower Leakage',  SURF_ID='STEEL PLATE' /
&VENT XB=-0.76, 0.76,-0.28,-0.28,0.00,1.00, ID='Outside Lower Leakage', SURF_ID='STEEL PLATE' /
&HVAC ID='Lower Leakage', TYPE_ID='LEAK', VENT_ID='Inside Lower Leakage', VENT2_ID='Outside Lower Leakage', AREA=0.0395,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /
&VENT XB=-0.76, 0.76,-0.24,-0.24,1.20,2.20, ID='Inside Upper Leakage',  SURF_ID='STEEL PLATE' /
&VENT XB=-0.76, 0.76,-0.28,-0.28,1.20,2.20, ID='Outside Upper Leakage', SURF_ID='STEEL PLATE' /
&HVAC ID='Upper Leakage', TYPE_ID='LEAK', VENT_ID='Inside Upper Leakage', VENT2_ID='Outside Upper Leakage', AREA=0.0395,
      LEAK_PRESSURE_EXPONENT=0.6, LEAK_REFERENCE_PRESSURE=1., DISCHARGE_COEFFICIENT=0.61 /

&DUMP DT_DEVC=10., DT_HRR=10. /

&SLCF PBY=-0.08, QUANTITY='TEMPERATURE', VECTOR=.TRUE. /
&SLCF PBY=-0.08, QUANTITY='PRESSURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=-0.08, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN', CELL_CENTERED=.TRUE. /
&SLCF PBY=-0.08, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE', CELL_CENTERED=.TRUE. /
&SLCF PBY=-0.08, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE', CELL_CENTERED=.TRUE. /
&SLCF PBY=-0.08, QUANTITY='VOLUME FRACTION', SPEC_ID='METHANE', CELL_CENTERED=.TRUE. /
&SLCF PBY=-0.08, QUANTITY='MASS FRACTION', SPEC_ID='SOOT', CELL_CENTERED=.TRUE. /
&SLCF PBY=-0.08, QUANTITY='HRRPUV', CELL_CENTERED=.TRUE. /
&SLCF PBY=-0.08, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=-0.08, QUANTITY='PRESSURE', CELL_CENTERED=.TRUE. /

&BNDF QUANTITY='WALL TEMPERATURE', CELL_CENTERED=.TRUE. /

&DEVC ID='HRR_ideal', XB=0.44,0.60,-0.16, 0.00,0.20,0.28, QUANTITY='MASS FLUX', SPEC_ID='METHANE',
      SPATIAL_STATISTIC='SURFACE INTEGRAL', CONVERSION_FACTOR=50000, UNITS='kW' /
&DEVC ID='HRR_actual',XB=-2.00,2.00,-2.00,2.000,0.00,3.00, QUANTITY='HRRPUV',
      SPATIAL_STATISTIC='VOLUME INTEGRAL', TEMPORAL_STATISTIC='SMOOTHED', SMOOTHING_FACTOR=0.99 /
&DEVC ID='O2',       XYZ= 0.32, 0.10,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='OXYGEN' /
&DEVC ID='CO2',      XYZ= 0.32, 0.10,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='CARBON DIOXIDE' /
&DEVC ID='CO',       XYZ= 0.32, 0.10,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='CARBON MONOXIDE' /
&DEVC ID='TC-4',     XYZ= 0.32, 0.10,2.00, QUANTITY='THERMOCOUPLE' /
&DEVC ID='TC-5',     XYZ= 0.32, 0.10,1.40, QUANTITY='THERMOCOUPLE' /
&DEVC ID='TC-6',     XYZ= 0.32, 0.10,0.50, QUANTITY='THERMOCOUPLE' /
&DEVC ID='CH4',      XYZ= 0.32, 0.10,2.00, QUANTITY='VOLUME FRACTION', DRY=T, SPEC_ID='METHANE' /
&DEVC ID='SOOT',     XYZ= 0.32, 0.10,2.00, QUANTITY='MASS FRACTION',   SPEC_ID='SOOT' /
&DEVC ID='PRESSURE', XYZ= 0.32, 0.10,2.00, QUANTITY='PRESSURE' /

&TAIL/

