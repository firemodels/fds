&HEAD CHID='jet_deardorff_dx5cm', TITLE='Centerline decay of a turbulent jet' /    
	
&MESH IJK=100,16,16, XB=-5,0,-.4,.4,-.4,.4 /

&MULT ID='m1', DX=5.0,DY=1.6,DZ=1.6, I_UPPER=3,J_UPPER=2,K_UPPER=2 / 4 x 3 x 3 = 36 meshes
&MESH IJK=100,32,32, XB=0,5.0, -2.4,-0.8, -2.4,-0.8, MULT_ID='m1' /

&TIME T_END=100. /

&MISC TURBULENCE_MODEL='DEARDORFF' /

&SURF ID='jet', VEL=-2.0, COLOR='INVISIBLE' /

&VENT XB=-5,-5, -.4,.4, -.4,.4, SURF_ID='jet', N_EDDY=100, L_EDDY=.2, VEL_RMS=.2 /

&VENT PBX=20, SURF_ID='OPEN' /
&VENT PBY= 2.4, SURF_ID='OPEN' /
&VENT PBY=-2.4, SURF_ID='OPEN' /
&VENT PBZ= 2.4, SURF_ID='OPEN' /
&VENT PBZ=-2.4, SURF_ID='OPEN' /

&SLCF PBY=0, QUANTITY='VELOCITY', VECTOR=.TRUE. /

&DEVC XB= 0.4,20,0,0,0,0, QUANTITY='U-VELOCITY', ID='VEL1', POINTS=50 /

&TAIL /

