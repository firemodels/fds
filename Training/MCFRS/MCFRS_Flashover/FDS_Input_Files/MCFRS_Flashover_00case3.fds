&OBST XB=2.2, 2.3, 6.2, 8.6, 1.9, 3.2, SURF_ID='PARTBRD' / liner in small container

&REAC ID                 = 'WOOD'
     FYI                = 'Tewarson, SFPE Handbook, Douglas Fir'
     C                  = 1.
     H                  = 1.7
     O                  = 0.74
     N                  = 0.002
     HEAT_OF_COMBUSTION = 16400.
     CO_YIELD = 0.01
     SOOT_YIELD         = 0.015 /


***** openings

&OBST XB=1.0, 1.5, 4.1, 4.7, 2.4, 2.6, / vent over large container
&HOLE XB=1.1,1.4, 4.2,4.6, 2.2,2.7 CTRL_ID='vent timer' /


&HOLE XB=0.2,0.4,-0.1,0.2,0.2,0.4 / front hole

&HOLE XB=-0.1,0.2, 0.2,1.1, 0.1,0.2 /  door
&HOLE XB=-0.1,0.2, 0.2,0.3, 0.2,2.0 CTRL_ID='door timer1'/  door
&HOLE XB=-0.1,0.2, 0.3,1.1, 0.2,2.0 CTRL_ID='door timer2'/  door


***** materials

&MATL ID                    = 'SHEETMETAL'
      SPECIFIC_HEAT         = 0.47
      CONDUCTIVITY          = 48.0
      DENSITY               = 8000.0
      /

&SURF ID                 = 'METAL WALL'
      FYI                = '18 guage sheet metal'
      COLOR = 'RED',
      MATL_ID            = 'SHEETMETAL'
      THICKNESS          = 0.00635 /

 ** change ref temp from 360 to 300

&MATL ID                    = 'MT_PARTBRD'
      FYI='http://www.customthermoelectric.com/MaterialProperties6.htm'
      SPECIFIC_HEAT         = 1.34
      CONDUCTIVITY          = 0.17
      DENSITY               = 1000.0
      N_REACTIONS = 1
      NU_FUEL = 1.0
      REFERENCE_TEMPERATURE=300.0
      HEAT_OF_REACTION= 3000.0
      HEAT_OF_COMBUSTION=16400.0/

&SURF ID='PARTBRD'
      MATL_ID = 'MT_PARTBRD'
      THICKNESS=0.016 /

&MATL ID                    = 'MT_CONCRETE'
      SPECIFIC_HEAT         = 0.88
      CONDUCTIVITY          = 1280.
      DENSITY               = 2200.
      /

&SURF ID='CONCRETE'
      MATL_ID = 'MT_CONCRETE'
      THICKNESS=0.016
      RGB=220,220,220/

***** boundary conditions

&VENT XB=-0.4,-0.4,-0.4, 9.2,0.0,3.6, SURF_ID='OPEN' /
&VENT XB= 2.8, 2.8,-0.4, 9.2,0.0,3.6, SURF_ID='OPEN' /
&VENT XB=-0.4, 2.8,-0.4,-0.4,0.0,3.6, SURF_ID='OPEN' /
