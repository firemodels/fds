          FLOW_RATE = 10.0
          SPRAY_ANGLE = 0.,30.
          PARTICLE_VELOCITY = 10. /

&PART ID='Water_drops' , SPEC_ID='WATER VAPOR', SAMPLING_FACTOR=10 , DIAMETER=1000./
&DEVC XYZ=0.5,0.5,1.9 , PROP_ID ='Spray', QUANTITY='TIME', SETPOINT=5., ID = 'Spray'  /

&TAIL /