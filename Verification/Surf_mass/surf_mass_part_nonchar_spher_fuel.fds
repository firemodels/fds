&HEAD CHID='surf_mass_part_nonchar_spher_fuel',TITLE='Surface mass, particles, nonchar, sphere, fuel' /

&MESH 	IJK= 6,6,6,
	XB = -1.5,1.5,-1.5,1.5,0.0,3.0 /

&TIME T_END=100., DT = 0.01, WALL_INCREMENT = 1 /  
&DUMP DT_HRR = 0.1, DT_DEVC = 0.1 DT_PL3D = 10000 DT_SLCF = 10000 /

&REAC FUEL = 'WOOD'
	SOOT_YIELD 	= 0.0
	CO_YIELD	= 0.0
	C 		= 3.4
	H		=6.2
	O 		=2.5
	HEAT_OF_COMBUSTION = 12000.
	Y_O2_INFTY	=0.01
	IDEAL 		=.TRUE. /

&SURF ID                        = 'Wood'
      COLOR                     = 'IVORY BLACK'
      MATL_ID(1,1)            	= 'PINE'
	GEOMETRY                = 'SPHERICAL'
	THICKNESS                 = 0.01
      EXTERNAL_FLUX 		= 50. /
      
&MATL ID = 'PINE'
      EMISSIVITY 	= 1.0
	DENSITY 	= 360.
	CONDUCTIVITY	= 0.05
	SPECIFIC_HEAT	= 1.0
	N_REACTIONS	= 1
	A		= 1E20
	E 		= 1.6E5
	N_S		= 1
	NU_SPEC		= 1.0
        SPEC_ID         = 'FUEL'
	HEAT_OF_REACTION= 1000
	HEAT_OF_COMBUSTION(1) = 12000/
	

&PART ID='Wood_part', SAMPLING_FACTOR=1,SURF_ID='Wood', PROP_ID='wood image'
	QUANTITIES='DROPLET_TEMPERATURE','DROPLET MASS','DROPLET DIAMETER' /

&INIT PART_ID='Wood_part', XB=-0.5,0.5,-0.5,0.5,0,0.02,NUMBER_INITIAL_DROPLETS = 1  /

&PROP ID='wood image', SMOKEVIEW_ID='TUBE', SMOKEVIEW_PARAMETERS='L=0.1','D=0.02' /

&DEVC 	XB = -1.5,1.5,-1.5,1.5,0.0,3.0, 
	QUANTITY = 'DENSITY', 
	ID = 'density', 
	STATISTICS = 'VOLUME INTEGRAL',
	SPEC_ID='FUEL' /

&DEVC 	XB = -1.5,1.5,-1.5,1.5,0.0,3.0 , 
	QUANTITY = 'MPUV', 
	PART_ID = 'Wood_part', 
	ID = 'mpuv',IOR = 3, 
	STATISTICS = 'VOLUME INTEGRAL'  /

&DEVC 	XYZ = 0.0,0.0,0.0,IOR = 3, 
	QUANTITY = 'BURNING RATE', 
	ID = 'Burning rate'
	STATISTICS = 'TIME INTEGRAL' /

&TAIL /
