&DEVC ID='TIMER2', XYZ=0.0,0.0,0.0, QUANTITY='TIME' /
&CTRL ID='vent timer', FUNCTION_TYPE='CUSTOM', INPUT_ID='TIMER2', RAMP_ID='vent times' /
&RAMP ID='vent times', T=    0,  F=-1 /
&RAMP ID='vent times', T=   619, F=-1 /
&RAMP ID='vent times', T=   621, F= 1 /
&RAMP ID='vent times', T=   669, F= 1 /
&RAMP ID='vent times', T=   671, F=-1 /
&RAMP ID='vent times', T=   856, F=-1 /
&RAMP ID='vent times', T=   858, F= 1 /
&RAMP ID='vent times', T=  1063, F= 1 /
&RAMP ID='vent times', T=  1065, F=-1 /
&RAMP ID='vent times', T=  1164, F=-1 /
&RAMP ID='vent times', T=  1166, F= 1 /
&RAMP ID='vent times', T=  1404, F= 1 /
&RAMP ID='vent times', T=  1406, F=-1 /
&RAMP ID='vent times', T=  1499, F=-1 /
&RAMP ID='vent times', T=  1501, F= 1 /
&RAMP ID='vent times', T=  1559, F= 1 /
&RAMP ID='vent times', T=  1561, F=-1 /

&OBST XB=0.3, 2.1, 6.3, 8.5, 2.8, 2.9, SURF_ID='PARTBRD' / liner in small container
