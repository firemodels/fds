&HEAD CHID='McCaffrey_22_kW_20', TITLE='McCaffrey, NBSIR 79-1910, 21.7 kW Natural Gas' /

MESH IJK=100,100,150, XB=-1.5,1.5,-1.5,1.5,-.75,3.75 /

&MULT ID='m1', DX=1.5,DY=1.5,DZ=1.5, I_UPPER=1,J_UPPER=1,K_UPPER=2 /
&MESH IJK=100,100,100, XB=-1.5,0,-1.5,0,-.75,.75, MULT_ID='m1'/

&TIME T_END=30. /
&DUMP DT_DEVC_LINE=25. /

&MISC SUPPRESSION=.FALSE., SECOND_ORDER_INTERPOLATED_BOUNDARY=.TRUE. /

&SURF ID='burner', HRRPUA=241., ADIABATIC=.TRUE., COLOR='RED' /
&OBST XB=-0.15,0.15,-.15,0.15,-.10,0.00,SURF_IDS='burner','INERT','INERT' /

&REAC FUEL='METHANE' /

&RADI RADIATIVE_FRACTION=0.20 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DEVC ID='temp20', XB= 0.00, 0.00,0.00,0.00,0.0075,2.9925, POINTS=200, QUANTITY='THERMOCOUPLE', Z_ID='Height' /
&DEVC ID='velo20', XB= 0.00, 0.00,0.00,0.00,0.0075,2.9925, POINTS=200, QUANTITY='VELOCITY',  HIDE_COORDINATES=.TRUE. /

&DEVC ID='vx20_f', XB=-.2925,.2925,0,0,0.125,0.125, POINTS=40, QUANTITY='VELOCITY', X_ID='Radius'/ flame
&DEVC ID='vx20_i', XB=-.2925,.2925,0,0,0.5,0.5,     POINTS=40, QUANTITY='VELOCITY', HIDE_COORDINATES=.TRUE./ intermittent
&DEVC ID='vx20_p', XB=-.2925,.2925,0,0,1.0,1.0,     POINTS=40, QUANTITY='VELOCITY', HIDE_COORDINATES=.TRUE./ plume 

&DEVC XYZ=0,0,0.3, QUANTITY='VELOCITY'/

&SLCF PBY=0.0, QUANTITY='TEMPERATURE',VECTOR=.TRUE. /
&SLCF PBY=0.0, QUANTITY='HRRPUV'  /
&SLCF PBY=0.0, QUANTITY='MASS FRACTION', SPEC_ID='METHANE' /

&TAIL /

