&HEAD CHID='tunnel_fire_20', TITLE='Fire in a long tunnel' /

&MULT ID='m1', DX=20, I_UPPER=79/ 80 mesh case
&MESH IJK=20,20,20, XB=0,20,-5,5,0,10, MULT_ID='m1'/

&TIME T_END=600.0 /

&PRES TUNNEL_PRECONDITIONER=T /

&REAC FUEL='N-HEPTANE', SOOT_YIELD=0.05, RADIATIVE_FRACTION=0.35 /
&SURF ID='FIRE', HRRPUA=995, COLOR='ORANGE' /
&VENT XB=786,794,-4,4,0,0, XYZ=790,0,0, RADIUS=4, SURF_ID='FIRE' /

&SURF ID='inflow', VEL=-2, COLOR='INVISIBLE' /
&SURF ID='rough wall',
      COLOR='GRAY',
      DEFAULT=T,
      ROUGHNESS=1.0E-1 /

&VENT PBX=0, SURF_ID='inflow'/
&VENT PBX=1600, SURF_ID='OPEN'/

&SLCF PBY=0.0, QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0.0, QUANTITY='PRESSURE', CELL_CENTERED=T/
&SLCF PBY=0.0, QUANTITY='TEMPERATURE', CELL_CENTERED=T/

&DUMP DT_DEVC=1. /

&DEVC ID='m_dot_in',  XB=  10,  10,-5,5,0,10, QUANTITY='MASS FLUX X', SPATIAL_STATISTIC='AREA INTEGRAL' /
&DEVC ID='m_dot_out', XB=1590,1590,-5,5,0,10, QUANTITY='MASS FLUX X', SPATIAL_STATISTIC='AREA INTEGRAL' /
&DEVC ID='V_dot_in',  XB=  10,  10,-5,5,0,10, QUANTITY='U-VELOCITY', SPATIAL_STATISTIC='AREA INTEGRAL' /
&DEVC ID='V_dot_out', XB=1590,1590,-5,5,0,10, QUANTITY='U-VELOCITY', SPATIAL_STATISTIC='AREA INTEGRAL' /

&DEVC ID='p', XB=1,1599,0,0,5,5, QUANTITY='PRESSURE', POINTS=1601, STATISTICS_START=200. /

&TAIL /
