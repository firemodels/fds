&HEAD CHID='e_coefficient'/

&MESH ID='MESH', IJK=10,10,20, XB=0.00,1.00,0.00,1.00,0.00,2.00 /
&MISC POROUS_FLOOR=.FALSE., PARTICLE_CFL=.TRUE./
&TIME T_END=20.00/
&DUMP NFRAMES=40./

&SPEC ID='WATER VAPOR' /

&VENT MB = 'XMIN', SURF_ID = 'OPEN'/
&VENT MB = 'YMIN', SURF_ID = 'OPEN'/
&VENT MB = 'XMAX', SURF_ID = 'OPEN'/
&VENT MB = 'YMAX', SURF_ID = 'OPEN'/
&VENT MB = 'ZMAX', SURF_ID = 'OPEN'/
