&HEAD CHID='Surf_mass_vent_char_cyl_fuel',TITLE='Surface mass, vent, char, cylinder, fuel' /

&MESH 	IJK= 6,6,6,
	XB = -1.5,1.5,-1.5,1.5,0.0,3.0 /

&TIME TWFIN=100., DT = 0.01, WALL_INCREMENT = 1 / 
&DUMP DT_HRR = 0.01, DT_DEVC = 0.01 DT_PL3D = 10000 DT_SLCF = 10000 /

&REAC ID = 'WOOD'
	SOOT_YIELD 	= 0.0
	CO_YIELD	= 0.0
	C 		= 3.4
	H		=6.2
	O 		=2.5
	HEAT_OF_COMBUSTION = 12000.
	Y_O2_INFTY	=0.01
	IDEAL 		=.TRUE. /


&SURF ID                        = 'Wood'
      COLOR                     = 'IVORY BLACK'
      MATL_ID(1,1)            	= 'PINE'
      THICKNESS                 = 0.01
	GEOMETRY                = 'CYLINDRICAL'
      EXTERNAL_FLUX 		= 50. /
      
&MATL ID = 'PINE'
      EMISSIVITY 	= 1.0
	DENSITY 	= 360.
	CONDUCTIVITY	= 0.05
	SPECIFIC_HEAT	= 1.0
	N_REACTIONS	= 1
	A		= 1E20
	E 		= 1.6E5
	N_S		= 1
	NU_RESIDUE	= 0.5
	NU_FUEL		= 0.5
	NU_WATER	= 0.0
	HEAT_OF_REACTION= 1000
	RESIDUE 	= 'CHAR'
	HEAT_OF_COMBUSTION(1,1) = 12000/
	
&MATL ID = 'CHAR'
	EMISSIVITY 	= 1.0
	DENSITY 	= 180.
	CONDUCTIVITY	= 1.0
	SPECIFIC_HEAT	= 1.6 /

&VENT ID = 'Sample', SURF_ID = 'Wood', XB = -0.5,0.5,-0.5,0.5,0,0.0 /

&DEVC 	XB = -1.5,1.5,-1.5,1.5,0.0,3.0, 
	QUANTITY = 'DENSITY', 
	ID = 'density', 
	STATISTICS = 'VOLUME INTEGRAL', 
	SPEC_ID = 'fuel'/

&DEVC 	XYZ = 0.0,0.0,0.0,IOR = 3, 
	QUANTITY = 'SURFACE DENSITY', 
	ID = 'Surface density'
	STATISTICS = 'SURFACE INTEGRAL' 
	XB = -0.5,0.5,-0.5,0.5,0.0,0.0 /

&DEVC 	XYZ = 0.0,0.0,0.0,IOR = 3, 
	QUANTITY = 'BURNING RATE', 	
	ID = 'Burning rate'
	STATISTICS = 'TIME INTEGRAL' /

&TAIL /