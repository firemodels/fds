&HEAD CHID = 'reig_coarse_stoich' TITLE = '1.0 g/s Methane, 70 g/s Air, 10% Co-Flow O2' /

MESH IJK = 80,100,200, XB = -.35,.35,-.475,.475,-.03,1.97 /

&MULT ID='m1', DX=.35,DY=.475,DZ=.5, I_UPPER=1,J_UPPER=1,K_UPPER=3 /
&MESH IJK=40,50,50, XB=-.35,0,-.475,0,-.03,.47, MULT_ID='m1' /

&TIME T_BEGIN=0., T_END = 20.0 /

&MISC SECOND_ORDER_INTERPOLATED_BOUNDARY=.TRUE.
      EXTINCTION_MODEL='EXTINCTION 3' /

&RADI RADIATIVE_FRACTION = 0.12, RTE_SOURCE_CORRECTION=.FALSE. /

&SPEC ID = 'NITROGEN' /
&SPEC ID = 'OXYGEN',          LUMPED_COMPONENT_ONLY = .TRUE. /
&SPEC ID = 'CARBON DIOXIDE',  LUMPED_COMPONENT_ONLY = .TRUE. /
&SPEC ID = 'WATER VAPOR',     LUMPED_COMPONENT_ONLY = .TRUE. /
&SPEC ID = 'METHANE',         LUMPED_COMPONENT_ONLY = .TRUE. /

&SPEC ID='M1', SPEC_ID = 'METHANE' /
&SPEC ID='M2', SPEC_ID = 'METHANE' /

&SPEC ID = 'AIR'
      SPEC_ID ='NITROGEN','OXYGEN'
      VOLUME_FRACTION = 0.79,0.21
      BACKGROUND = .TRUE. /

&SPEC ID = 'AIR2'
      SPEC_ID ='NITROGEN','OXYGEN'
      VOLUME_FRACTION = 0.79,0.21 /

&SPEC ID = 'PRODUCTS'
      SPEC_ID ='NITROGEN','CARBON DIOXIDE','WATER VAPOR'
      VOLUME_FRACTION = 0.714932,0.095023,0.190045 /

&REAC ID='R1',
      FUEL='M1',
      SPEC_ID_NU='M1','AIR','PRODUCTS', NU=-1,-9.52381,10.52381/

&REAC ID='R2',
      FUEL='M1',
      SPEC_ID_NU='M1','AIR','M2','AIR2', NU=-1,-9.52381,1,9.52381
      ALT_REAC_ID='R1' /

&REAC ID='R3',
      FUEL='M2',
      SPEC_ID_NU='M2','AIR','PRODUCTS', NU=-1,-9.52381,10.52381
      AUTO_IGNITION_TEMPERATURE=727
      CRITICAL_FLAME_TEMPERATURE=0 /

&REAC ID='R4'
      FUEL='AIR2'
      SPEC_ID_NU='AIR2','AIR', NU=-1,1
      CRITICAL_FLAME_TEMPERATURE=0 /

&SURF ID = 'FUEL SLOT',   MASS_FLUX(1) = 0.040, TAU_MF(1) = 0.01, SPEC_ID(1) = 'M1', COLOR = 'RED' /
&SURF ID = 'COFLOW SLOT', MASS_FLUX(1) = 0.240, TAU_MF(1) = 0.01, SPEC_ID(1) = 'AIR', COLOR = 'BLUE'
                          MASS_FLUX(2) = 0.2569, RAMP_MF(2) = 'CN2', SPEC_ID(2) = 'NITROGEN' /

&RAMP ID = 'CN2', T =  0.0, F = 0.0 /
&RAMP ID = 'CN2', T =  5.0, F = 0.0 /
&RAMP ID = 'CN2', T = 15.0, F = 1.0 /
&RAMP ID = 'CN2', T = 20.0, F = 1.0 /

&OBST XB = -0.025, 0.025, -0.250, 0.250, -0.03, 0.00, SURF_IDS = 'FUEL SLOT','INERT','INERT' /

&OBST XB = -0.250, 0.250, -0.375, 0.375, -0.03,-0.02, SURF_IDS = 'COFLOW SLOT','INERT','INERT' /

&OBST XB = -0.075, 0.075, -0.300, 0.300, -0.03,-0.01, SURF_ID = 'INERT' /

&OBST XB = -0.350,-0.250, -0.475, 0.475, -0.03, 0.00, SURF_ID = 'INERT' /
&OBST XB =  0.250, 0.350, -0.475, 0.475, -0.03, 0.00, SURF_ID = 'INERT' /
&OBST XB = -0.250, 0.250,  0.375, 0.475, -0.03, 0.00, SURF_ID = 'INERT' /
&OBST XB = -0.250, 0.250, -0.475,-0.375, -0.03, 0.00, SURF_ID = 'INERT' /

&VENT MB = 'XMIN', SURF_ID = 'OPEN' /
&VENT MB = 'XMAX', SURF_ID = 'OPEN' /
&VENT MB = 'YMIN', SURF_ID = 'OPEN' /
&VENT MB = 'YMAX', SURF_ID = 'OPEN' /
&VENT MB = 'ZMIN', SURF_ID = 'OPEN' /
&VENT MB = 'ZMAX', SURF_ID = 'OPEN' /

&SLCF PBX = 0.0, QUANTITY = 'VELOCITY', VECTOR = .TRUE. /
&SLCF PBX = 0.0, QUANTITY = 'TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBX = 0.0, QUANTITY = 'HRRPUV', CELL_CENTERED=.TRUE./
&SLCF PBX = 0.0, QUANTITY = 'VOLUME FRACTION', SPEC_ID='M1', CELL_CENTERED=.TRUE. /
&SLCF PBX = 0.0, QUANTITY = 'VOLUME FRACTION', SPEC_ID='M2', CELL_CENTERED=.TRUE. /
&SLCF PBX = 0.0, QUANTITY = 'VOLUME FRACTION', SPEC_ID = 'OXYGEN', CELL_CENTERED=.TRUE. /
&SLCF PBX = 0.0, QUANTITY = 'VOLUME FRACTION', SPEC_ID = 'NITROGEN', CELL_CENTERED=.TRUE. /

&SLCF PBY = 0.0, QUANTITY = 'VELOCITY', VECTOR = .TRUE. /
&SLCF PBY = 0.0, QUANTITY = 'TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY = 0.0, QUANTITY = 'HRRPUV', CELL_CENTERED=.TRUE./
&SLCF PBY = 0.0, QUANTITY = 'VOLUME FRACTION', SPEC_ID='M1', CELL_CENTERED=.TRUE. /
&SLCF PBY = 0.0, QUANTITY = 'VOLUME FRACTION', SPEC_ID='M2', CELL_CENTERED=.TRUE. /
&SLCF PBY = 0.0, QUANTITY = 'VOLUME FRACTION', SPEC_ID = 'OXYGEN', CELL_CENTERED=.TRUE. /
&SLCF PBY = 0.0, QUANTITY = 'VOLUME FRACTION', SPEC_ID = 'NITROGEN', CELL_CENTERED=.TRUE. /

&DEVC ID = 'O2',  XYZ = 0.0,0.33,0.0, QUANTITY = 'VOLUME FRACTION', SPEC_ID = 'OXYGEN' /
&DEVC ID = 'CN2', XYZ = 0.0,0.33,0.0, QUANTITY = 'VOLUME FRACTION', SPEC_ID = 'NITROGEN' /
&DEVC ID = 'TN2', XYZ = 0.0,0.33,0.0, QUANTITY = 'VOLUME FRACTION', SPEC_ID = 'NITROGEN' /

&TAIL /
